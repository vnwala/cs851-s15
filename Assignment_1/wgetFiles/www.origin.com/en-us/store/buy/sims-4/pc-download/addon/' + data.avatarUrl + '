        ACCESS_DENIED: "Unable to log in. Please contact <a href="https://www.origin.com/en-us/store/buy/sims-4/pc-download/base-game/\&quot;https:\/\/help.ea.com\&quot;">Origin Support<\/a> for assistance"
    };

    var qvWhitelist = ['intcmp', 'promo-code', 'friend-id', 'referring-site'];
    var searchItems = window.location.search.substr(1).split('&');
    var qvList = {};
    var hasQv = false;
    var autoLogin = window.location.search.substr(1).indexOf('auto-login=true') !== -1;
    
    for(var i = 0; i < searchItems.length; i++)
    {
        var qvItem = searchItems[i].split('=');
        if(typeof qvItem[0] !== undefined && typeof qvItem[1] !== undefined && $.inArray(qvItem[0], qvWhitelist) !== -1)
        {
            qvList[qvItem[0]] = qvItem[1];
        }
    }
    for(key in qvList)
    {   
        
        if(key.length > 0)
        {
            hasQv = true;
        }
    }

    if(navigator.userAgent.indexOf('Origin') == -1)
    {
        $('body').removeClass('origin-client');
    }
    
    if(hasQv || $.cookie('o_cc') === 'z' || autoLogin)
    {
        $.ajax({
            url: '/' + $('body').data('locale') + '/store/v1/gus',
            cache: false,
            data: qvList
        })

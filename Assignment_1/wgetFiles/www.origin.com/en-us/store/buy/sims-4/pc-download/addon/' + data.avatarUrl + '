                    .fadeIn("fast")
                    .css('display', 'inline-block');

                if(data.hasUser)
                {
                    if(data.forceAutoLogin)
                    {
                        setTimeout(autoLoginInterval, 10*1000);
                    }
                    setInterval(autoLoginInterval, 29*60*1000);
                }
            }
            else
            {
                $('ul.display-logged-out')
                    .fadeIn("fast")
                    .css('display', 'inline-block');
            }

            //login error
            if (data.hasLoginErrorMessage)
            {
                $('div.mod-alert p#mod-alert-message').html(errors[data.loginErrorMessage]);

                $('#loginErrorAlert').removeClass('hidden');
            }

            runUtag(data);
        })
        .fail(function(){
            notLoggedIn();
        });
    }
    else
    {
        notLoggedIn();
    }

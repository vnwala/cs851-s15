			function courses_without_excluded(courses) {
				var show_course = true;
				var cou_cookie = $.cookie('IJ.BusquedaOfertas.CursosExcluidos');
				
				if (cou_cookie) {
					var cou_ids = cou_cookie.split(',');
					
					for (j=0; j< courses.length; j++) {
						for (i=0; i<cou_ids.length; i++) {
							if (cou_ids[i] == courses[j].id)
								courses.splice(j, 1);
						}
					}
				}
			}


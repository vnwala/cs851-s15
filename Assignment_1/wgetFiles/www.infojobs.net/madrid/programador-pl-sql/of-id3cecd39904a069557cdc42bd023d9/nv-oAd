<script type="text/javascript">
	// <![CDATA[

	function prepareClicks() {
		track("L");
	
		try{
			aSelector = jQuery("a");
			prepareClickOuts(aSelector);
			customXitiClick();
			
		} catch(err){
			try{
				saveError(err, err.message);
			}catch(err2){
				var imgSrc = '/js/pix.xhtml?e=' + err + '&msg=' + window.self.location + '&t=' + (new Date()).getTime();
				(new Image()).src = imgSrc;
			}
		}
		
	}


	function prepareClickOuts(aLinks) {
		aLinks.click(function() {
			if(isClickOut(this.href)) {
				var href = this.href;
				sNext = href.indexOf("?", 0)>0?href.substr(0, href.indexOf("?", 0)):href;
				var clickLabel = getXitiElementName(this);
				clickLabel = (clickLabel == "")?xtpage + "~" + sNext:xtpage + "~" + clickLabel;
				clickLabel = clickLabel.replace(" ", "");
				setXitiEvent(this, clickLabel, "S", 1);
			} else if (isOpenNewWin(this.href)){
				this.target = "_blank";
			}
		});		
	}
	
	function isOpenNewWin(sUrlDest) {
		return isGoogleLink(sUrlDest);
	}
	
	function isGoogleLink(sUrlDest) {
		googleleadsPatt = new RegExp("googleads.g.doubleclick.net\/", "i");
		googlecomPatt = new RegExp("www.google.com\/", "i");
		var isGoogle = googleleadsPatt.test(sUrlDest) || googlecomPatt.test(sUrlDest);
		return isGoogle;
	}
	
	
	function isClickOut(sUrlDest) {
		sUrlDestFiltered = (sUrlDest.indexOf("?", 0)>0)?sUrlDest.substr(0, sUrlDest.indexOf("?", 0)):sUrlDest;
		bClickOut = false;
		sbDomain = getBaseDomain();
		
		//formacionPatt = new RegExp("formacion.infojobs.net\/", "i");
		planCarreraPatt = new RegExp("plandecarrera.infojobs.net\/", "i");
		
		clickInPatt = new RegExp(sbDomain + "\/", "i");
		bannerPatt = new RegExp("zsc.scmspain.com\/", "i");
		jscallPatt = new RegExp("javascript:", "i");
		if(!isSocialMenuSiteLink(sUrlDest)){
			//if (formacionPatt.test(sUrlDestFiltered) || planCarreraPatt.test(sUrlDestFiltered)) {
			if ( planCarreraPatt.test(sUrlDestFiltered) ) {
				bClickOut = true;
			} else if (!clickInPatt.test(sUrlDestFiltered) 
					&& !bannerPatt.test(sUrlDestFiltered) 
					&& !jscallPatt.test(sUrlDestFiltered)
					&& !isGoogleLink(sUrlDestFiltered)) {
				bClickOut = true;
			}
		}
		return bClickOut;
	}
	
	function getBaseDomain(){
		var hostname = window.location.hostname;
		return hostname.substring(hostname.lastIndexOf(".",hostname.lastIndexOf(".")-1));
	}
	
	function isSocialMenuSiteLink(sUrlDest){
		var isSMSL = false;
		var sites=jQuery("div.sites a[href]");
		if(sites != null){
			for(var i=0;isSMSL == false &&  sites.length>i ;i++){
				if(sUrlDest == sites[i].href){
					isSMSL = true;
				}
			}
		}
		return isSMSL;
	}
	
	
	function setXitiEvent(anchor, name, type, openwin) {
		var oldxtkey = xtkey;
		xtkey = true;
		
		xt_click(anchor,'C', xtn2, name, type, anchor.href, openwin);
		xtkey = oldxtkey;
		return false;
	}
	
	
	function customXitiClick(){
		var customElems = [['#candidate-login', '.header.c.login.', 'N'], 
						['#candidate-signin', '.header.c.registration.', 'N'], 
						['#im-candidate', '.header.c.login.', 'N'],
						['#im-employer', '.header.e.login', 'N'],
						['#showMoreOffers', '.home_ver_todas_ofertas', 'N'],
						['#show_ultima_ofertas', '.home_ver_mas_ofertas', 'A']];
		
		for(i=0; i<customElems.length; i++) {
			var xitiContextData = {};
			xitiContextData.xitiName = customElems[i][1];
			xitiContextData.xitiType = customElems[i][2];
			
			xitiFunction = function(eventDetails) {
				var href = eventDetails.currentTarget.href.replace(location.protocol+"//"+location.hostname, "");
				clickLabel = xtpage + this.xitiName;
				
				setXitiEvent(eventDetails.currentTarget, clickLabel, this.xitiType, 0);
			};
			
			// The references to this within the xitiFunction are replaced by the object xitiContextData
			// because the references to external variables as the vector index (i) are dinamically binded by default
			xitiCallWithContext = jQuery.proxy(xitiFunction, xitiContextData);
			
			jQuery(customElems[i][0]).click(xitiCallWithContext);
		}
	}	
	
	
	
	function getXitiElementName(anchor) {
		var name = "";
		
		if (anchor.name) {
			name = anchor.name;
		} else if (anchor.href) {
			name = anchor.href.replace(location.protocol+"//"+location.hostname, "");
			name = name.indexOf("?", 0)>0?name.substr(0, name.indexOf("?", 0)):name;
		} else if (anchor.id) {
			name = anchor.id
		}
		
		return name;
	}
	
	// ]]>
</script>
<script type="text/javascript">
	var ttr = [{"page":"jobsearch::search-results::search-results-list.xhtml", "event":"SEARCH_RESULTS_PAGE_VIEWS", "time":"P"},
			{"page":"candidate::cv-views::companies.xhtml", "event":"WHO_SEES_ME_CLIENT", "time":"B"}, 
			{"page":"candidate::applications::applications-list.xhtml", "event":"APPLICATION_TRACKING_CLIENT", "time":"L"},
			{"page":"employer::manage::offers-management-view.xhtml", "event":"OFFERS_MNGM_CLIENT", "time":"P"}];
	
	
	function track(when) {
		for(i=0; i<ttr.length; i++) {
			if (xtpage.indexOf(ttr[i].page) > 0 && (ttr[i].time == "B" || ttr[i].time == when) ) {
				Tracking.Event.doTrace(ttr[i].event, null);
			}
		}
	}
</script>


			var _querystring_mas_cursos = "welcome=1#xtor=AL-228-[normal]-[ij]|[mas_cursos_galactico]"; 
			var _querystring_logo = "welcome=1#xtor=AL-228-[normal]-[ij]|[logo_galactico]"; 
			var _search = "";
			var _show = (!$("div.cabecera-corporativa").length);
			var _showExecutive = (!$('#job.executive,.icon-executive').length);
			var _OV = 1147;

		    if (location.href.search('IJ_PUSH')!=-1) {
		        $.cookie('IJ.Push', '', { path: '/' });
		        _OV='1153';
		    }
		          
		    if ($.cookie('IJ.Push')) {
		        _OV='1153';
		    }

		   
			// Remove courses, if found
			if (_showExecutive) {
				if ($('h1.heading-a').length)
					return true;
					
				if ($("#courses")) $("#courses").remove();

				if (_show)
					$(".container-expanded .col-4").append("<div style='margin:0px 20px 20px 30px;font-size:16px;border:1px solid #d5e1e5'><strong style='padding:15px 10px 10px;display:block;border-bottom:1px solid #d5e1e5;font-size:18px;background:#f2f2f2;text-align:center'>Amplía tu formación</strong><ul id='side_courses' style='list-style-type: none;padding:0px;margin:0px'></ul>        <a href='http://www.lectiva.com/buscar.htm?" + _querystring_mas_cursos + "' style='display:block;text-align:center;padding:10px;text-decoration:none;font-size:16px;background:#f2f2f2;text-decoration:underline'> Ver más cursos</a></div>");

				var _url = "//services.lectiva.com/cfs/async.aspx?i=6&tr=1&ov="+_OV+"&morefields=1&tc=" + _total + "&il=" + _provinceid + "&ic=" + _categoryid + "&is=" + _subcategoryid;
				
				if (_querystring.length)
					_url += "&qp=" + encodeURIComponent(_querystring);
				
				$.ajax({
					url: _url,
					dataType: "jsonp",
					success: function (result) { cfs_render(result); }
				});
			}
	 
			function courses_without_excluded(courses) {
				var show_course = true;
				var cou_cookie = $.cookie('IJ.BusquedaOfertas.CursosExcluidos');
				
				if (cou_cookie) {
					var cou_ids = cou_cookie.split(',');
					
					for (j=0; j< courses.length; j++) {
						for (i=0; i<cou_ids.length; i++) {
							if (cou_ids[i] == courses[j].id)
								courses.splice(j, 1);
						}
					}
				}
			}

			function cfs_render(courses) {
				courses_without_excluded(courses);
			 
				 if (courses.length) {
					 $.each(courses, function(index, course) {
						 if (index < 3
							&& _show
							&& _showExecutive)
						{
							cfs_include_course_side(course);
							
							if ($(".secondary-content").height()+100>=$(".main-content").height()) {
								$(".main-content").css('min-height',$(".secondary-content").height()+100);
								$("#job .DatosTabulados.info-proceso").css('margin-top','100px');
							}
						}
					 });
				}
			}
	 
			$("#side_courses li").live("click", function(e) {
				location.href = $(this).children('a').attr('href');
			});

			function cfs_include_course_side(course) {
				$("#side_courses").append($(
					"<li style='margin:auto;padding:20px;font-size:16px;line-height:18px;background:#FFF;border-bottom:1px solid #d5e1e5;cursor:pointer'>" +
					"	<a href='http://www.infojobs.net/madrid/programador-pl-sql/of-id3cecd39904a069557cdc42bd023d9/&quot;&#32;+&#32;course.url&#32;+&#32;&quot;' style='text-decoration:none'><b>" + course.name + "</b></a>" +
					"	<p style='margin:5px 0px 0px;color:#666666;font-weight:normal;font-size:14px;line-height:16px'>" + course.degree + " | " + (course.place != "" ? course.place : course.ubication) + "</p>" +
					"</li>"
				));
			}
		});
		//]]>
	</script>

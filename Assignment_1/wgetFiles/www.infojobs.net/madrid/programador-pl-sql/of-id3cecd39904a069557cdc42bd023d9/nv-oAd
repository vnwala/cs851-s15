	// xt_pi = "&f6=Al menos 1 año&f7=Madrid&f8=1&f9=23475864772&f12=&f20=Programas informáticos&f11=PACK&f10=ps&f16=Castinfo&f15=0&f14=false&f13=&f19=false&f18=(Seleccionar)&f17=true&f1=España-Madrid&f3=Programación&f2=Informática y telecomunicaciones&f5=48803334&f4=Ciclo Formativo Grado Superior";		
	


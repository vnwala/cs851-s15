var isLoggedin = false;

(function(){
    var r=function(){
            $('body').hide();
            location='https://www-ssl.intel.com/content/www/xa/en/tablets/great-tablets.html'+location.search;
        },
        h=location.hostname.toLowerCase();
    if(document.cookie.indexOf('SMSESSION')!=-1){isLoggedin = true;}
    if((document.cookie.indexOf('SMSESSION')!=-1) && (location.protocol=='http:')){r();}
    else if(h.indexOf('.intel.com',h.length-10)===-1){
        $.ajax({
            dataType:'jsonp',
            url:'https://www-ssl.intel.com/libs/apps/intel/myintel/profile.html/validate/loggedin',
            success:function(d,s,x){if(x.status==200 && d.isLoggedIn){r();}}
        });
    }
})();

</script>

  <script>
    $(function() {
      if (window.displayTealiumTags == true) {

        var tealiumProductDataInfo = tealiumProductData;
        var productIds = new Array();
        var uid = JSON.parse($.cookie('uid'));

        $.each(tealiumProductDataInfo, function(keys, values) {
          $.each(values, function(key, value) {
            if (key == 'product_id') {
              productIds.push(value);
            }
          });
        });
        var dataToPost = {
          site_category1: "",
          product_id: productIds,
          customer_email: uid != null ? CryptoJS.AES.decrypt(uid.e, uid.s, { format: PLNDR.Utilities.cryptoJsonFormatter }).toString(CryptoJS.enc.Utf8) : null
        };
        var query = $.deparam.querystring();
        if (query.keyword) {
          dataToPost["search_keyword"] = query.keyword;
          dataToPost["search_results"] = "0";
        }
        tealium_pagetracking(dataToPost, "product", "browse_products");
      }
    });
  </script>


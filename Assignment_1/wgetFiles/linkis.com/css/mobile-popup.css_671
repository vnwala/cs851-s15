.overlay{
    height: 110%; /* more overlay for windows phone */
}
.mobile-popup-holder{
	position:fixed;
	top:0; left:0; right:0; bottom:0;
	z-index:130;
	text-align:center;
}
.mobile-popup-insider{
	display:table;
	height:100%;
	width:100%;
	vertical-align:middle;
	text-align:center;
}
.mobile-popup-content{
	display:table-cell;
	vertical-align:middle;
	text-align:center;
}
.mobile-popup-content-insider{
	position:relative;
	width:70%;
	padding:0 17px 17px 17px;
	margin:auto;
	background:#fff;
	border-radius:7px;
	text-align:center;
}
.mobile-popup-holder .popup-close{
	position:absolute;
	top:-6%;
    right:-8%;
	display:block;
    padding: 10%;
}
.mobile-popup-holder h1{
	float:left;
	margin:20px 0;
	font-size:16px;
	font-weight:bold;
	color:#666;
	font-family:'PT Sans', sans-serif;
    line-height: 100%; /* make line height = font size and make correct background logo resize */
    text-align: left;
    width: 92%;
}
.mobile-popup-holder h2{
	float:none;
	clear:left;
	margin:20px 0;
	font-size:16px;
	font-weight:normal;
	color:#666;
	font-family:'PT Sans', sans-serif;
    text-align: left;
}

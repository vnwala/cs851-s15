    show_aw_popup: 0,
    stat_id: null,
    nopopups: 0,
    addThisSrc: 'http://s7.addthis.com/js/300/addthis_widget.js#async=1&pubid=',
    addThisId: 'ra-513082ca32a826c6',
    referrer_link: '',

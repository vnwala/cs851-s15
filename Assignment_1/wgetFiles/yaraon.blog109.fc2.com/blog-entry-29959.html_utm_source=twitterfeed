function mkToday() {
var bgcol='#bbffff';   /* 「当日」の背景色 */
var today=new Date();
if (today.getFullYear()==2015 && today.getMonth()==02-1){
var D=">"+today.getDate()+"<";
T=document.getElementById("navical").getElementsByTagName("TD");
for (i=0;i<T.length;i++) {
 if (T[i].innerHTML.indexOf(D)>-1) {T[i].style.background=bgcol;}
} } }

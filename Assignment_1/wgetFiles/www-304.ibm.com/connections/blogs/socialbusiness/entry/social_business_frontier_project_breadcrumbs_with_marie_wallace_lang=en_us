<script>
	function showFeedTip(focusId) {
   		showDialog("Right-click and select Copy Shortcut to get the URL to this feed.", focusId);//alert
	}

	function jumpToPage(to, max) {
		if(to > max) {
			to = max;
		}

		if(to < 1) {
			to = 1;
		}

		to = to - 1;
		var base = "https://www-304.ibm.com/connections/blogs/socialbusiness?sortby=0&maxresults=15&lang=en_us";
		window.location = base + "&page=" +  to;
	}

	function jumpToPageForComment(to, max) {
		if(to > max) {
			to = max;
		}

		if(to < 1) {
			to = 1;
		}

		to = to - 1;
		var base = "https://www-304.ibm.com/connections/blogs/socialbusiness/entry/social_business_frontier_project_breadcrumbs_with_marie_wallace?maxresults=15&lang=en_us";
		window.location = base + "&page=" +  to;
	}
</script>


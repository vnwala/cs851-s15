                   //The parameters to pass to xhrGet, the url, how to handle it, and the callbacks.
                   var elsewhereXhrArgs = {
                       //url: website+elsewhereFileName,
                       url: elsewhereUrl,
                       handleAs: "text",
                       load: function(data) {
                           elsewhereTargetNode.innerHTML = data;
                       },
                       error: function(error) {
                           elsewhereTargetNode.innerHTML = "An unexpected error occurred when include the elsewhere file: " + error;
                       }
                   }

                   //Call the asynchronous xhrGet
                   var deferred = dojo.xhrGet(elsewhereXhrArgs);
               </script>

    		 		  if(this.receivers.length == 0){
    		 		 		this.form().sendBtn.disabled = true;
    		 		 		this.form().sendBtn.className = "lotusFormButton lotusBtnDisabled";

    		 		 		dojo.query('[name="notifyLabelTR"]', this.form())[0].style.display = "none";

    // Url template
    var utf8_url = '//www.ibm.com/common/js/dynamicnav/www/{{locale}}-utf8.js';

    // Set locale 
    var locale = locales[djConfig.locale];
    if (!locale)
        locale = locales['en'];

    // Fetch file
    utf8_url = utf8_url.replace('{{locale}}', locale);
    if (_supportsIOScript())
    {
        dojo.io.script.get({ url: utf8_url});
    }
    else
    {
        var script = document.createElement('script');
        script.setAttribute('type', 'text/javascript');
        script.setAttribute('src', utf8_url);
        document.getElementsByTagName('body')[0].appendChild(script);
    }
}());
    

ibmweb.dynnav  = {
    dataCallback: function(args)
    {
        var ul = document.createElement('ul');
        for (var i = 0; i < args.footerLinkData.length; i++)
        {
            var span = document.createElement('span');
            console.log("span :"+span);
            if (span.textContent)
                span.textContent = args.footerLinkData[i].name;
               
            else
                span.innerHTML = args.footerLinkData[i].name;
            
            var a = document.createElement('a');
            a.href = args.footerLinkData[i].link;
            a.appendChild(span); 
            
            var li = document.createElement('li');
            
            li.appendChild(a);
            ul.appendChild(li);
        }
        

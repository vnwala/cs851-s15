	app_ot_usr = 'guest';
} else if (userType==='205' || userType==='208') {
	app_ot_usr = 'reg';
} else if (userType === '206' || parseInt(userType) >= 206 ) {
	app_ot_usr = 'vip';
}

$app_ot_btns.on('click',function(){
	var $this = $(this);
	if (!$this.hasClass('hm_ot_selected')) {
		$app_ot_btns.removeClass('hm_ot_selected');
		$this.addClass('hm_ot_selected');
		categoryView($this.data('tabcat'),'',app_ot_usr);
		tabga($this.data('tabcat'));
	}
	$this = null;//just in case... 
});

var milf = null;

if(milf)
{
    $app_ot_btns.removeClass('hm_ot_selected');
    $('#milf_performers').addClass('hm_ot_selected');
    categoryView('milf_performers','',app_ot_usr);
    milf = null;
}

<script>
if( typeof ga$ != 'undefined' )
{
    (function($)
    {
        if( getCookie('NetiA') && typeof dxlive_iframe_key != 'undefined' )
        {
            $("iframe").each(function(e)
            {
                if($(this).attr('src') != 'http://www.dxlive.com/banner/20001117' &&
                   $(this).attr('src') != 'http://www.dxlive.com/banner/home/20001117' &&
	           $(this).attr('src') != 'http://bn.dxlive.com/banner/20001117' &&
		   $(this).attr('src') != 'http://bn.dxlive.com/banner/home/20001117')
                   return;

                var user_name = getCookie('NetiA').split(":")[0];
                var new_src = $(this).attr('src') + "?user_name=" + user_name + "&md5=" + dxlive_iframe_key;
                $(this).attr('src', new_src);
            })
        }
    })(ga$)
}
</script>


<script type="text/javascript">
// INSERT THE VERTICAL ALIGN SCRIPT
window._load_queue.push(function(){
if (jQuery("js_product_usp").length === 0) { return; }
window._load_queue.push(
"//s.s-bol.com/nl/static/js/dev/widgets/vertical_align-min.3185428754.js",
function() {
new BOL.widgets.VerticalAlign($("js_product_usp"), {
'alignObjectClass' : '.usp_text',
'alignContainerClass' : '.usp_main'
});
}
);
});
// INSERT THE TOGGLE SCRIPT
window._load_queue.push(function() {
// TAB TOGGLES FOR BUYBLOCK
var buyblockTabGroup = new BOL.utils.Collection();
toggleIdx = 0;
$$('.js_toggle_tab').each( function(toggle) {
toggleContent = $$('.js_toggle_tab_content')[toggleIdx];
toggleTabGroup = $$('.js_toggle_tab_group')[0];
var buyblockTab = new BOL.widgets.ToggleGrouped({
'trigger': $(toggle),
'target': $(toggleContent),
'group': buyblockTabGroup,
'event': 'click',
'activeClassName': 'js_active',
'container': toggleTabGroup,
'openClassName': 'active',
'closedClassName': 'inactive'
});
buyblockTabGroup.items.push(buyblockTab);
toggleIdx++;
});
// check hash to open tab by default.
if ( buyblockTabGroup.items.size() > 0 ) {
hash = location.hash;
toggleMatch = false;
if (hash != '' ) {
anchor = $$('a[name="' + hash.replace('#','') + '"]')[0];
if (typeof anchor !== 'undefined') {
tabId = anchor.readAttribute('data-open_tab');
for (var index = 0; index < buyblockTabGroup.items.size(); ++index) {
var item = buyblockTabGroup.items[index];
if (item._options.trigger.id == tabId) {
buyblockTabGroup.items[index].openToggle();
toggleMatch = true;
}
}
}
}
if (!toggleMatch) {
buyblockTabGroup.items[0].openToggle();
}
}
// TOGGLE MORE LINKS.
$$('.js_readmore_content').each(function(toggle) {
BOL.ToggleMoreDescription = new BOL.widgets.ToggleMore({
'openClassName' : 'js_link-less',
'closedClassName' : 'js_link-more',
'trigger' : $(toggle).select('.js_readmore_trigger a')[0],
'target' : $(toggle).select('.wrapper')[0],
'triggerContainer' : $(toggle).select('.js_readmore_trigger')[0],
'container' : $(toggle),
'contentHeightClass' : $(toggle).select('.content')[0]
});
});
});
// SCROLL TO TOP
window._load_queue.defer(function(){
window._load_queue.push(
"//s.s-bol.com/nl/static/js/dev/widgets/scrolltop-min.2286427366.js",
function() {
new BOL.widgets.scrollTop();
}
);
});
if ('true' == 'true') {

<script type="text/javascript">
function MainMenu(isLocked) {
this.allCategoriesEl = jQuery('#main_menu_bar .main_menu_bar__all_categories_button');
this.allCategoriesPlaceholderEl = jQuery('#category_menu .menu__all_categories_placeholder');
this.categoryMenuWrapperEl = jQuery('#category_menu');
this.categoryMenuEl = this.categoryMenuWrapperEl.find('.menu:first');
this.expandedDetailsEl = this.categoryMenuWrapperEl.find('.details:first');
this.clickToCloseEl = this.expandedDetailsEl.find('a.click_to_close');
this.compareOffsetX = -1;
this.compareStartYOffset = -1;
this.lastOffset = {x: -1, y: -1};
this.menuHideTimeout = 0;
this.hideDetailsTimeout = 0;
this.autoShowTimeout = 0;
this.preventOpenTimeout = 0;
this.currentLinkEl = null;
this.categoryMenuLocked = isLocked; // menu is locked only on homepage
this.preventMouseMeasure = true; // we only want to measure mouse moves when menu is open
this.closedByPlaceholder = false; // prevents mouseenter even firing when menu just closed from placeholder
this.clickPrevent = false; // prevents hiding right after hover showed menu
this.bindEvents();
if (this.categoryMenuLocked) {
jQuery(document).on('mousemove', this.documentMeasureMousemove);
//this.allCategoriesEl.addClass('locked');
//this.categoryMenuWrapperEl.show().addClass('locked');
this.calculateHeight();
this.expandedDetailsEl.data('prevent-open', true);
}
var allCategoriesText = this.allCategoriesEl.find('strong.category_text').text();
this.allCategoriesPlaceholderEl.find('strong.category_text').text(allCategoriesText);
this.categoryMenuEl.find('ul li:last').addClass('no_bottom_border');
this.clickToCloseEl.hide();
};
MainMenu.prototype = {
bindEvents: function() {
for (key in this) {
if (typeof this[key] === 'function') {
this[key] = jQuery.proxy(this[key], this);
}
}
this.clickToCloseEl.click(this.clickToCloseElClick);
this.categoryMenuEl.find('ul:first').mouseenter(this.enableMouseMeasure).mouseleave(this.disableMouseMeasure)
.find('li a')
.mouseover(this.expandedMenuLinkElMouseOver)
.mouseenter(this.expandedMenuLinkElMouseEnter)
.mouseleave(this.expandedMenuLinkElMouseLeave)
.click(this.expandedMenuLinkElClick);
this.allCategoriesPlaceholderEl.click(this.allCategoriesPlaceholderElClick);
this.allCategoriesEl.mouseenter(this.allCategoriesElMouseEnter).mouseleave(this.allCategoriesElMouseLeave).click(this.allCategoriesElClick);
this.categoryMenuWrapperEl.mouseover(this.categoryMenuMouseOver).mouseleave(this.categoryMenuMouseLeave);
this.categoryMenuWrapperEl.find('.close_button').click(this.hideCategoryMenu);
this.expandedDetailsEl.click(this.preventHide)
if (this.categoryMenuLocked) {
this.expandedDetailsEl.mouseenter(this.clearHideTimeout);
}
jQuery(document)
.on('click', this.bodyClick)
.on('bolmenu:show', this.toggleCategoryMenu);
// touch specific binds
var startedOnTouch = false;
jQuery('.menu_surrounding_background')
.live('touchstart', function() {
startedOnTouch = true;
})
.live('touchend', jQuery.proxy(function() {
if (startedOnTouch) {
this.hideCategoryMenu();
}
}, this));
},
// Event Handlers
// *******************************************
allCategoriesElClick: function(e) {
e.preventDefault();
e.stopPropagation();
this.clearTimeouts();
this.toggleCategoryMenu();
},
allCategoriesPlaceholderElClick: function(e) {
e.preventDefault();
e.stopPropagation();
if (this.clickPrevent) { return; }
this.clearTimeouts();
this.toggleCategoryMenu();
this.closedByPlaceholder = true;
},
allCategoriesElMouseEnter: function() {
if (this.closedByPlaceholder) { return; }
if (!this.categoryMenuWrapperEl.is(':visible')) {
this.clearTimeouts();
this.menuHideTimeout = setTimeout(this.showCategoryMenu, 200);
}
},
allCategoriesElMouseLeave: function() {
this.clearTimeouts();
this.closedByPlaceholder = false;
},
clickToCloseElClick: function(e) {
e.preventDefault();
this.hideCategoryMenu();
},
// Category menu events
categoryMenuMouseOver: function() {
this.clearTimeouts();
// if (this.menuHideTimeout > 0) { clearTimeout(this.menuHideTimeout); }
this.expandedDetailsEl.addClass('expanded');
},
categoryMenuMouseLeave: function() {
this.menuHideTimeout = setTimeout(this.hideCategoryMenu, 500);
},
expandedMenuLinkElMouseLeave: function(e) {
if (this.categoryMenuLocked) {
this.setHideTimeout();
}
this.clearAutoShowTimeout();
if (this.currentLinkEl === null) return;
if (e.currentTarget == this.currentLinkEl[0]) {
this.compareOffsetX = Math.round(this.lastOffset.x);
this.compareStartYOffset = Math.round(this.lastOffset.y);
this.compareMousemove = true;
} else {
this.compareMousemove = false;
}
},
expandedMenuLinkElMouseEnter: function(e) {
if (this.categoryMenuLocked) { this.clearHideTimeout(e); }
this.setAutoShowTimeout();
},
expandedMenuLinkElMouseOver: function(e) {
this.currentLinkEl = jQuery(e.currentTarget);
},
expandedMenuLinkElClick: function(e) {
if (this.preventOpenTimeout > 0) { clearTimeout(this.preventOpenTimeout); }
this.expandedDetailsEl.data('prevent-open', false);
this.currentLinkEl = jQuery(e.currentTarget);
this.showDetails();
return false;
},
preventHide: function(e) {
if (e.target.tagName !== "A") {
e.preventDefault();
e.stopPropagation();
return false;
}
},
resetClickPrevent: function() {
this.clickPrevent = false;
},
// Mouse measure action
enableMouseMeasure: function() {
this.preventMouseMeasure = false;
},
disableMouseMeasure: function() {
this.preventMouseMeasure = true;
this.compareMousemove = false;
},
documentMeasureMousemove: function(e) {
if (this.preventMouseMeasure) { return true; }
var targetEl = jQuery(e.target);
if (this.compareMousemove === true) {
var offsetX = e.offsetX==undefined ? e.originalEvent.layerX : e.offsetX;
var offsetY = e.offsetY==undefined ? e.originalEvent.layerY : e.offsetY;
var distanceY = Math.abs(this.compareStartYOffset - offsetY),
distanceX = Math.abs(this.compareOffsetX - offsetX);
if (this.compareOffsetX >= offsetX || (distanceY > 36 && distanceY >= distanceX)) {
this.compareMousemove = false;
this.currentLinkEl = targetEl;
this.showDetails();
}
} else {
if (targetEl.prop('tagName') == 'A') {
this.currentLinkEl = targetEl;
this.showDetails();
}
}
this.lastOffset = {
x: e.offsetX==undefined ? e.originalEvent.layerX : e.offsetX,
y: e.offsetY==undefined ? e.originalEvent.layerY : e.offsetY
};
},
bodyClick: function(e) {
var clickedEl = jQuery(e.target);
if (clickedEl.parents('#main_menu_bar').length !== 1 && clickedEl.parents('#expanded_category_menu').length !== 1) {
this.hideCategoryMenu();
}
return true;
},
// Other functions
// ******************************************
hideDetails: function() {
this.clickToCloseEl.hide();
this.categoryMenuEl.find('li a').removeClass('sub_expanded');
this.expandedDetailsEl.hide().removeClass('expanded').css({
width: 'auto'
}).find('.item').removeClass('visible');
this.categoryMenuWrapperEl.removeClass('expanded').css({
width: '194px'
});
this.expandedDetailsEl.data('prevent-open', true);
},
hideCategoryMenu: function() {
if (!this.categoryMenuLocked) {
jQuery(document).off('mousemove', this.documentMeasureMousemove);
}
this.clearTimeouts();
this.hideDetails();
if (this.categoryMenuLocked === false) {
if (typeof this.overlayEl !== 'undefined') {
if (WSP.utils.has3DSupport()) {
this.overlayEl.removeClass('darken');
var el = jQuery('.menu_surrounding_background:first');
setTimeout(function() { el.remove(); }, 200);
} else {
this.overlayEl.remove();
}
}
this.categoryMenuWrapperEl.hide();
}
},
showCategoryMenu: function() {
jQuery(document).on('mousemove', this.documentMeasureMousemove);
this.categoryMenuWrapperEl.show();
this.calculateHeight();
if (this.categoryMenuLocked === false) {
// show overlay if not locked
this.overlayEl = jQuery('<div class="menu_surrounding_background" />');
jQuery(document.body).prepend(this.overlayEl);
this.overlayEl.css({
height: jQuery(document).height()
}).addClass('darken');
// prevent click for a small period of time (in case it started automatically via mouseenter)
this.clickPrevent = true;
setTimeout(this.resetClickPrevent, 500);
jQuery(document).trigger('bolmenu:showed_category_menu');
}
},
showDetails: function() {
if (this.currentLinkEl === null) { return; }
if (this.categoryMenuLocked && this.expandedDetailsEl.data('prevent-open')) {
this.preventOpenTimeout = setTimeout(jQuery.proxy(function(){
this.expandedDetailsEl.data('prevent-open', false);
}, this), 200);
return;
}
this.clearAutoShowTimeout();
this.compareOffsetX = -1;
// magic auto sizing happens here
this.clickToCloseEl.hide();
this.categoryMenuWrapperEl.addClass('expanded');
this.categoryMenuEl.find('li a').removeClass('sub_expanded');
this.currentLinkEl.addClass('sub_expanded');
this.expandedDetailsEl.show().find('.item').removeClass('visible').filter("[data-item-id='" + this.currentLinkEl.data('item-id') + "']").addClass('visible');
this.expandedDetailsEl.css({ width: 'auto' });
this.categoryMenuWrapperEl.css({ width: '972px' });
var width = this.expandedDetailsEl.find('.item.visible').width() + 50;
if (width > 766) {
width = 776; // Max width of details
} else if (width < 100) {
width = 100; // Min width of details
}
this.expandedDetailsEl.width(width);
this.categoryMenuWrapperEl.width(width + 194 + 1); // 1 is border
this.clickToCloseEl.show();
jQuery(document).trigger('bolmenu:showed_category_menu_details');
jQuery(document).trigger('wsp:simplemenu_hide_menus');
},
toggleCategoryMenu: function() {
if (this.categoryMenuWrapperEl.is(':visible')) {
this.hideCategoryMenu();
} else {
this.showCategoryMenu();
}
},
calculateHeight: function() {
var height = this.categoryMenuEl.height();
this.expandedDetailsEl.height(height);
},
// autoshowTimeout specific
clearAutoShowTimeout: function() {
if (this.autoShowTimeout > 0) { clearTimeout(this.autoShowTimeout); }
},
setAutoShowTimeout: function() {
this.clearAutoShowTimeout();
this.autoShowTimeout = setTimeout(this.showDetails, 500);
},
// hideTimeout specific
clearHideTimeout: function() {
if (this.hideDetailsTimeout > 0) { clearTimeout(this.hideDetailsTimeout); }
},
setHideTimeout: function() {
this.clearHideTimeout();
this.hideDetailsTimeout = setTimeout(this.hideDetails, 200);
},
clearTimeouts: function() {
if (this.menuHideTimeout > 0) { clearTimeout(this.menuHideTimeout); }
if (this.preventOpenTimeout > 0) { clearTimeout(this.preventOpenTimeout); }
if (this.categoryMenuLocked) { this.clearHideTimeout(); }
}
};
new MainMenu(false);
</script>


<script type="text/javascript">
window._load_queue.instant(
function(){
if (WSP.utils.Cookie.isCookieSet('hideCategoryHint') || WSP.utils.Cookie.isCookieSet('hideCategoryHintSession')) return;
var targetEl = jQuery('#main_menu_bar .main_menu_bar__all_categories_button');
var flextip = new WSP.core.Flextip({
targetEl: targetEl,
content: jQuery("<div class=\"category_hint small_details\">Vanaf nu vind je alle categorie&euml;n gemakkelijk onder &eacute;&eacute;n knop.</div>"),
template: WSP.core.templates.Flextip.right,
flextipType: 'category_information',
calculationOffset: { left: 1, top: -3 },
showCloseButton: true,
hidePointer: false,
zIndex: 30
});
targetEl.on(WSP.events.Flextip.closeClick, function(){
WSP.utils.Cookie.write('hideCategoryHint', true, 3650, false); // Ten years seem long enough.
WSP.utils.Cookie.remove('categoryHintTimesShown');
});
var flextipRemove = function() {
flextip.remove();
jQuery(document)
.off('bolmenu:showed_category_menu_details', flextipRemove)
.off('bolmenu:showed_category_menu', flextipRemove);
};
jQuery(document)
.on('bolmenu:showed_category_menu_details', flextipRemove)
.on('bolmenu:showed_category_menu', flextipRemove);
var categoryHintTimesShown = parseInt(WSP.utils.Cookie.read('categoryHintTimesShown') || 0);
if (categoryHintTimesShown < 2) {
WSP.utils.Cookie.write('categoryHintTimesShown', categoryHintTimesShown+1);
} else {
WSP.utils.Cookie.write('hideCategoryHintSession', true);
WSP.utils.Cookie.remove('categoryHintTimesShown');
}
}
);
</script>


window._load_queue.push(function() {
if(!WSP.utils.touchSupported()) {
return;
}
var inputEl = jQuery('#searchfor'),
inputWrapperEl = jQuery('.js_search_input'),
removeButtonEl = jQuery('#searchfor_input_remove');
var enableButton = function() {
removeButtonEl.show();
};
var disableButton = function() {
removeButtonEl.hide();
};
var updateButtonState = function() {
if(inputEl.length == 0 || (inputEl.length == 1 && inputEl.val().length == 0)) {
disableButton();
} else {
enableButton();
}
};
var buttonClick = function() {
inputEl.val('').focus();
updateButtonState();
};
removeButtonEl.click(buttonClick);
inputEl.keyup(updateButtonState);
updateButtonState();
});
</script>




<script type="text/javascript">
window._load_queue.push(function(){
jQuery('.bol_search .search_button').hover(function() {
jQuery(this).addClass('over');
}, function() {
jQuery(this).removeClass('over');
});
});
window._load_queue.push(function() {
if (WSP.utils.touchSupported()) {
return; // disable sticky bar for touch devices
}
// Sticky Header
var docEl = jQuery(document),
stickyHeader = jQuery('.js_sticky_header'),
stickyHeaderOffset = stickyHeader.offset().top,
offsetEl = jQuery('#content_wrapper');
jQuery(window).on('throttledscroll', function() {
if(docEl.scrollTop() > stickyHeaderOffset) {
if (!stickyHeader.hasClass('sticky')) {
stickyHeader.addClass('sticky');
offsetEl.css({
'margin-top': stickyHeader.height()
});
docEl.trigger('bolstickybar:focusto', 'sticky');
}
} else {
if (stickyHeader.hasClass('sticky')) {
stickyHeader.removeClass('sticky');
offsetEl.css({
'margin-top': '0px'
});
docEl.trigger('bolstickybar:focusto', 'top');
}
}
});
});
</script>


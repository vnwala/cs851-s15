/****************************************
	Menu
 ****************************************/
 
#Menu {
	clear: both;
	margin: 0 auto;
	padding: 0 10px;
	height: 39px;
	position:relative;
	z-index:29;
	background: #fff; 
	width:auto;
	text-align:center;
}

#Menu ul {
	margin: auto;
	padding: 0;
	display: inline-block;
}

#Menu li {
	float: left;
	zoom: 1;
	background: #fff;
	z-index: 2;
	list-style-type:none;
}

#Menu li a {
	font-family:'Bitter', Arial;
	display: block;
	float: left;
	text-decoration: none;
	padding:9px 30px;
	color: #000;
    border:0px;
    list-style: none outside none;
	font-size: 16px;
	text-transform: uppercase;
	letter-spacing: 1px;
	border-top:3px solid #fff;
	font-weight:400;
}

#Menu li > a {
	display: block;
	/*background: black;*/
	z-index:1;
}

#Menu li a:hover, #Menu li:hover a{
	text-decoration: none;
	background:#000;
	background:rgba(0,0,0,0.8);
	color: #fff;
	border-top:3px solid #000;
}

#Menu li ul {
	clear: left;
	text-align: left;
	font-size: 0.9em;
	background:#000;
	background:rgba(0,0,0,0.8);
	margin-left:0;
	visibility:hidden;
  	opacity:0;
  	-o-transition:visibility 0s linear 0.2s,opacity 0.2s linear;
  	-webkit-transition:visibility 0s linear 0.2s,opacity 0.2s linear;
  	-moz-transition:visibility 0s linear 0.2s,opacity 0.2s linear;
  	transition:visibility 0s linear 0.2s,opacity 0.2s linear;
	font-size: .9em;
	left: -60000px;
	position: absolute;
	text-align: left;
	top: 39px;
	width: 250px;
	padding:20px 10px
}

#Menu li ul li {
	color: #fff;
	text-align: left;
	width:100%;
	background:transparent;
	padding:0;
	border-top:0;
}

#Menu li.hasSubCats ul li a {
	padding: 5px;
	font-size: 1em;
	text-align: left;
	margin: 0px 6px 4px 0px;
	width: 100%;
	height: auto;
	font-weight:400;
	color:#fff;
	background:transparent;
	border-top:0;
	-webkit-box-sizing:border-box;
	-moz-box-sizing:border-box;
	box-sizing:border-box;
}
#Menu li a.redcat, #Menu li ul li a.redcat{
	color:red;
}

#Menu li.hasSubCats,#Menu li.noSubCats {
	z-index:2;
}
#Menu li.noSubCats {
	z-index:1;
}

#Menu li.hasSubCats:hover {
	position:relative;
}


#Menu li ul li a:hover {
	text-decoration: none;
	background:#fff;
	color:#000;
}

#Menu li:hover ul, #Menu li.over ul {
	visibility:visible;
	opacity:1;
	-o-transition-delay:0s;
	-webkit-transition-delay:0s;
	-moz-transition-delay:0s;
	transition-delay:0s;
	left: 0;
}


  font-style: normal;
  font-weight: 400;
  src: local('Bitter-Regular'), url(http://themes.googleusercontent.com/static/fonts/bitter/v4/2PcBT6-VmYhQCus-O11S5-vvDin1pK8aKteLpeZ5c0A.woff) format('woff');
}

@font-face {
  font-family: 'Bitter';
  font-style: normal;
  font-weight: 700;
  src: local('Bitter-Bold'), url(http://themes.googleusercontent.com/static/fonts/bitter/v4/evC1haE-MsorTl_A7_uSGbO3LdcAZYWl9Si6vvxL-qU.woff) format('woff');
}

/* Global Reset */
* {
	margin: 0;
	padding: 0;
}

/****************************************
	General
 ****************************************/
 
body {
	font-family: Arial, sans-serif;
	font-size: 0.8em;
	height: 100%;
}

input, select {
	font-size: 11px;
}

input {
	padding: 1px;
}

a {
	font-weight: normal;
	text-decoration: underline;
}

h2{
	font-size: 1.3em;
	font-weight: bold;
}

h3 {
	font-size: 1.1em;
	margin-bottom: 6px;
}

h4 {
	font-size: 1.1em;
	margin-bottom: 4px;
}

img, img a {
	border: 0;
}

ul, ol {
	margin: 0 0 10px 30px;
}

li {
	margin: 0;
	margin-bottom: 2px;
	padding: 0;
}

blockquote {
	margin-left: 28px;
}

select option {
	padding: 0 10px;
}

hr {
	margin: 10px 0;
	padding: 0;
	background: #e6e6e8;
	height: 1px;
	border:0;
}

.CartContents tfoot .SubTotal td {
	border-top: 0px solid;
	text-align: right;
	font-weight: bold;
}

.SidePanel{
	margin-bottom:10px;
	overflow:hidden;
}

div.paginglistdiv{
	text-align:right;
	width:270px; 
	margin: 0 auto;
}

.hidden{
	display:none;
}

.ActivePage{
	text-decoration:underline;
	font-size: 0.95em;
}


#morecolours,#morestyles,#morecelebrities,#moresizes,#morebrands{
	margin-top:-10px;
	background:#c0c0c0;
	color:#333;
	height:15px;
	border:1px solid #333;
	text-align:center;
	width:80px;
	margin:auto;
	margin-bottom:20px;
}

.catproddiv{
	float: left;
	width: 224px;
	margin: 10px 5px;
	height: 365px;
	position: relative;
	border: 1px solid #fff;
	-webkit-transition: all 0.2s linear;
	-moz-transition: all 0.2s linear;
	-o-transition: all 0.2s linear;
	transition: all 0.2s linear;
	padding: 0;
}

.norightmargin{
	margin-right:0px;
}

.catproddiv img{
	width:224px;
	margin:auto;
	-webkit-transition: opacity 0.2s ease-in-out;
  	-moz-transition: opacity 0.2s ease-in-out;
  	-o-transition: opacity 0.2s ease-in-out;
  	transition: opacity 0.2s ease-in-out;	
}

.catproddiv img.secondimage{
	position: absolute;
	left: 0;
	top: 0;
	opacity: 0;
}

.catproddiv:hover img.secondimage{
	opacity:1;
}

.catproddiv:hover{
	border:1px solid #ededed;
	box-shadow:0 0 30px rgba(0,0,0,0.05);
}

.xProductImage{
	/*width:176px;*/
	padding:0px;
	overflow:hidden;
	text-align:center;
	height:304px;
	margin-bottom:8px;
}
.xProductDetails{
	text-align:center;
	margin-left:5px;
	margin-right:5px;
	overflow:hidden;
	font-size:12px;
	font-family:'Bitter', Arial, sans-serif;
	letter-spacing:1px;
	color:#999;
}

.Content .catproddiv:hover .xProductDetails a{
	color:#000;
	text-decoration:none;
}


.Content .xProductDetails a{
	text-decoration:none;
	color:#666;
}

.Content .xProductDetails a:hover{
	text-decoration:none;
}

.xProductPriceRating{
	text-align:center;
	color:#00A9C7;
	font-size:12px;
	font-weight:normal;
	letter-spacing:1px;
	font-style: normal;
	border-bottom:1px solid #fff;
	height:17px;
	position:absolute;
	bottom:10px;
	width:100%;
	margin:20px 0 0;
}

/* Forms */
.HorizontalFormContainer dl, .VerticalFormContainer dl {
	margin: 0;
}

.HorizontalFormContainer dt img {

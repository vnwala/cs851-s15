	<script language="JavaScript1.2">
		// function to hide/show DVD/VHS bestsellers
	 	// x passed in is the list to SHOW
	 	function bsToggle(x) 
	 	{
	 		if (x == 'dvd') 
			{
				document.getElementById('DVDHeader').style.color = "#ff0000";
				document.getElementById('DVDid').style.color = "#ff0000";
				document.getElementById('DVDid').style.visibility = "visible";
				document.getElementById('DVDid').style.lineHeight = "normal";
				document.getElementById('DVDid').style.height = "auto";
				document.getElementById('USEDHeader').style.color = "#000000";
				document.getElementById('USEDid').style.visibility = "hidden";
				document.getElementById('USEDid').style.lineHeight = "0pt";
				document.getElementById('USEDid').style.height = "0pt";
				document.getElementById('GAMESHeader').style.color = "#000000";
				document.getElementById('GAMESid').style.visibility ="hidden";
				document.getElementById('GAMESid').style.lineHeight = "0pt";
				document.getElementById('GAMESid').style.height = "0pt";
				document.getElementById('BLURAYHeader').style.color = "#000000";
				document.getElementById('BLURAYid').style.visibility ="hidden";
				document.getElementById('BLURAYid').style.lineHeight = "0pt";
				document.getElementById('BLURAYid').style.height = "0pt";
		  	}//if dvd
		  	if (x == 'used') 
			{
				document.getElementById('DVDHeader').style.color = "#000000";
				document.getElementById('DVDid').style.color = "#000000";
				document.getElementById('DVDid').style.visibility = "hidden";
				document.getElementById('DVDid').style.lineHeight = "0pt";
				document.getElementById('DVDid').style.height = "0pt";
				document.getElementById('USEDHeader').style.color = "#ff0000";
				document.getElementById('USEDid').style.color = "#ff0000";
				document.getElementById('USEDid').style.visibility = "visible";
				document.getElementById('USEDid').style.lineHeight = "normal";
				document.getElementById('USEDid').style.height = "auto";
				document.getElementById('GAMESHeader').style.color = "#000000";
				document.getElementById('GAMESid').style.visibility = "hidden";
				document.getElementById('GAMESid').style.lineHeight = "0pt";
				document.getElementById('GAMESid').style.height = "0pt";
				document.getElementById('BLURAYHeader').style.color = "#000000";
				document.getElementById('BLURAYid').style.visibility ="hidden";
				document.getElementById('BLURAYid').style.lineHeight = "0pt";
				document.getElementById('BLURAYid').style.height = "0pt";
		  	}//if used 
		  	if (x == 'games') 
			{
				document.getElementById('DVDHeader').style.color = "#000000";
				document.getElementById('DVDid').style.color = "#000000";
				document.getElementById('DVDid').style.visibility = "hidden";
				document.getElementById('DVDid').style.lineHeight = "0pt";
				document.getElementById('DVDid').style.height = "0pt";
				document.getElementById('USEDHeader').style.color = "#000000";
				document.getElementById('USEDid').style.visibility = "hidden";
				document.getElementById('USEDid').style.lineHeight = "0pt";
				document.getElementById('USEDid').style.height = "0pt";
				document.getElementById('GAMESHeader').style.color = "#ff0000";
				document.getElementById('GAMESid').style.color = "#ff0000";
				document.getElementById('GAMESid').style.visibility = "visible";
				document.getElementById('GAMESid').style.lineHeight = "normal";
				document.getElementById('GAMESid').style.height = "auto";
				document.getElementById('BLURAYHeader').style.color = "#000000";
				document.getElementById('BLURAYid').style.visibility ="hidden";
				document.getElementById('BLURAYid').style.lineHeight = "0pt";
				document.getElementById('BLURAYid').style.height = "0pt";
			}// if games 
		  	if (x == 'bluray')
		  	{
				document.getElementById('DVDHeader').style.color = "#000000";
				document.getElementById('DVDid').style.color = "#000000";
				document.getElementById('DVDid').style.visibility = "hidden";
				document.getElementById('DVDid').style.lineHeight = "0pt";
				document.getElementById('DVDid').style.height = "0pt";
				document.getElementById('USEDHeader').style.color = "#000000";
				document.getElementById('USEDid').style.visibility = "hidden";
				document.getElementById('USEDid').style.lineHeight = "0pt";
				document.getElementById('USEDid').style.height = "0pt";
				document.getElementById('GAMESHeader').style.color = "#000000";
				document.getElementById('GAMESid').style.color = "#000000";
				document.getElementById('GAMESid').style.visibility = "hidden";
				document.getElementById('GAMESid').style.lineHeight = "0pt";
				document.getElementById('GAMESid').style.height = "0pt";
				document.getElementById('BLURAYHeader').style.color = "#ff0000";
				document.getElementById('BLURAYid').style.color = "#ff0000";
				document.getElementById('BLURAYid').style.visibility ="visible";
				document.getElementById('BLURAYid').style.lineHeight = "normal";
				document.getElementById('BLURAYid').style.height = "auto";
			}// if bluray

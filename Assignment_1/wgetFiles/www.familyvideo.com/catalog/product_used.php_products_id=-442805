<script src="includes/ajax/ajax.js" type="text/javascript"></script>

<link href="includes/ajax/search_autocomplete.css" rel="stylesheet" type="text/css">
<script language="javascript" src="includes/menu.js"></script>


<script language='javascript'>
var timeOutID;
var suggestionMO = 0;

function letsSearch(e)
{
   //lets grab the search text so we know how long it is
   var searchString = document.getElementById( 'search' ).value;

   //grab keyboard event and figure out which key it is
   var evtobj = e; //seems like this work in IE7 / Firefox / Safari...
   var unicode=evtobj.charCode? evtobj.charCode : evtobj.keyCode;

   //IE can't use getElementsByName() reliably, so lets work around it
   //var suggestions = document.getElementsByName( 'suggestions' );

   //create a temp array with all the <li> elements from inside the results <div>
   var temp = document.getElementById( 'results' ).getElementsByTagName( 'li' );
   var suggestions = new Array();
   for ( var j = 0; j < temp.length; j++ )
   {
      if ( j > 0 && j < temp.length - 1 )
      {
         //add all the "real" <li> elements into the suggestions array
         suggestions.push( temp[j] );
      }//if not on the first or last element in the temp array
   }//for each <li> tag in the results <div>

   //alert( suggestions.length );
   if ( unicode == 40 && suggestions.length > 0 )
   {
      var anythingSelected = 0;
      var stopUpdating = 0;
      //alert( suggestions.length);

      for( var i = 0; i < suggestions.length; i++ )
      {
         if ( ( suggestions[i].className == 'suggestionsSelected' || suggestions[i].className == 'bottomAutoSelected' ) && stopUpdating == 0)
         {
            anythingSelected = 1;

            //check where we are in the suggestions box
            if ( suggestions.length == 1 )
            {
               //do nothing if this is the only suggestion
            }//if only suggestion
            else if ( i == suggestions.length - 2 )
            {
               //cycle back to start
               suggestions[i].className = 'suggestions';
               //suggestions[i + 1].className = 'bottomAutoSelected';
               suggestions[i + 1].className = 'suggestionsSelected';
               stopUpdating = 1;
            }//else if last ACTUAL suggestion
            else if ( i == suggestions.length - 1 )
            {
               //cycle back to start
               suggestions[i].className = 'suggestions';
               //suggestions[i].className = 'bottomAuto';
               suggestions[0].className = 'suggestionsSelected';
               stopUpdating = 1;
            }//else if last lne "5 results for 'searchTerm'"
            else
            {
               //go down one level
               suggestions[i].className = 'suggestions';
               var nextIndex = i + 1;
               //alert(nextIndex);
               suggestions[nextIndex].className = 'suggestionsSelected';
               stopUpdating = 1;
            }//else just somewhere in the beginning/middle
         }//if this suggestion is selected
      }//for all suggestions

      //if nothing selected, start with the first!
      if ( anythingSelected == 0 )
      {
         suggestions[0].className = 'suggestionsSelected';
      }//if nothing selected, grab the first one
   }//if down arrow AND suggestions
   else if ( unicode == 38 && suggestions.length > 0 )
   {
      var anythingSelected = 0;
      var stopUpdating = 0;

      for( i = suggestions.length - 1; i >= 0; i-- )
      {
         if ( ( suggestions[i].className == 'suggestionsSelected' || suggestions[i].className == 'bottomAutoSelected' ) && stopUpdating == 0)
         {
            anythingSelected = 1;

            if ( suggestions.length == 1 )
            {
               //do nothing if this is the only suggestion
            }//if only sugg   estion
            else if ( i == 0 )
            {
               //cycle back to last line
               suggestions[0].className = 'suggestions';
               //suggestions[suggestions.length - 1].className = 'bottomAutoSelected';
               suggestions[suggestions.length - 1].className = 'suggestionsSelected';
               stopUpdating = 1;
            }//else if at beginning of suggestions
            else if ( i == suggestions.length - 1 )
            {
               //cycle back to start
               suggestions[i].className = 'suggestions';
               //suggestions[i].className = 'bottomAuto';
               suggestions[i - 1].className = 'suggestionsSelected';
               stopUpdating = 1;
            }//else if last lne "5 results for 'searchTerm'"
            else   
            {
               //go down one level
               suggestions[i].className = 'suggestions';
               var prevIndex = i - 1;
               //alert(nextIndex);
               suggestions[prevIndex].className = 'suggestionsSelected';
               stopUpdating = 1;
            }//else somewhere in the middle
         }//if this suggestion is selected
      }//for all suggestions

      //if nothing selected, start with the last!
      if ( anythingSelected == 0 )
      {
         //suggestions[suggestions.length - 1].className = 'bottomAutoSelected';
         suggestions[suggestions.length - 1].className = 'suggestionsSelected';
      }//if nothing selected, grab the last one
   }//else if up arrow AND suggestions
   else if ( unicode == 13 )
   {
      //alert( 'enter button' );
      //enterTrap();
   }//else if enter button
   else
   {
      if ( searchString.length >= 3 )
      {
         if ( timeOutID != null )
         {
            //if the timeOutID already is a number and the user is still typing, clear that pending request to make room for the new one
            clearTimeout( timeOutID );
         }//if timeOutID is not null

         //set the wait to submit the AJAX query for half a second after last keypress
         timeOutID = setTimeout( 'doTimeOut();', 500 );
      }//if searchString at least three characters long
      else
      {
         document.getElementById( "results" ).innerHTML = "";
         document.getElementById( "results" ).style.visibility = "hidden";
         //document.getElementById( "results" ).style.height = "0px";
         document.getElementById( "results" ).style.border = "0px";
      }//else not enough search Text
   }//else not up or down arrow
}//letsSearch()

function doTimeOut()
{
   var searchIndex = document.getElementById( "searchPulldown" ).selectedIndex;
   var searchString = document.getElementById( "search" ).value;
   var searchType = document.getElementById( "searchPulldown" ).options[searchIndex].value;

   AJAXWrapper( "/catalog/includes/ajax/search_autocomplete.php?whatText=" + searchString + "&whatType=" + searchType, "updateSearch" );
}//doTimeOut()

function updateSearch()
{
   if ( request.readyState == 4 && request.status == 200 )
   {
      //grab output from the AJAX page
      var outputStream = request.responseText;
      //alert( "'"+outputStream+"'" );

      if ( outputStream.length > 0  )
      {
         document.getElementById( "results" ).innerHTML = outputStream;
         document.getElementById( "results" ).style.visibility = "visible";
         document.getElementById( "results" ).style.border = "1px solid #888";
      }//if any output
      else
      {
         document.getElementById( "results" ).innerHTML = '';
         document.getElementById( "results" ).style.visibility = "hidden";
         //document.getElementById( "results" ).style.height = "0px";
         document.getElementById( "results" ).style.border = "0px";
      }//else no output
   }//if request is COMPLETE and status is OK
}//updateSearch()

function searchSpecific( thisObject )
{
   var strInputCode = thisObject.innerHTML;
   strInputCode = strInputCode.replace( /&(lt|gt);/g, function (strMatch, p1)
   {
      return (p1 == "lt")? "<" : ">";
   });
   var strTagStrippedText = strInputCode.replace(/<\/?[^>]+(>|$)/g, "");

   //alert( "you clicked " + strTagStrippedText );
   document.getElementById( 'search' ).value = strTagStrippedText;

   //submit form
   submitMe();
}//searchSpecific

function enterTrap()
{
   //IE can't use getElementsByName() reliably, so lets work around it
   //var suggestions = document.getElementsByName( 'suggestions' );

   //create a temp array with all the <li> elements from inside the results <div>
   var temp = document.getElementById( 'results' ).getElementsByTagName( 'li' );
   var suggestions = new Array();
   for ( var j = 0; j < temp.length; j++ )
   {
      if ( j > 0 && j < temp.length - 1 )
      {
         //add all the "real" <li> elements into the suggestions array
         suggestions.push( temp[j] );
      }//if not on the first or last element in the temp array
   }//for each <li> tag in the results <div>

   if ( suggestions.length > 0 )
   {
      var anythingSelected = 0;

      for( var i = 0; i < suggestions.length; i++ )
      {
         if ( suggestions[i].className == 'suggestionsSelected' || suggestions[i].className == 'bottomAutoSelected' )
         {
            anythingSelected++;
   
            //if something was selected, search on it specifically
            searchSpecific( suggestions[i] );
         }//if the field is selected
      }//for every suggestion
   }//if any suggestions
   else
   {
      //if no suggestions, don't submit
      //return false;

      submitMe();
   }//else no suggestions

   if ( anythingSelected == 0 )
   {
      //if nothing selected, submit what the user typed
      submitMe();
   }//if nothing selected
}//enterTrap()

function blurResults()
{
   //alert( "Had to blur ya" );

   if ( suggestionMO == 0 )
   {
      //zero out the suggestions box
      document.getElementById( "results" ).innerHTML = '';
      document.getElementById( "results" ).style.visibility = "hidden";
      document.getElementById( "results" ).style.border = "0px";
   }//if we're not mousing over a suggestion, blur the suggestions box
}//blurResults()

function stopTheBlur( thisObject )
{
   //if this variable is 1, the suggestions div won't disappear
   suggestionMO = 1;

   //IE can't use getElementsByName() reliably, so lets work around it
   //var suggestions = document.getElementsByName( 'suggestions' );

   //create a temp array with all the <li> elements from inside the results <div>
   var temp = document.getElementById( 'results' ).getElementsByTagName( 'li' );
   var suggestions = new Array();
   for ( var j = 0; j < temp.length; j++ )
   {
      if ( j > 0 && j < temp.length - 1 )
      {
         //add all the "real" <li> elements into the suggestions array
         suggestions.push( temp[j] );
      }//if not on the first or last element in the temp array
   }//for each <li> tag in the results <div>

   //alert( suggestions.length );
   if ( suggestions.length > 0 )
   {
      for( var i = 0; i < suggestions.length; i++ )
      {
         if ( suggestions[i].id == thisObject.id )
         {
            if ( suggestions[i].className == 'bottomAuto' || suggestions[i].className == 'bottomAutoSelected' )
            {
               suggestions[i].className = 'bottomAutoSelected';
            }//if its the bottom row
            else
            {
               suggestions[i].className = 'suggestionsSelected';
            }//else one of the other suggestions
         }//if the moused selection
         else
         {
            if ( suggestions[i].className == 'bottomAuto' || suggestions[i].className == 'bottomAutoSelected' )
            {
               suggestions[i].className = 'bottomAuto'; 
            }//if its the bottom row
            else
            {
               suggestions[i].className = 'suggestions';
            }//else one of the other suggestions
         }//else not the moused selection
      }//for every suggestion
   }//if any suggestions
}//stopTheBlur()

function startTheBlur( thisObject )
 
{
   //if this variable is at 0, the suggestions div WILL disappear
suggestionMO = 0;

   //IE can't use getElementsByName() reliably, so lets work around it
   //var suggestions = document.getElementsByName( 'suggestions' );

   //create a temp array with all the <li> elements from inside the results <div>
   var temp = document.getElementById( 'results' ).getElementsByTagName( 'li' );
   var suggestions = new Array();
   for ( var j = 0; j < temp.length; j++ )
   {
      if ( j > 0 && j < temp.length - 1 )
      {
         //add all the "real" <li> elements into the suggestions array
         suggestions.push( temp[j] );
      }//if not on the first or last element in the temp array
   }//for each <li> tag in the results <div>

   if ( suggestions.length > 0 )
   {
      for( var i = 0; i < suggestions.length; i++ )
      {
         suggestions[i].className = 'suggestions';
      }//for every suggestion
   }//if any suggestions
}//startTheBlur()

</script>





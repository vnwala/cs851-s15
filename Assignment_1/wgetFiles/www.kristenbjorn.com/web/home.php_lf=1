                            	//$('#slider').css({'height':(($(window).height())-361)+'px'});
                            	$(window).resize(function(){
                                $('.SliderImage').width();
                                var cw = $('#slide2').width();
                                var hh = ((cw * 400)/1400);
                                $("[id^=slide]").each(function() {
                                  $(this).css({'height':hh+'px'});
                                });
                                $('.SliderImage').css({'height':hh+'px'});
                            	});
                              

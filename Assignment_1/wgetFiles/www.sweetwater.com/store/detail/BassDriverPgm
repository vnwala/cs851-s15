	<script type="text/javascript">
		$(function() {
			$("#threePaymentsInfoBtn").Popup({ popup: $("#threePaymentsPopup") });
		});
	</script><script type="text/javascript">
		$(document).ready(function() { 
	var openPopup = function(e)
	{
		e.preventDefault();
		var $this = $(this),
			name = "_sw_all_access",
			url = $this.attr('href'),
			width = window.outerWidth || document.body.offsetWidth,
			height = window.outerHeight || document.body.offsetHeight,
			width_prop = 0.9,
			top, left, params;
		
		if (typeof window.screenLeft !== 'undefined')
		{
			left = window.screenLeft + Math.round( width * ((1 - width_prop) / 2) );
			top = window.screenTop;
		}
		else if(typeof window.screenX !== 'undefined')
		{
			left = window.screenX + Math.round( width * ((1 - width_prop) / 2) );
			top = window.screenY;
		}
		width = Math.round(width * width_prop);
		params = 'location=yes,menubar=yes,resizable=yes,scrollbars=yes,toolbar=yes,width='+ width +',height='+ height +',top='+ top +',left='+ left;
		var newwindow = window.open(url, name, params);
		if (window.focus) { newwindow.focus() }
	};
	// Links with targets
	$('.rebate-link').on('click', openPopup);
});
		</script>

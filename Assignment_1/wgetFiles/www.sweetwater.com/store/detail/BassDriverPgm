	<script type="text/javascript">
				$(document).ready(function() {
					//For firefox which retains disable state on refresh?
					var submit_btn = $("#se-question-submit").removeAttr("disabled");

					$("#se_question_form").on("submit", function(e)
					{
						e.preventDefault();
						var $this = $(this);

						// Disable any further submissions.
						submit_btn.attr("disabled", true).attr("text", submit_btn.val()).val("Submitting...");

						// Gather up the form data.
						var data_str = $(this).serialize() + "&RPC=";

						$.ajax({
							type: "POST",
							url: "/store/detail/BassDriverPgm",
							data: data_str,
							dataType: "json",
							success: function(response) {
								console.log(response);

								// Error
								if (response.status === false && response.hasOwnProperty("error")) {
									var field;
									for( var field_name in response.error ) {
										if (!response.error.hasOwnProperty(field_name)) { continue; }
										field = $("#" + field_name + ":not([error])", $this);
										if (field.length == 0) { continue; }
										field.attr("error", true).after("<div class=\"note error\">"+ response.error[field_name] +"</div>");
									}
									$("input[error], textarea[error]", $this).eq(0)[0].focus();
								}
								// Success
								else {
									$("#sd-contact-form").addClass("success");
									var field, type;
									$("input, textarea, select", $this).each(function() {
										field = $(this);
										type = field.attr("type");
										if (field.is("input")) {
											switch (type) {
												case "radio":
												case "checkbox":
													field.removeAttr("checked");
													break;
												case "hidden":
												case "submit":
												case "button":
												case "reset":
												case "image":
													break;
												default:
													field.val("");
											}
										} else {
											field.val("");
										}
										if (field.attr("error") !== undefined) {
											field.removeAttr("error").parent().find(".note.error").remove();
										}
									});
								}

								// re-enable the button
								submit_btn.removeAttr("disabled", false).val(submit_btn.attr("text"));
							}
						});
					});
				});
			</script>

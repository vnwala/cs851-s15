/*****************online tab: start******************/
var $app_ot_btns = $('.app_ot_btn'),
	app_ot_usr;
if (userType === undefined || userType === null || userType === '') {
	app_ot_usr = 'guest';
} else if (userType === '205' || userType === '208') {
	app_ot_usr = 'reg';
} else if (userType === '206' || parseInt(userType) >= 206) {
	app_ot_usr = 'vip';
}
$app_ot_btns.on('click', function() {
	var $this = $(this);
	if (!$this.hasClass('selected')) {
		$app_ot_btns.removeClass('selected');
		$this.addClass('selected');
		categoryView($this.data('tabcat'), '', app_ot_usr);
	}
	$this = null; //just in case...
}); 
/*****************online tab: end********************/
</script>

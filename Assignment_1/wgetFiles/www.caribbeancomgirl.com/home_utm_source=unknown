window.scroll( 0, 60 );
var obj = new Object();
obj.y = document.documentElement.scrollTop || document.body.scrollTop;
window.onload = function() {
	timerID = setTimeout(function(){
		if(obj.y<60){$('html,body').animate({ scrollTop: 60 }, 'fast');}
	}, 1);
}

                        retrieveType: 'jd',
                        typeNoSelection: 'jd',
                        specialWords: /(hr)|(it)|(qa)|(c#)|(bi)/gi,
                        groupItems: false,
                        showEmptyListMessage: false,
                        showCategoryType: false,
                        clickOnSuggestRedirect: false,
                        typeTranslations: {
                            "keyword" : "Keyword",
                            "geocity": "Location",
                            "company": "Company",
                            "category": "Category",
                            "jd": "Job title",
                            "jobid": "Job ID",
                            "postalcode": "Postal code"
                        },
                        customURL: {
                            
                        }
                    };

                    var whereSettings = {
                        retrieveType: 'geocity',
                        typeNoSelection: 'geocity',
                        resultClick: function (data) {
                            jQuery("#FootercityID").val(data['attributes']['id']);
                        },
                        deleteLastSelection: function () {
                            jQuery("#FootercityID").val(0);
                        }
                    };

                    jQuery.extend(whatSettings, commonsSettings);
                    jQuery.extend(whereSettings, commonsSettings);

                    jQuery("#FooterSearchWhat").autoSuggest(url, whatSettings);
                    jQuery("#FooterSearchWhere").autoSuggest(url, whereSettings);

                });

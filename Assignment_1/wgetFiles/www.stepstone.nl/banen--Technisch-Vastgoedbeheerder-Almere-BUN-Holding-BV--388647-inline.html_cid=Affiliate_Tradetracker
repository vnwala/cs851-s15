if(key == 'events'){
for(item in sc_variables.events){
if(tmpLTE.indexOf(item) == -1){
tmpLTE.push(item);
if(sc_variables.events[item] != '') s_events.push(item+':'+sc_variables.events[item]);
else s_events.push(item);	
}	
}
}
else{
s[key] = sc_variables[key];
}	
}


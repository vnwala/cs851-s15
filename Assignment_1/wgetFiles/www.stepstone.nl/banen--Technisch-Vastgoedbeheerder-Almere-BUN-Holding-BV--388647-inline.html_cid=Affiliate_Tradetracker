			<script type="text/javascript">
			jQuery( document ).ready(function() {
				var switchTo5x=false;
				jQuery('#socialMediaSwitch').bind(
					  	'click', function() {
					  		jQuery(this).fadeOut("fast", function(){
					  			jQuery('.ajaxLoader').fadeIn("fast");
					  		});
				   			loadButtons();
					  	}
				); 
				function loadPlusone(){
					jQuery.ajax({
					  	type: "GET",
					  	url: "https://apis.google.com/js/plusone.js",
					  	dataType: "script"
					});
				};
				function loadButtons(){
					jQuery.ajax({
					  	type: "GET",
					  	url: "http://w.sharethis.com/button/buttons.js",
					  	dataType: "script"
					}).done(function(){
						stLight.options({publisher:'c24d3b01-5172-4c6f-ab9c-65fd0bff7ac9' });
						stLight.subscribe("click",fnCallback);
						jQuery('.ajaxLoader').fadeOut("fast", function(){
							jQuery( ".socialButtonArea" ).animate({
							    height: "+=115px"
							}, 400);

							loadPlusone();
						});
					});
				};
			});
			</script>
			
				<script type="text/javascript">
					jQuery('#socialMediaSwitch').unbind( "click" );
					function fnCallback (event,service)
					{
						var s=s_gi(s_account); 
						var tracking_name = '';
						switch(service)
						{
							case 'twitter':
								tracking_name = 'listing_twitter_social';
								break;

							case 'facebook':
								tracking_name = 'listing_facebook_social';
								break;

							case 'linkedin':
								tracking_name = 'listing_linkedin_social';
								break;
						}
						saveSSALog({ "OfferId": "388647", "jacid": "", "destination": service, "type": "socialmedia", "url": "/5/index.cfm?event=ssalogajax.doSocialMediaLog" }); announceInterception({ "offerId": "388647", "jacid": "", "suid": "d31d2984-0228-4f5d-aef6-7e59e12d092a", "ssaPOP": "", "ssaPOR": ""}, "/5/index.cfm?event=offerView.logSocialMediaButtonClick");
						s.tl(this,'o', tracking_name);
					}
					function fnCallbackPluseOne(plusone) {
					 	var s=s_gi(s_account); 
						var tracking_name = 'listing_plusone_social';

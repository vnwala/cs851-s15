			<script type="text/javascript">
			jQuery( document ).ready(function() {
				var switchTo5x=false;
				jQuery('#socialMediaSwitch').bind(
					  	'click', function() {
					  		jQuery(this).fadeOut("fast", function(){
					  			jQuery('.ajaxLoader').fadeIn("fast");
					  		});
				   			loadButtons();

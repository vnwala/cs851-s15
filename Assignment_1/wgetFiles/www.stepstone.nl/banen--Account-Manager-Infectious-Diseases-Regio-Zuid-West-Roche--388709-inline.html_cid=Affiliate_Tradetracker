		function trackClick(handler,prefix,showOnMap, noEvents){
			var listingInfo = '';
			if (prefix != '' && showOnMap != '') {
				listingInfo = prefix + "LIA_" + showOnMap;
			}

			if(typeof sendSC != 'undefined'){
            
                if (noEvents == true) {
                    sendSC({prop30:listingInfo, events: ''});
                } else {
                    //Override default events sent
                    sendSC({prop30:listingInfo, events: 'event28'});
                }
            
			}


		  	if(handler.length && typeof Ajax !== 'undefined') new Ajax.Request(handler + "&nocache=" + Math.random());
			return false;
		}
	</script>

	

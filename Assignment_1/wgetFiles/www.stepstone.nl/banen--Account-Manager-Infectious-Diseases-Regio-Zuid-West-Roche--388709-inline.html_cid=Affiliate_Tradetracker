								tracking_name = 'listing_linkedin_social';
								break;
						}
						saveSSALog({ "OfferId": "388709", "jacid": "", "destination": service, "type": "socialmedia", "url": "/5/index.cfm?event=ssalogajax.doSocialMediaLog" }); announceInterception({ "offerId": "388709", "jacid": "", "suid": "16c2781a-38b9-4b77-bcb0-645ed05f33ba", "ssaPOP": "", "ssaPOR": ""}, "/5/index.cfm?event=offerView.logSocialMediaButtonClick");
						s.tl(this,'o', tracking_name);
					}

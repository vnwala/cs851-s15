        jQuery(document).ready(function(){
            function fSelectionAdded (elem) {}

            var url = '/autosuggest/';
            var commonsSettings = {
                appName: 'core',
                language: 'nl',
                country: 'nl',
                selectionAdded: fSelectionAdded,
                multiselection: false,
                showBottomPanel: true,
                clearButton: false,
                clearSelectedSuggestion: false,
                labelBtnClose: "Sluiten",
                showOnlyCompanyPres: false,
                submitForm: function() { return true; }

            }

            var whatSettings = {
                cursor: true,
                retrieveType: 'jd',
                typeNoSelection: 'jd',
                specialWords: /(hr)|(it)|(qa)|(c#)|(bi)/gi,
                groupItems: false,
                showEmptyListMessage: false,
                showCategoryType: false,
                typeTranslations: {
                    "keyword" : "Keyword",
                    "geocity": "Location",
                    "company": "Company",
                    "company_cp": "Company",
                    "category": "Category",
                    "jd": "Job title",
                    "jobid": "Job ID",
                    "postalcode": "Postal code"
                },
                customURL: {
                    
                }
            };

            var whereSettings = {
                cursor: false,
                retrieveType: 'geocity',
                typeNoSelection: 'geocity',
                resultClick: function (data) {
                    jQuery("#cityID").val(data['attributes']['id']);
                },
                deleteLastSelection: function () {
                    jQuery("#cityID").val(0);
                },
                refreshField: function() {
                    jQuery("#cityID").val(0);
                }

                
            };

            jQuery.extend(whatSettings, commonsSettings);
            jQuery.extend(whereSettings, commonsSettings);

            jQuery("#SearchWhat").autoSuggest(url, whatSettings);
            jQuery("#SearchWhere").autoSuggest(url, whereSettings);

        });
    /* ]]> */
    </script>



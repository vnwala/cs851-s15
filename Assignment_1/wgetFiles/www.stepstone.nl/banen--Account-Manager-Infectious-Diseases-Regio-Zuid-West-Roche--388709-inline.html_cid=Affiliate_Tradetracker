<script type="text/javascript">
	function cookiePolicyAccept(){
		document.getElementById('cookieConsentFormMarginBottom').style.display='none';
		document.getElementById('cookieConsentForm').style.display='none';
        
        if(typeof sendSC != 'undefined'){
            var sc = {};
            sc.eVar1 = "anonymous";
            sc.eVar4 = 'cookie_fpn_accept';
            sc.events = null;
            sc.prop4 = null;
            sc.prop6 = null;
            sc.prop7 = null;
            sc.prop8 = null;
            sc.prop9 = null;
            sc.prop13 = null;
            sc.prop14 = null;
            sc.prop15 = null;
            sc.prop16 = null;
            sc.eVar24 = null;
            sc.eVar28 = null;
            sc.eVar29 = null;
            sc.eVar30 = null;
            sc.eVar32 = null;
            sc.eVar34 = null;

            sendSC(sc);
        }
        
		setCookie('cookieConsentAccepted','1',120);
	}
	function setCookie(c_name,value,exdays){
		var exdate=new Date();
		exdate.setDate(exdate.getDate() + exdays);
		var c_value=escape(value) + ((exdays==0)||(exdays=='')||(exdays==null) ? "" : "; expires="+exdate.toUTCString());
		document.cookie=c_name + "=" + c_value;
	}
</script>


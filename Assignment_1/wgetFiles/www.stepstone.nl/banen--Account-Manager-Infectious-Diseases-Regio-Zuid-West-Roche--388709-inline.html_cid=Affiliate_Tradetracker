function sendSClink(sc_variables, tl_link, tl_linkType, tl_linkName, timeInterval){
if(typeof(s_account) != 'undefined'){
var s = s_gi(s_account);

var tmpLTV = new Array();
var tmpLTE = new Array();
var s_events = new Array();

tlParams = new Object();
tlParams.link =	typeof tl_link !== 'undefined' ? tl_link : null;
tlParams.linktype = typeof tl_linkType !== 'undefined' ? tl_linkType : 'o';
tlParams.linkname = typeof tl_linkName !== 'undefined' ? tl_linkName : null;

timeInterval = timeInterval || 50;

for(key in sc_variables){					
if(tmpLTV.indexOf(key) == -1) tmpLTV.push(key);

if(key == 'events'){
for(item in sc_variables.events){
if(tmpLTE.indexOf(item) == -1){
tmpLTE.push(item);
if(sc_variables.events[item] != '') s_events.push(item+':'+sc_variables.events[item]);
else s_events.push(item);	
}	
}
}
else{
s[key] = sc_variables[key];
}	
}

s.linkTrackVars = tmpLTV.join(',');

if(tmpLTE.length > 0){
s.linkTrackEvents = tmpLTE.join(',');
s.events = s_events.join(',');
}

if(tlParams.link === true && tlParams.linkname !== null){
s.tl(tlParams.link,tlParams.linktype,tlParams.linkname);
}
else if(tlParams.link !== null){
preventAbortError = setInterval(function(){
s.tl(tlParams.link,tlParams.linktype);
clearInterval(preventAbortError);
},timeInterval);							
}
else{
return;
}
}				
}


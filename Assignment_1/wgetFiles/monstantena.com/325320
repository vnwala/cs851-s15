		<script type="text/javascript" src="js/bootstrap.min.js"></script>		<script type="text/javascript">
//<![CDATA[
	var page_newinfo = 2;
	var page_po = 2;

	function loadMoreNewinfo() {
		$("#newinfo-btn").text("読込中...");
		$.getJSON('/ajaxes/rsslist/1/10/' + page_newinfo,
			function(json) {
				$("#newinfo-btn").text("さらに表示...");
				for(var i in json.data) {
					$("#newinfo").append("<tr><td>"
						+ '<a href="http://monstantena.com/rsslists/o/'&#32;+&#32;json.data[i].Rsslist.id&#32;+&#32;'" target="_blank" class="rsslink rssid' + json.data[i].Rsslist.id + '">'
						+ json.data[i].Rsslist.title
						+ " (" + json.data[i].Site.name_short + ")"
						+ "</a>"
						+ "</td></tr>");
				}
			page_newinfo++;
		});
	}
	function loadMorePo() {
		$("#po-btn").text("読込中...");
		$.getJSON('/ajaxes/rsslist/2/10/' + page_po,
			function(json) {
				$("#po-btn").text("さらに表示...");
				for(var i in json.data) {
					$("#populer").append("<tr><td>"
						+ '<a href="http://monstantena.com/rsslists/o/'&#32;+&#32;json.data[i].Ninkilist.id&#32;+&#32;'" target="_blank" class="rsslink nrssid' + json.data[i].Ninkilist.id + '">'
						+ json.data[i].Ninkilist.title
						+ " (" + json.data[i].Ninkilist.sitenames + ")"
						+ "</a>"
						+ "</td></tr>");
				}
			page_po++;
		});
	}

	$(document).ready(function () {
		$('#newinfo-btn').on('click', function(e) {
			loadMoreNewinfo();
		});
		$('#po-btn').on('click', function(e) {
			loadMorePo();
		});
});
//]]>

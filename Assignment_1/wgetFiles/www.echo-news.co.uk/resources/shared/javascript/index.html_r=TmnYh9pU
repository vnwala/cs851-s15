

/* -- 0-console.js -- */

// Avoid `console` errors in browsers that lack a console.
;(function() {
    var method;
    var noop = function noop() {};
    var methods = [
        'assert', 'clear', 'count', 'debug', 'dir', 'dirxml', 'error',
        'exception', 'group', 'groupCollapsed', 'groupEnd', 'info', 'log',
        'markTimeline', 'profile', 'profileEnd', 'table', 'time', 'timeEnd',
        'timeStamp', 'trace', 'warn'
    ];
    var length = methods.length;
    var console = (window.console = window.console || {});

    while (length--) {
        method = methods[length];

        // Only stub undefined methods.
        if (!console[method]) {
            console[method] = noop;
        }
    }
}());


/* -- 1-jquery.cookie.js -- */

/**
 * jQuery Cookie plugin
 *
 * Copyright (c) 2010 Klaus Hartl (stilbuero.de)
 * Dual licensed under the MIT and GPL licenses:
 * http://www.opensource.org/licenses/mit-license.php
 * http://www.gnu.org/licenses/gpl.html
 *
 */
$.cookie = function (key, value, options) {
    // key and value given, set cookie...
    if (arguments.length > 1 && (value === null || typeof value !== "object")) {
        options = jQuery.extend({}, options);
        if (value === null) {
            options.expires = -1;
        }

        if (typeof options.expires === 'number') {
            var days = options.expires, t = options.expires = new Date();
            t.setDate(t.getDate() + days);
        }

        return (document.cookie = [
            encodeURIComponent(key), '=',
            options.raw ? String(value) : encodeURIComponent(String(value)),
            options.expires ? '; expires=' + options.expires.toUTCString() : '', // use expires attribute, max-age is not supported by IE
            options.path ? '; path=' + options.path : '',
            options.domain ? '; domain=' + options.domain : '',
            options.secure ? '; secure' : ''
        ].join(''));
    }

    // key and possibly options given, get cookie...
    options = value || {};
    var result, decode = options.raw ? function (s) { return s; } : decodeURIComponent;
    return (result = new RegExp('(?:^|; )' + encodeURIComponent(key) + '=([^;]*)').exec(document.cookie)) ? decode(result[1]) : null;
};


/* -- 2-JsonEncode.js -- */

/**
 *  Encode JSON
 *      $.toJSON(object/array);
 *  Parse JSON
 *      $.parseJSON(str);
 *
 */


/*
    json2.js
    2008-01-17
    Public Domain
    No warranty expressed or implied. Use at your own risk.
    See http://www.JSON.org/js.html
*/
if (!this.JSON) {

    JSON = function () {

        function f(n) {    // Format integers to have at least two digits.
            return n < 10 ? '0' + n : n;
        }

        Date.prototype.toJSON = function () {

// Eventually, this method will be based on the date.toISOString method.

            return this.getUTCFullYear()   + '-' +
                 f(this.getUTCMonth() + 1) + '-' +
                 f(this.getUTCDate())      + 'T' +
                 f(this.getUTCHours())     + ':' +
                 f(this.getUTCMinutes())   + ':' +
                 f(this.getUTCSeconds())   + 'Z';
        };


        var m = {    // table of character substitutions
            '\b': '\\b',
            '\t': '\\t',
            '\n': '\\n',
            '\f': '\\f',
            '\r': '\\r',
            '"' : '\\"',
            '\\': '\\\\'
        };

        function stringify(value, whitelist) {
            var a,          // The array holding the partial texts.
                i,          // The loop counter.
                k,          // The member key.
                l,          // Length.
                r = /["\\\x00-\x1f\x7f-\x9f]/g,
                v;          // The member value.

            switch (typeof value) {
            case 'string':

                return r.test(value) ?
                    '"' + value.replace(r, function (a) {
                        var c = m[a];
                        if (c) {
                            return c;
                        }
                        c = a.charCodeAt();
                        return '\\u00' + Math.floor(c / 16).toString(16) +
                                                   (c % 16).toString(16);
                    }) + '"' :
                    '"' + value + '"';

            case 'number':

// JSON numbers must be finite. Encode non-finite numbers as null.

                return isFinite(value) ? String(value) : 'null';

            case 'boolean':
            case 'null':
                return String(value);

            case 'object':

// Due to a specification blunder in ECMAScript,
// typeof null is 'object', so watch out for that case.

                if (!value) {
                    return 'null';
                }

// If the object has a toJSON method, call it, and stringify the result.

                if (typeof value.toJSON === 'function') {
                    return stringify(value.toJSON());
                }
                a = [];
                if (typeof value.length === 'number' &&
                        !(value.propertyIsEnumerable('length'))) {

// The object is an array. Stringify every element. Use null as a placeholder
// for non-JSON values.

                    l = value.length;
                    for (i = 0; i < l; i += 1) {
                        a.push(stringify(value[i], whitelist) || 'null');
                    }

// Join all of the elements together and wrap them in brackets.

                    return '[' + a.join(',') + ']';
                }
                if (whitelist) {

// If a whitelist (array of keys) is provided, use it to select the components
// of the object.

                    l = whitelist.length;
                    for (i = 0; i < l; i += 1) {
                        k = whitelist[i];
                        if (typeof k === 'string') {
                            v = stringify(value[k], whitelist);
                            if (v) {
                                a.push(stringify(k) + ':' + v);
                            }
                        }
                    }
                } else {

// Otherwise, iterate through all of the keys in the object.

                    for (k in value) {
                        if (typeof k === 'string') {
                            v = stringify(value[k], whitelist);
                            if (v) {
                                a.push(stringify(k) + ':' + v);
                            }
                        }
                    }
                }

// Join all of the member texts together and wrap them in braces.

                return '{' + a.join(',') + '}';
            }
        }

        return {
            stringify: stringify,
            parse: function (text, filter) {
                var j;

                function walk(k, v) {
                    var i, n;
                    if (v && typeof v === 'object') {
                        for (i in v) {
                            if (Object.prototype.hasOwnProperty.apply(v, [i])) {
                                n = walk(i, v[i]);
                                if (n !== undefined) {
                                    v[i] = n;
                                }
                            }
                        }
                    }
                    return filter(k, v);
                }

                if (/^[\],:{}\s]*$/.test(text.replace(/\\./g, '@').
replace(/"[^"\\\n\r]*"|true|false|null|-?\d+(?:\.\d*)?(?:[eE][+\-]?\d+)?/g, ']').
replace(/(?:^|:|,)(?:\s*\[)+/g, ''))) {


                    j = eval('(' + text + ')');


                    return typeof filter === 'function' ? walk('', j) : j;
                }
                throw new SyntaxError('parseJSON');
            }
        };
    }();
}

/**
 * Jquery wrapper to json
 */
(function($) {
    $.fn.toJSON = function(obj) {
        return JSON.stringify(obj);
    }
})(jQuery);




/* -- Date.format.js -- */

/**
 * http://phpjs.org/functions/date:380
 * 
 * PHP date() in javascript, available via Date::format method.
 * See http://uk3.php.net/manual/en/function.date.php for available format options.
 * 
 * Example:
 * var date = new Date();
 * var formatted = date.format("Y-m-d H-i-s");
 */
 
Date.prototype.format = function(format) {

    // This will be accessbile via date object instead of global function namespace
    var date = function(format, timestamp) {
        // http://kevin.vanzonneveld.net
        // +   original by: Carlos R. L. Rodrigues (http://www.jsfromhell.com)
        // +      parts by: Peter-Paul Koch (http://www.quirksmode.org/js/beat.html)
        // +   improved by: Kevin van Zonneveld (http://kevin.vanzonneveld.net)
        // +   improved by: MeEtc (http://yass.meetcweb.com)
        // +   improved by: Brad Touesnard
        // +   improved by: Tim Wiel
        // +   improved by: Bryan Elliott
        //
        // +   improved by: Brett Zamir (http://brett-zamir.me)
        // +   improved by: David Randall
        // +      input by: Brett Zamir (http://brett-zamir.me)
        // +   bugfixed by: Kevin van Zonneveld (http://kevin.vanzonneveld.net)
        // +   improved by: Brett Zamir (http://brett-zamir.me)
        // +   improved by: Brett Zamir (http://brett-zamir.me)
        // +   improved by: Theriault
        // +  derived from: gettimeofday
        // +      input by: majak
        // +   bugfixed by: majak
        // +   bugfixed by: Kevin van Zonneveld (http://kevin.vanzonneveld.net)
        // +      input by: Alex
        // +   bugfixed by: Brett Zamir (http://brett-zamir.me)
        // +   improved by: Theriault
        // +   improved by: Brett Zamir (http://brett-zamir.me)
        // +   improved by: Theriault
        // +   improved by: Thomas Beaucourt (http://www.webapp.fr)
        // +   improved by: JT
        // +   improved by: Theriault
        // +   improved by: RafaÅ‚ Kukawski (http://blog.kukawski.pl)
        // +   bugfixed by: omid (http://phpjs.org/functions/380:380#comment_137122)
        // +      input by: Martin
        // +      input by: Alex Wilson
        // %        note 1: Uses global: php_js to store the default timezone
        // %        note 2: Although the function potentially allows timezone info (see notes), it currently does not set
        // %        note 2: per a timezone specified by date_default_timezone_set(). Implementers might use
        // %        note 2: this.php_js.currentTimezoneOffset and this.php_js.currentTimezoneDST set by that function
        // %        note 2: in order to adjust the dates in this function (or our other date functions!) accordingly
        // *     example 1: date('H:m:s \\m \\i\\s \\m\\o\\n\\t\\h', 1062402400);
        // *     returns 1: '09:09:40 m is month'
        // *     example 2: date('F j, Y, g:i a', 1062462400);
        // *     returns 2: 'September 2, 2003, 2:26 am'
        // *     example 3: date('Y W o', 1062462400);
        // *     returns 3: '2003 36 2003'
        // *     example 4: x = date('Y m d', (new Date()).getTime()/1000); 
        // *     example 4: (x+'').length == 10 // 2009 01 09
        // *     returns 4: true
        // *     example 5: date('W', 1104534000);
        // *     returns 5: '53'
        // *     example 6: date('B t', 1104534000);
        // *     returns 6: '999 31'
        // *     example 7: date('W U', 1293750000.82); // 2010-12-31
        // *     returns 7: '52 1293750000'
        // *     example 8: date('W', 1293836400); // 2011-01-01
        // *     returns 8: '52'
        // *     example 9: date('W Y-m-d', 1293974054); // 2011-01-02
        // *     returns 9: '52 2011-01-02'
        var that = this,
            jsdate, f, formatChr = /\\?([a-z])/gi,
            formatChrCb,
            // Keep this here (works, but for code commented-out
            // below for file size reasons)
            //, tal= [],
            _pad = function (n, c) {
                if ((n = n + '').length < c) {
                    return new Array((++c) - n.length).join('0') + n;
                }
                return n;
            },
            txt_words = ["Sun", "Mon", "Tues", "Wednes", "Thurs", "Fri", "Satur", "January", "February", "March", "April", "May", "June", "July", "August", "September", "October", "November", "December"];
        formatChrCb = function (t, s) {
            return f[t] ? f[t]() : s;
        };
        f = {
            // Day
            d: function () { // Day of month w/leading 0; 01..31
                return _pad(f.j(), 2);
            },
            D: function () { // Shorthand day name; Mon...Sun
                return f.l().slice(0, 3);
            },
            j: function () { // Day of month; 1..31
                return jsdate.getDate();
            },
            l: function () { // Full day name; Monday...Sunday
                return txt_words[f.w()] + 'day';
            },
            N: function () { // ISO-8601 day of week; 1[Mon]..7[Sun]
                return f.w() || 7;
            },
            S: function () { // Ordinal suffix for day of month; st, nd, rd, th
                var j = f.j();
                return j < 4 | j > 20 && ['st', 'nd', 'rd'][j%10 - 1] || 'th'; 
            },
            w: function () { // Day of week; 0[Sun]..6[Sat]
                return jsdate.getDay();
            },
            z: function () { // Day of year; 0..365
                var a = new Date(f.Y(), f.n() - 1, f.j()),
                    b = new Date(f.Y(), 0, 1);
                return Math.round((a - b) / 864e5) + 1;
            },

            // Week
            W: function () { // ISO-8601 week number
                var a = new Date(f.Y(), f.n() - 1, f.j() - f.N() + 3),
                    b = new Date(a.getFullYear(), 0, 4);
                return _pad(1 + Math.round((a - b) / 864e5 / 7), 2);
            },

            // Month
            F: function () { // Full month name; January...December
                return txt_words[6 + f.n()];
            },
            m: function () { // Month w/leading 0; 01...12
                return _pad(f.n(), 2);
            },
            M: function () { // Shorthand month name; Jan...Dec
                return f.F().slice(0, 3);
            },
            n: function () { // Month; 1...12
                return jsdate.getMonth() + 1;
            },
            t: function () { // Days in month; 28...31
                return (new Date(f.Y(), f.n(), 0)).getDate();
            },

            // Year
            L: function () { // Is leap year?; 0 or 1
                var j = f.Y();
                return j%4==0 & j%100!=0 | j%400==0;
            },
            o: function () { // ISO-8601 year
                var n = f.n(),
                    W = f.W(),
                    Y = f.Y();
                return Y + (n === 12 && W < 9 ? -1 : n === 1 && W > 9);
            },
            Y: function () { // Full year; e.g. 1980...2010
                return jsdate.getFullYear();
            },
            y: function () { // Last two digits of year; 00...99
                return (f.Y() + "").slice(-2);
            },

            // Time
            a: function () { // am or pm
                return jsdate.getHours() > 11 ? "pm" : "am";
            },
            A: function () { // AM or PM
                return f.a().toUpperCase();
            },
            B: function () { // Swatch Internet time; 000..999
                var H = jsdate.getUTCHours() * 36e2,
                    // Hours
                    i = jsdate.getUTCMinutes() * 60,
                    // Minutes
                    s = jsdate.getUTCSeconds(); // Seconds
                return _pad(Math.floor((H + i + s + 36e2) / 86.4) % 1e3, 3);
            },
            g: function () { // 12-Hours; 1..12
                return f.G() % 12 || 12;
            },
            G: function () { // 24-Hours; 0..23
                return jsdate.getHours();
            },
            h: function () { // 12-Hours w/leading 0; 01..12
                return _pad(f.g(), 2);
            },
            H: function () { // 24-Hours w/leading 0; 00..23
                return _pad(f.G(), 2);
            },
            i: function () { // Minutes w/leading 0; 00..59
                return _pad(jsdate.getMinutes(), 2);
            },
            s: function () { // Seconds w/leading 0; 00..59
                return _pad(jsdate.getSeconds(), 2);
            },
            u: function () { // Microseconds; 000000-999000
                return _pad(jsdate.getMilliseconds() * 1000, 6);
            },

            // Timezone
            e: function () { // Timezone identifier; e.g. Atlantic/Azores, ...
                // The following works, but requires inclusion of the very large
                // timezone_abbreviations_list() function.
    /*              return this.date_default_timezone_get();
    */
                throw 'Not supported (see source code of date() for timezone on how to add support)';
            },
            I: function () { // DST observed?; 0 or 1
                // Compares Jan 1 minus Jan 1 UTC to Jul 1 minus Jul 1 UTC.
                // If they are not equal, then DST is observed.
                var a = new Date(f.Y(), 0),
                    // Jan 1
                    c = Date.UTC(f.Y(), 0),
                    // Jan 1 UTC
                    b = new Date(f.Y(), 6),
                    // Jul 1
                    d = Date.UTC(f.Y(), 6); // Jul 1 UTC
                return 0 + ((a - c) !== (b - d));
            },
            O: function () { // Difference to GMT in hour format; e.g. +0200
                var tzo = jsdate.getTimezoneOffset(),
                    a = Math.abs(tzo);
                return (tzo > 0 ? "-" : "+") + _pad(Math.floor(a / 60) * 100 + a % 60, 4);
            },
            P: function () { // Difference to GMT w/colon; e.g. +02:00
                var O = f.O();
                return (O.substr(0, 3) + ":" + O.substr(3, 2));
            },
            T: function () { // Timezone abbreviation; e.g. EST, MDT, ...
                // The following works, but requires inclusion of the very
                // large timezone_abbreviations_list() function.
    /*              var abbr = '', i = 0, os = 0, default = 0;
                if (!tal.length) {
                    tal = that.timezone_abbreviations_list();
                }
                if (that.php_js && that.php_js.default_timezone) {
                    default = that.php_js.default_timezone;
                    for (abbr in tal) {
                        for (i=0; i < tal[abbr].length; i++) {
                            if (tal[abbr][i].timezone_id === default) {
                                return abbr.toUpperCase();
                            }
                        }
                    }
                }
                for (abbr in tal) {
                    for (i = 0; i < tal[abbr].length; i++) {
                        os = -jsdate.getTimezoneOffset() * 60;
                        if (tal[abbr][i].offset === os) {
                            return abbr.toUpperCase();
                        }
                    }
                }
    */
                return 'UTC';
            },
            Z: function () { // Timezone offset in seconds (-43200...50400)
                return -jsdate.getTimezoneOffset() * 60;
            },

            // Full Date/Time
            c: function () { // ISO-8601 date.
                return 'Y-m-d\\Th:i:sP'.replace(formatChr, formatChrCb);
            },
            r: function () { // RFC 2822
                return 'D, d M Y H:i:s O'.replace(formatChr, formatChrCb);
            },
            U: function () { // Seconds since UNIX epoch
                return jsdate / 1000 | 0;
            }
        };
        this.date = function (format, timestamp) {
            that = this;
            jsdate = (timestamp == null ? new Date() : // Not provided
            (timestamp instanceof Date) ? new Date(timestamp) : // JS Date()
            new Date(timestamp * 1000) // UNIX timestamp (auto-convert to int)
            );
            return format.replace(formatChr, formatChrCb);
        };
        return this.date(format, timestamp);
    }
    
    return date(format, this.getTime() / 1000)
};


/* -- FlashDetect.js -- */

/**
 *   FlashDetect:
 *      Check if a browser currently supports flash
 *      Usage:  FlashDetect.versionAtLeast(10);
 */


/*
Copyright (c) Copyright (c) 2007, Carl S. Yestrau All rights reserved.
Code licensed under the BSD License: http://www.featureblend.com/license.txt
Version: 1.0.4
*/
var FlashDetect = new function(){
    var self = this;
    self.installed = false;
    self.raw = "";
    self.major = -1;
    self.minor = -1;
    self.revision = -1;
    self.revisionStr = "";
    var activeXDetectRules = [
        {
            "name":"ShockwaveFlash.ShockwaveFlash.7",
            "version":function(obj){
                return getActiveXVersion(obj);
            }
        },
        {
            "name":"ShockwaveFlash.ShockwaveFlash.6",
            "version":function(obj){
                var version = "6,0,21";
                try{
                    obj.AllowScriptAccess = "always";
                    version = getActiveXVersion(obj);
                }catch(err){}
                return version;
            }
        },
        {
            "name":"ShockwaveFlash.ShockwaveFlash",
            "version":function(obj){
                return getActiveXVersion(obj);
            }
        }
    ];
    /**
     * Extract the ActiveX version of the plugin.
     *
     * @param {Object} The flash ActiveX object.
     * @type String
     */
    var getActiveXVersion = function(activeXObj){
        var version = -1;
        try{
            version = activeXObj.GetVariable("$version");
        }catch(err){}
        return version;
    };
    /**
     * Try and retrieve an ActiveX object having a specified name.
     *
     * @param {String} name The ActiveX object name lookup.
     * @return One of ActiveX object or a simple object having an attribute of activeXError with a value of true.
     * @type Object
     */
    var getActiveXObject = function(name){
        var obj = -1;
        try{
            obj = new ActiveXObject(name);
        }catch(err){
            obj = {activeXError:true};
        }
        return obj;
    };
    /**
     * Parse an ActiveX $version string into an object.
     *
     * @param {String} str The ActiveX Object GetVariable($version) return value.
     * @return An object having raw, major, minor, revision and revisionStr attributes.
     * @type Object
     */
    var parseActiveXVersion = function(str){
        var versionArray = str.split(",");//replace with regex
        return {
            "raw":str,
            "major":parseInt(versionArray[0].split(" ")[1], 10),
            "minor":parseInt(versionArray[1], 10),
            "revision":parseInt(versionArray[2], 10),
            "revisionStr":versionArray[2]
        };
    };
    /**
     * Parse a standard enabledPlugin.description into an object.
     *
     * @param {String} str The enabledPlugin.description value.
     * @return An object having raw, major, minor, revision and revisionStr attributes.
     * @type Object
     */
    var parseStandardVersion = function(str){
        var descParts = str.split(/ +/);
        var majorMinor = descParts[2].split(/\./);
        var revisionStr = descParts[3];
        return {
            "raw":str,
            "major":parseInt(majorMinor[0], 10),
            "minor":parseInt(majorMinor[1], 10),
            "revisionStr":revisionStr,
            "revision":parseRevisionStrToInt(revisionStr)
        };
    };
    /**
     * Parse the plugin revision string into an integer.
     *
     * @param {String} The revision in string format.
     * @type Number
     */
    var parseRevisionStrToInt = function(str){
        return parseInt(str.replace(/[a-zA-Z]/g, ""), 10) || self.revision;
    };
    /**
     * Is the major version greater than or equal to a specified version.
     *
     * @param {Number} version The minimum required major version.
     * @type Boolean
     */
    self.majorAtLeast = function(version){
        return self.major >= version;
    };
    /**
     * Is the minor version greater than or equal to a specified version.
     *
     * @param {Number} version The minimum required minor version.
     * @type Boolean
     */
    self.minorAtLeast = function(version){
        return self.minor >= version;
    };
    /**
     * Is the revision version greater than or equal to a specified version.
     *
     * @param {Number} version The minimum required revision version.
     * @type Boolean
     */
    self.revisionAtLeast = function(version){
        return self.revision >= version;
    };
    /**
     * Is the version greater than or equal to a specified major, minor and revision.
     *
     * @param {Number} major The minimum required major version.
     * @param {Number} (Optional) minor The minimum required minor version.
     * @param {Number} (Optional) revision The minimum required revision version.
     * @type Boolean
     */
    self.versionAtLeast = function(major){
        var properties = [self.major, self.minor, self.revision];
        var len = Math.min(properties.length, arguments.length);
        for(i=0; i<len; i++){
            if(properties[i]>=arguments[i]){
                if(i+1<len && properties[i]==arguments[i]){
                    continue;
                }else{
                    return true;
                }
            }else{
                return false;
            }
        }
    };
    /**
     * Constructor, sets raw, major, minor, revisionStr, revision and installed public properties.
     */
    self.FlashDetect = function(){
        if(navigator.plugins && navigator.plugins.length>0){
            var type = 'application/x-shockwave-flash';
            var mimeTypes = navigator.mimeTypes;
            if(mimeTypes && mimeTypes[type] && mimeTypes[type].enabledPlugin && mimeTypes[type].enabledPlugin.description){
                var version = mimeTypes[type].enabledPlugin.description;
                var versionObj = parseStandardVersion(version);
                self.raw = versionObj.raw;
                self.major = versionObj.major;
                self.minor = versionObj.minor;
                self.revisionStr = versionObj.revisionStr;
                self.revision = versionObj.revision;
                self.installed = true;
            }
        }else if(navigator.appVersion.indexOf("Mac")==-1 && window.execScript){
            var version = -1;
            for(var i=0; i<activeXDetectRules.length && version==-1; i++){
                var obj = getActiveXObject(activeXDetectRules[i].name);
                if(!obj.activeXError){
                    self.installed = true;
                    version = activeXDetectRules[i].version(obj);
                    if(version!=-1){
                        var versionObj = parseActiveXVersion(version);
                        self.raw = versionObj.raw;
                        self.major = versionObj.major;
                        self.minor = versionObj.minor;
                        self.revision = versionObj.revision;
                        self.revisionStr = versionObj.revisionStr;
                    }
                }
            }
        }
    }();
};
FlashDetect.JS_RELEASE = "1.0.4";


/* -- Martini-AdminUser-Share-Stats.js -- */

/* Display current share stats */

;(function() {
    "use strict";

    var MartiniShareStats = {
        init: function() {
            // we don't have anything
            if ($('.shareStats').length === 0) {
                return;
            }
            // grab the stats
            $.get('/ws/public/share/stats/', MartiniShareStats.display);
        },
        display: function(data) {
            var twitter = data.data['t.co'] || [];
            var facebook = data.data['facebook.com'] || [];
            $('.shareStats').each(function (el) {

                var articleId = $(this).attr('data-article-id');

                var html = [];
                // deal with most read first
                if (typeof data.reads[articleId] !== 'undefined') {
                    html.push('<span class="shareStatsRead label" title="reads today"><i class="fa fa-eye"></i> '+data.reads[articleId]+'</span>');
                }

                // twitter and facebook
                var twitterCount = 0;
                var facebookCount = 0;

                if (typeof twitter[articleId] !== 'undefined') {
                    twitterCount = twitter[articleId] ;
                }

                if (typeof facebook[articleId] !== 'undefined') {
                    facebookCount = facebook[articleId] ;
                }
                if (facebookCount > 0 && facebookCount < 500) {
                    html.push('<span class="shareStatsFb label" title="facebook referrals today"><i class="fa fa-facebook"></i> '+facebookCount+'</span>');
                } else if (facebookCount >= 500) {
                    html.push('<span class="shareStatsFb label" title="facebook referrals today"><i class="fa fa-facebook"></i> '+facebookCount+' <i class="fa fa-level-up" title="over 500 referrals"></i></span>');
                }

                if (html.length > 0) {
                    $(this).html(html.join(' ')).show();
                }
            });
        }
    };
    window.MartiniShareStats = MartiniShareStats;
})();
$(document).ready(function() {
    MartiniShareStats.init();
});


/* -- Martini-Article-Search.js -- */

/* jshint strict: true, browser: true, devel: true, jquery: true, indent: 4, curly: true */

if (typeof isMobileDevice === 'undefined') {
    isMobileDevice = false;
}
;(function (isMobileDevice) {
    'use strict';

    /**
     * Improved image search.
     * Can be attached to any element with a class of: call-article-search
     * Required attributes: data-user-id (The current user id)
     *                      data-target: (The full name of the id you want to populate)
     * Optional attributes: data-width (Set the default width)
     *                      data-width-type (Set the type (ie, > , >= etc))
     *                      data-height (set the default height)
     *                      data-height-type (Set the type (ie, > , >= etc))
     * @type {Object}
     */
    var ArticleSearch = {
        id: 0,
        limit:50,
        resultTarget : '',
        /**
         * Entrance function, sets up all basic variables and launches the template
         * @param  {DomElement} el The calling element
         * @return {void}
         */
        display: function(el) {

            // restore the basic options in case this is a subsequent item
            this.clearResults();

            // clear any history from the form
            $('#article-search input').val('').attr('disabled', null);

            // get the width and height parameters

            this.resultTarget = $(el).attr('data-target');
            this.currentUserId = $(el).attr('data-user-id');

            // grab the template from the web service
            this.loadTemplate();
        },
        /**
         * Check if we have the template loaded, if not do a get request to fetch it
         * @return {void}
         */
        loadTemplate: function() {
            // is the template pre-laoded into the dom?
            if ($('#article-search').length === 1) {
                return this.launch();
            }

            // grab it
            $.get('/ws/public/template/ArticleSearch/', {}, ArticleSearch.load);

        },
        /**
         * Result of the ajax request, appending the content to the Dom and setting it as a dialog box
         * @context AjaxRequest
         * @param  {mixed} data The result of the ajax request
         * @return {void}
         */
        load: function(data) {

            // append it
            var html = data.content;
            $('body').append(html);

            var dialogWidth = 640;
            var dialogHeight = 600;
            if (isMobileDevice) {
                dialogWidth = 300;
                dialogHeight = 400;
            }
            //make it a dialogue box
            $("#article-search").dialog({
                bgiframe: true,
                autoOpen: false,
                height: dialogHeight,
                width: dialogWidth,
                title: 'Article search',
                modal: true
            });

            // launch it
            ArticleSearch.launch();

        },
        /**
         * Open the dialog, after ensuring all the buttons work and the content is correct
         * @return {void}
         */
        launch: function() {

            // bind the search and clear buttons
            this.bind();

            // set all the defualt settings
            this.setDefaults();

            // open it
            $('#article-search').dialog('open');


        },
        /**
         * Perform the search bindings
         * @return {void}
         */
        bind: function() {
            $('#article-search-submit').unbind('click').click(function (e) {
                e.preventDefault();
                ArticleSearch.search();
            });
            $('#article-search-clear').unbind('click').click(function (e) {
                e.preventDefault();
                ArticleSearch.clearResults();
                ArticleSearch.setDefaults();

            });
        },
        /**
         * Bind all the result buttons and images
         * @return {void}
         */
        bindResults: function() {
            // handle a direct select
            $('.artEditArticleSelect').unbind('click').click(function (e) {
                e.preventDefault();
                ArticleSearch.handleSelect(this);
            });
            // close the search details
            $('.artEditArticleClose').unbind('click').click(function (e) {
                e.preventDefault();
                $('#article-search-details').hide();
            });
            // set the page to update the site as required
            $('#article-search-pager').unbind('change').change(function (e) {
                var val = $(this).val();
                $('#article-search-current-page').val(val);
                ArticleSearch.search();
            });
            // handle the previous buttons
            $('#article-search-prev').unbind('click').click(function (e) {
                e.preventDefault();

                // get the current page
                var val = parseInt($('#article-search-current-page').val());

                // we don't do anyting if we are already on zero
                if (val === 0) {
                    return;
                }
                // decreate the value
                val--;

                // dop a new search
                $('#article-search-current-page').val(val);
                ArticleSearch.search();
            });
            // handle the next button
            $('#article-search-next').unbind('click').click(function (e) {
                e.preventDefault();
                // get the current
                var val = parseInt($('#article-search-current-page').val());
                // increment
                val++;
                $('#article-search-current-page').val(val);
                ArticleSearch.search();
            });

        },
        /**
         * Get the current id of the image/item selected and place it in the defined target
         * @param  {DomElement} e
         * @return {void}
         */
        handleSelect: function (e) {
            var id = $(e).attr('data-id');
            $(this.resultTarget).val(id);
            // call a change on the target, to ensure it gets captured
            $(this.resultTarget).change();
            $('#article-search').dialog('close');
        },

        /**
         * Clear all the results dom elements for a new search or a new pager
         * @return {void}
         */
        clearResults: function () {
            $('#article-search input, #article-select select').val('').attr('disabled', null);
            $('#article-search-results').empty();
            $('#article-search-form').show();
            $('#article-search-newsearch').hide();
            $('#article-search-current-page').val('0');
        },
        /**
         * Reset all the defaults
         */
        setDefaults: function () {

            $('#article-search-keyword').val('');

            // always zero to start with
            $('#article-search-current-page').val('0');

            $('#article-search-order').val('');


        },
        /**
         * Perform the search with an ajaz request
         * @return {void}
         */
        search: function() {
            // show a progress spinner
            this.showProgressBar();

            // build the data we require
            var options = {};

            options.keyword = $('#article-search-keyword').val();
            options.order_by = $('#article-search-order').val();
            options.page = $('#article-search-current-page').val();
            options.limit = this.limit;

            // request some results
            $.post('/ws/public/articlesearch/', options, ArticleSearch.displayResults);
        },
        displayResults : function(data) {
            // get the results
            var results = data.data;
            // and the total
            var total = data.total;
            // remove the progress spinner
            $('#article-search-progress-bar').remove();

            //clear any previous results
            $('#article-search-results').empty();

            if (total === 0) {
                $('#article-search-pager-totals').html('No results found');
                $('#article-search-results').html('No results found');
                return;
            }

            // create the li for each search result
            $.each(results, function(i, d) {
                var li = ArticleSearch.createLi(d);
                $('#article-search-results').append(li);
            });

            // generate the pager
            ArticleSearch.generatePager(total);

            // and bind the new buttons
            ArticleSearch.bindResults();
        },
        /**
         * Generate the pager and total description from the results
         * @param  {int} total
         * @return {void}
         */
        generatePager: function(total) {

            // get the current value
            var currentPage = parseInt($('#article-search-current-page').val());
            // work out what we are doing and showing
            var showing = (this.limit * currentPage) + 1;
            var to = showing + this.limit;

            // set the html
            $('#article-search-pager-totals').html("Showing "+showing+" to "+to+" of "+total);

            // clear old results
            $('#article-search-pager').empty();
            // see how many pages we need
            var pages = Math.floor(total/this.limit);
            for (var i = 0; i <= pages; i++) {
                // build the item
                var option = $('<option></option>').val(i).text((i+1));
                if (i === currentPage) {
                    option.attr('selected', 'selected');
                }

                // append it
                $('#article-search-pager').append(option);
            }

        },
        /**
         * Show the progress bar and hide other elements
         * @return {void}
         */
        showProgressBar: function() {
            // hide the form
            $('#article-search-form').hide();
            // show the new search bar
            $('#article-search-newsearch').show();

            // set the spinner
            var img = $('<img>').attr('src', '/resources/static/ajax-loader.gif').attr('style', 'margin: auto').attr('id', 'article-search-progress-bar');
            $('#article-search-results').append(img);
        },
        /**
         * Create an li element for the dom results, with the data-attribute spammed with all the details we require
         * @param  {object} data
         * @return {DomElement}
         */
        createLi: function(data) {
            // holder
            var li = $('<li></li>');

            var img = '';
            if (data.image_id > 0)
            {
                var d = $('<div></div>').addClass('story-figure');
                var imgLink = $('<a></a>').addClass('artEditArticleSelect').attr('data-id', data.id).html(data.firstpar);
                var imgSrc = $('<img />').attr('src', '/resources/images/'+data.image_id+'?type=mc'+'&htype='+data.offset).attr('alt', data.headline);

                imgLink.append(imgSrc);

                img = d.append(imgLink);
            }


            var paraLink = $('<a></a>').addClass('artEditArticleSelect').attr('data-id', data.id).html(data.firstpar);
            var para = $('<p></p>');
            para.append(paraLink);
            
            var datePara = $('<p></p>').html("Date posted: "+data.date);

            // standard link with attributes
            var headline = $('<h2></h2>').addClass('headline');
            var link = $('<a></a>').addClass('artEditArticleSelect').attr('data-id', data.id).html(data.headline);

            headline.append(link);


            li.append(img, headline, datePara, para);

            return li;
        },
        init: function() {
            $('.call-article-search').unbind('click').click(function (e) {
                e.preventDefault();

                ArticleSearch.display(this);
            });
        }
    };
    window.ArticleSearch = ArticleSearch;
    // init function call
    $(document).ready(function () {
        ArticleSearch.init();
    });

})(isMobileDevice);


/* -- Martini-Brightcove.js -- */

/**
*   Generate the brightcove players dynamically.
*   This is now reliant on GoogleDfp for the advertising
*
*/

// Singleton holder
var MartiniBrightcove = {};

// Hold the players and google account id/path
MartiniBrightcove.players = [];
MartiniBrightcove.googleID = '';

/**
 * Set the google id so that it matches the GoogleDfp
 * @param String id
 */
MartiniBrightcove.setGoogleId = function (id) {
    this.googleID = id;
    // as we now how the google id, we can expan the adds
    this.gatherDetails();

    this.expand();
};


MartiniBrightcove.gatherDetails = function() {

    // no videos available
    if (typeof bcHolder === 'undefined' || bcHolder.length === 0) {
        return;
    }
    var duplicates = {};
    $.each(bcHolder, function (i, params) {

        if (duplicates[params.id] === 1) {
            return;
        }
        duplicates[params.id] = 1;

        MartiniBrightcove.players.push(params);
    })

};


/**
 * Loop through the items we#ve saved and render them to the page
 */
MartiniBrightcove.expand = function() {
    $.each(this.players, function (i, obj) {
        MartiniBrightcove.render(obj.id, obj.params);
    });

    // now that the Dom is in place, we have over everything to brightcove
    brightcove.createExperiences();
};

/**
 * Render the DOM needed for brightcove to create a player
 * @param  {string} id
 * @param  {object} params
 * @return {void}
 */
MartiniBrightcove.render = function(id, params){
    // extract the ad server url
    var adServerUrl = params['adServerURL'];

    // repalce the path and referrer with the actual items that we are using
    adServerUrl = adServerUrl.replace('[AdUnit_path]', this.googleID);
    adServerUrl = adServerUrl.replace('[referrer_url]', document.location.href);

    // re-set the url
    params['adServerURL'] = adServerUrl;

    var wx = this.getSize(params['ratio'], id);
    params['width'] = wx.width;
    params['height'] = wx.height;

    // create an object
    var domObj = $('<object></object>').addClass('BrightcoveExperience').attr('id', 'myExperience'+id);
    // assign paramaters
    $.each(params, function (key, val) {
        var p = $('<param />').attr('name', key).attr('value', val);
        domObj.append(p);

    });

    // insert it into the dom
    $('#bc'+id).after(domObj);
};

/**
 * Get the overridden dom based width and height
 * @param  {array} ratio
 * @param  {string} id
 * @return {object}
 */
MartiniBrightcove.getSize = function(ratio, id) {
    // get the parent div width
    var w = $('#bc'+id).width();

    // calculate the new height based on the ration
    var h = (w / ratio[0]) * ratio[1];

    h = Math.round(h);

    return {width: w, height: h};
};

// if this is called early, we can set it to the correct parameter
var bcHolder = bcHolder || [];


/* -- Martini-BrowserHash.js -- */


/* user identification data */

var BrowserOptions = {

    options : {},

    getOptions : function() {
        try {
            this.options['plugins'] = this.getPlugins();
        } catch(ex) {
            this.options['plugins'] = "permission denied";
        }

        try {
            this.options['timezone'] = new Date().getTimezoneOffset();
        } catch(ex) {
            this.options['timezone'] = "permission denied";
        }

        try{
            this.options['video'] = screen.width+"x"+screen.height+"x"+screen.colorDepth;
        }catch(ex) {
            this.options['video'] = "permission denied";
        }

        try{
            this.options['local_storage'] = this.testDomStorage();
        } catch(ex) {
            this.options['local_storage'] = "permission denied";
        }
        return this.options;
    },

    /* code borrowed from http://panopticlick.eff.org/index.php */
    getPlugins: function() {
        // fetch and serialize plugins
        var plugins = "";
        // in Mozilla and in fact most non-IE browsers, this is easy
        if (navigator.plugins) {
            var np = navigator.plugins;
            var plist = new Array();
            // sorting navigator.plugins is a right royal pain
            // but it seems to be necessary because their order
            // is non-constant in some browsers
            for (var i = 0; i < np.length; i++) {
                plist[i] = np[i].name + "; ";
                plist[i] += np[i].description + "; ";
                plist[i] += np[i].filename + ";";
                for (var n = 0; n < np[i].length; n++)
                {
                    plist[i] += " (" + np[i][n].description +"; "+ np[i][n].type +
                        "; "+ np[i][n].suffixes + ")";
                }
                plist[i] += ". ";
            }
            plist.sort();
            for (i = 0; i < np.length; i++)
                plugins+= "Plugin "+i+": " + plist[i];
        }
        // in IE, things are much harder; we use PluginDetect to get less
        // information (only the plugins listed below & their version numbers)
        if (plugins == "") {
            var pp = [];
            pp[0] = "Java";
            pp[1] = "QuickTime";
            pp[2] = "DevalVR";
            pp[3] = "Shockwave";
            pp[4] = "Flash";
            pp[5] = "WindowsMediaplayer";
            pp[6] = "Silverlight";
            pp[7] = "VLC";
            var version;
            for ( p in pp ) {
                version = PluginDetect.getVersion(pp[p]);
                if (version) {
                    plugins += pp[p] + " " + version + "; "
                }
            }
            plugins += ieAcrobatVersion();
        }
        return plugins;
    },

    ieAcrobatVersion : function() {
        // estimate the version of Acrobat on IE using horrible horrible hacks
        if (window.ActiveXObject) {
            for (var x = 2; x < 10; x++) {
                try {
                    oAcro=eval("new ActiveXObject('PDF.PdfCtrl."+x+"');");
                    if (oAcro) {
                        return "Adobe Acrobat version" + x + ".?";
                    }
                }
                catch(ex) {
                    // do nothibng
                }
            }
            try {        // find version 4
                oAcro4 = new ActiveXObject('PDF.PdfCtrl.1');
                if (oAcro4) {
                    return "Adobe Acrobat version 4.?";
                }
            } catch(ex) {

            }

            // find version 6
            try {
                oAcro7=new ActiveXObject('AcroPDF.PDF.1');
                if (oAcro7) {
                    return "Adobe Acrobat version 7.?";
                }
            } catch (ex) {
                // do nothibng
            }

            return "";
        }
    },

    setDomStorage : function() {
        try {
            localStorage.sTest = "yes";
            sessionStorage.sTest = "yes";
        } catch (ex) {
            // do nothing
        }
    },

    deleteDomStorage: function() {
        try {
            localStorage.clear();
            sessionStorage.clear()
        } catch (ex) {
            // do nothing
        }
    },

    testDomStorage :function() {
        this.setDomStorage();
        var supported = "";
        try {
            if (localStorage.sTest == "yes") {
                supported += "DOM localStorage: Yes";
            } else {
                supported += "DOM localStorage: No";
            }
        } catch (ex) {
            supported += "DOM localStorage: No";
        }

        try {
            if (sessionStorage.sTest == "yes") {
                supported += ", DOM sessionStorage: Yes";
            } else {
                supported += ", DOM sessionStorage: No";
            }
        } catch (ex) {
            supported += ", DOM sessionStorage: No";
        }
        this.deleteDomStorage();
        return supported;
    }
};

/* -- Martini-Call-Function.js -- */

/* jshint strict: true, browser: true, devel: true, jquery: true, indent: 4, curly: true */
/**
 * A simple way of turning a srint foo.bar to something executable
 *
 * So if you have a string of martini.init, you would call this function with
 *     call.fromString('martini.init', window, "123");
 * which would be the equivalent of
 *     martini.init("123");
 *
 * If you already have a context defined, you can use
 *     call.fromString('init', martini, "123", 234...)
 * Which would work as well.
 * You can pass as many secondary arguments as required
 */
;(function () {
    "use strict";
    var call = {
        /**
         * Take a string and execute it the proper way (not using some eval hack)
         * @param  {string} methodStr
         * @param  {object} context   The intial context of the scope we are calling, would normally be window
         * @return {mixeed}
         */
        fromString: function(methodStr, context /*, args */) {

            // grab all and any supplemental argumens and get rid of the first two (name and context)
            var args = Array.prototype.slice.call(arguments, 2);

            // search for any namespaces in the name
            var namespaces = methodStr.split(".");

            // the last item is the actual function we want to call
            var method = namespaces.pop();

            // update the context with each of the namespaces
            for (var i = 0; i < namespaces.length; i++) {
                context = context[namespaces[i]];
            }

            // call the actual function with the argurms set
            return context[method].apply(context, args);
        }
    };
    // export to the global namespace
    window.call = call;
})();

/* -- Martini-Click2FindPanel.js -- */


/* jshint strict: true, browser: true, devel: true, jquery: true, indent: 4, curly: true */
;(function() {

    "use strict";

    var c2ftradedir = {
        init: function () {
            if ($('#c2fbandbox').length === 0) {
                return;
            }

            var limit = parseInt($('#c2fbandbox').attr('data-limit'));

            if (isNaN(limit)) {
                limit = 5;
            } 

            $.get("/trade_directory/supers/"+limit, {}, c2ftradedir.load);
        },

        load: function(data) {
            var el = $('#c2fbandbox');
            el.toggle(); // show it
            $('#c2fbandbox').remove(); // clear it from the original data

            $.each(data, function(g, f) {
                var item = el.clone();

                item.attr('id', null);

                // add the alpha class for the first item
                if (g === 0) {
                    item.addClass('alpha');
                }

                // build a url
                var category = '';
                try {
                    category = f.categories.shift();
                }
                catch(err) {
                    return;
                }

                var url = '/trade_directory/advert/'+category.tokenisedname+'/'+f.id+'/'+encodeURI(f.serviceName)+'?ref=ft';

                // set it
                item.find('h5 a').attr('href', url).html(f.serviceName);

                // set the companyu name

                // set the category
                var catLinkUrl = '/trade_directory/search/category/'+category.tokenisedname+'?ref=ft';
                item.find('.bizColMore a').attr('href', catLinkUrl).html('More '+category.displayname);


                // create the logo
                if (f.logoId !== null)
                {
                    var logoUrl = '/resources/images/'+f.logoId+'/';
                    item.find('.bizColLogo a').attr('href', url);

                    item.find('img').attr('src', logoUrl).attr('alt', f.serviceName);
                }
                else
                {
                    item.find('.bizColLogo').remove();
                }

                $('.bizdir').before(item);
            });
        }
    };
    $(document).ready(function() {
        c2ftradedir.init();
    });

})();


/* -- Martini-CommentHandler.js -- */

/**
 * Handles all the comments
 *
 */

commentHandler = {

    init: function ()
    {
        $('.suspend').click( function (e) {
            e.preventDefault();
            commentHandler.suspend(this);
        });

        $('.cquote').click( function (e) {
            e.preventDefault();
            commentHandler.quote(this);
        });

        $('#emailme').click (function (e) {
            commentHandler.emailMe();
        });
    },
    suspend: function (e)
    {
        // get the comment id
        id = $(e).attr('id').replace(/s_/,'');

        $.ajax({url: '/admin/comments/?action=suspend&id='+id,
                success: commentHandler.success,
                error: commentHandler.failure,
                action: 'get'});
    },
    /**
     * The succesful return of an ajax delete
     */
    success: function (t)
    {
        $('#comment_'+t.commentId).remove();
    },
    failure: function (a, b, c, d)
    {
        console.log(a, b, c, d);
    },
    removeQuote: function()
    {
        $('#c_quote').html('');
        $('#c_quotepreview').html('');
    },
    quote : function(e)
    {
        // get the comment id
        id = $(e).attr('id').replace(/q_/,'');
        comment = $('#comment_'+id+'_full').html();
        name = $('#comment_'+id+'_name').html();

        // create the quote content
        content = $('<p></p>').html("<b>"+name+"</b> wrote:<br />"+comment+"</p>");

        el = $('<p></p>').html($('<blockquote></blockquote>').html(content));
        el.append($('<a></a>').attr('id', 'rq').html('Remove quote'));
        container = $('<div></div>').html(el.html());

        $('#c_quotepreview').html(container.html());

        // attach the remove quote
        $('#rq').click( function (e) {e.preventDefault();commentHandler.removeQuote();});

        // add the bbcode style
        comment = comment.replace(/"/, '&quot;');
        $('#c_quote').val("[quote][p][bold]"+name+"[/bold] wrote:\n"+comment+"[/p][/quote]");

        $('#c_comment').focus();

    },
    emailMe : function()
    {
        // check if this thing is now checked
        var p = $('#emailme').is(':checked') ? 'true' : 'false';

        $('#emailme').parent().addClass('loading');

        $.get
        (
            document.location.pathname,
            {
                action: 'alert_subscription',
                alert_subscription : p
            },
            function(response)
            {
                $('#emailme').parent().removeClass('loading');
                if (p === 'true') {
                    $('#emm').html('You have successfully subscribed to this article');
                }
                else {
                    $('#emm').html('You have successfully unsubscribed to this article');
                }
            },
            'json'
        );
    },
    changeLike : function(type, ele)
    {
        articleId = $('#commentsList').attr('article_id');
        commentId = $(ele).parents('div[id*="comment_"]').attr('id').replace(/comment_/, '');
        $.ajax(
        {
            type: "POST",
            url: "/ws/article/"+articleId+"/comments/"+commentId+"/",
            data: {'action': type},
            success: function () { commentHandler.scoreCookie($(ele), type); }
        });
        return false;
    },
    scoreCookie: function(el, val)
    {
        var postID = el.parents('div[id*="comment_"]').attr('id');
        var cookieVal = $.cookie('comment' + val);
        if(cookieVal != '')
        {
            cookieVal = cookieVal + ',' + postID;
        }
        else
        {
            cookieVal = postID;
        }
        $.cookie('comment' + val, cookieVal, { expires: 7 });
        var scoreVal = parseInt(el.siblings('[class*="commentScore"]').children('span').text());

        if (val == 'Plus')
        {
            scoreVal = scoreVal + 1;
            msg = "+1";
        }
        else
        {
            scoreVal = scoreVal - 1;
            msg = "-1";
        };
        if (scoreVal >= 0)
        {
            el.siblings('[class*="commentScore"]').removeClass().addClass('commentScorePlus');
        }
        else
        {
            el.siblings('[class*="commentScore"]').removeClass().addClass('commentScoreMinus');
        }
        el.siblings('[class*="commentScore"]').animate({opacity: 0}, 300 ).animate({opacity: 1}, 300 ).children('span').text(scoreVal);
        el.siblings('[class*="commentThumbs"]').remove();
        el.replaceWith('<li class="commentScored">You voted <span class="commentScored' + val + '">'+msg+'</span></li>');
    },
    checkScoreCookie: function(type, symbol)
    {
        var cookieVal = $.cookie('comment'+type);
        var cookieArr = cookieVal.split(',');
        if(cookieVal != '')
        {
            $.each(cookieArr, function(i)
            {
                $('#' + cookieArr[i] + ' .commentLike li:not(:first-child)').remove();
                $('#' + cookieArr[i] + ' .commentLike').append('<li class="commentScored">You voted <span class="commentScored'+type+'">'+symbol+'1</span></li>');
            });
        }
    }
}


/* -- Martini-Competitions.js -- */


competitionHandler = {

    closeModal: function()
    {
        $('#mask, .modalWrapper').fadeOut();
    },

    init: function()
    {
        if ($('#entryList .buttonStyle').length > 0) {
            
            $('.revealPhone, .revealText, .revealOnline').not('.disabled').click(function(event) {
                event.preventDefault();

                var mask = $('#mask');
                
                if(!mask.length)
                {
                    mask = $('<div id="mask"></div>').appendTo('body');
                }
                
                mask.unbind('click').click(competitionHandler.closeModal);
                
                var vpwidth = $(window).width();
                var vpheight = $(window).height();
                
                mask.css({
                    'display': 'none',
                    'background-color': '#000',
                    'position': 'fixed',
                    'top': '0',
                    'left': '0',
                    'z-index': '9998',
                    'height': vpheight,
                    'width': vpwidth,
                    opacity: 0.4
                });

                mask.fadeIn('slow');

                var modalWidth = $(this).next('div').children('.modal').width();
                var modalOffset = (vpwidth - modalWidth)/2;
                
                // For some reason ie8 wants display to be set to block to show us this fixed
                // position container
                $(this).next('div').css( {'display' : 'block', 'left' : modalOffset} ).fadeIn('slow', function() {  });
                
                // Omniture tracking
                try {
                    if($(this).hasClass('revealPhone'))
                    {
                        s.linkTrackVars = 'eVar2,eVar3,eVar6,eVar16,eVar17,eVar18,eVar19,eVar42,events';
                        s.linkTrackEvents = 'event40';
                        s.events='event40';
                        s.tl(this, 'o', 'Competitions:Advert:Phone');
                    }
                    else if($(this).hasClass('revealText'))
                    {
                        s.linkTrackVars = 'eVar2,eVar3,eVar6,eVar16,eVar17,eVar18,eVar19,eVar42,events';
                        s.linkTrackEvents = 'event41';
                        s.events = 'event41';
                        s.tl(this, 'o', 'Competitions:Advert:Text');
                    }
                    else if($(this).hasClass('revealOnline'))
                    {
                        s.linkTrackVars = 'eVar2,eVar3,eVar6,eVar16,eVar17,eVar18,eVar19,eVar42,events';
                        s.linkTrackEvents = 'event39';
                        s.events = 'event39';
                        s.tl(this, 'o', 'Competitions:Advert:Online');
                    }
                }
                catch(e) {}
            });
            
            $('.disabled').click(function(event) {
                event.preventDefault();
            });
            
            $('.modal h5 span').click(competitionHandler.closeModal);
            
            $(document).keyup(function(e) {
                if (e.keyCode == 27) { competitionHandler.closeModal(); }   // esc
            });
        }
    }
}


/* -- Martini-Custom-Sharing.js -- */

;(function($) {

    "use strict";

    var sharing = {
        init: function() {
            $('.custom-sharing').click(function (e) {
                e.preventDefault();

                sharing.create(this);
            });
        },
        /**
         * Create the url needed to share this page
         * @param  {Element} el
         * @return {void}
         */
        create : function(el) {
            // grab the share url
            var url = $(el).attr('data-share-url');

            // and the path we are sharing
            url += this.encode($(el).attr('data-share-path'));

            // face book wants a type and a title
            if (url.indexOf('facebook') !== -1) {
                var title = document.title;
                url += "&p[title]="+this.encode(title);
                url += "&display=popup";

                // record the event in omniture
                this.record('facebook', 'event33', 'Facebook Share');
            }
            // record for twitter
            else if (url.indexOf('twitter') !== -1) {
                var headline = $(el).attr('data-share-headline');
                url += "&text="+this.encode(headline);
                this.record('twitter', 'event32', 'Tweet');
            }
            // record for google
            else if (url.indexOf('plus.google') !== -1) {
                this.record('google_plus', 'event35', 'Google +1');
            }
            this.opener(url);
        },
        /**
         * Url encode the string
         * @param  {string} str
         * @return {str}
         */
        encode: function (str) {
            return encodeURIComponent(str);
        },
        /**
         * Open a window to the url
         * @param  {string} url
         * @return {void}
         */
        opener : function(url){
            window.open(url, '_blank', 'height=400,width=680px,location=no,menubar=no,resizable=yes,scrollbars=yes,status=no,titlebar=no');
        },
        /**
         * Record the action on omniture and in our own share tracking
         * @param  {string} name The name of the item
         * @param  {string} eventName  The omniture event we are triggering
         * @param  {stromg} eventType The event type we are tracking
         * @return {void}
         */
        record : function(name, eventName, eventType) {
            $.ajax({
                url: "/misc/like/",
                type: "POST",
                data: {url: document.location.href, 'type': name, 'title': document.title}
            });
            // record in omniture
            var s = s_gi(omnitureShareAccount);
            s.events = eventName;
            s.linkTrackVars = 'eVar2,eVar4,eVar5,eVar6,eVar10,eVar16,eVar17,eVar18,eVar19,eVar25,eVar43,events';
            s.linkTrackEvents = eventName;
            s.tl(document, 'o', eventType);
        }
    };

    $(document).ready(function() {
        sharing.init();
    });

})($);


/* -- Martini-Floating-Sections.js -- */

/* jshint strict: true, browser: true, devel: true, jquery: true, indent: 4, curly: true */
;(function () {
    'use strict';
    var Floating = {
    	init: function() {
            if ($('.floating-advert').length === 0) {
                return;
            }

            $('.floating-advert').each(function (element) {
                var hc = Floating.checkHeight(this);
                if (!hc) {
                    return;
                }

                var _this = this;
            
                var top = $(this).offset().top - parseFloat($(this).css('marginTop').replace(/auto/, 0));

                $(this).attr('data-float-top', top);

                $(window).scroll(function () { Floating.setUp(_this)} );

                //window.setTimeout(Floating.setUp(this), 10000);
            });


    	},
        checkHeight: function(el) {
            
            var comparison = $(el).attr('data-comparison');

            // do we want to do a comparison to check we have the space?
            if (comparison === 'true') {

                // we make sure the bigger
                var bigger = $(el).attr('data-bigger-column');
                var lesser = $(el).attr('data-smaller-column');
                // grab the ehights
                var lesserHeight = $(lesser).height();
                var biggerHeight = $(bigger).height();
                if (lesserHeight > biggerHeight) {
                    return false;
                }

            }
            return true;

        },
        setUp: function(el) {

            // get the bottom element that we want to stop a colision with
            var collisionEl = $(el).attr('data-collision-element');
            var collision = $(collisionEl).offset().top;

            var floatingHeight = $(el).height();

            var currentScrollPosition = $(window).scrollTop();

            // (stored earlier, the original position of the div)
            var originalPosition = parseInt($(el).attr('data-float-top'));

            // we are scrolled down far enough
            if (currentScrollPosition >= originalPosition) {
                // add the class
                $(el).addClass('fixed');

                // if we might collide with the 
                if (currentScrollPosition + floatingHeight >= collision) {
                    // lock the position
                    var foot = collision - floatingHeight - 10;
                    $(el).css({'top' : foot, 'position' : 'absolute'});
                } else {
                    // remove the lock
                    $(el).removeAttr('style');
                }
            } else {
                $(el).removeClass('fixed');
            }


        },
    };
    window.Floating = Floating;
    $(window).bind('load', function(){
        Floating.init();
    });
})();


/* -- Martini-Image-Search.js -- */

/* jshint strict: true, browser: true, devel: true, jquery: true, indent: 4, curly: true */
;(function (isMobileDevice) {
    'use strict';

    /**
     * Improved image search.
     * Can be attached to any element with a class of: call-image-search
     * Required attributes: data-user-id (The current user id)
     *                      data-target: (The full name of the id you want to populate)
     * Optional attributes: data-width (Set the default width)
     *                      data-width-type (Set the type (ie, > , >= etc))
     *                      data-height (set the default height)
     *                      data-height-type (Set the type (ie, > , >= etc))
     * @type {Object}
     */
    var ImageSearch = {
        width: 0,
        height: 0,
        heightType: '',
        widthType: '',
        resultTarget : '',
        currentUserId : '',
        limit:28,
        /**
         * Entrance function, sets up all basic variables and launches the template
         * @param  {DomElement} el The calling element
         * @return {void}
         */
        display: function(el) {

            // restore the basic options in case this is a subsequent item
            this.width = 0;
            this.height = 0;
            this.widthType = '';
            this.heightType = '';
            this.clearResults();

            // clear any history from the form
            $('#image-search input').val('').attr('disabled', null);

            // get the width and height parameters
            var searchWidth = parseInt($(el).attr('data-width'));
            var searchHeight = parseInt($(el).attr('data-height'));

            // check we have valid ints for the height and widthj
            if (!isNaN(searchWidth)) {
                this.width = searchWidth;
            }
            if (!isNaN(searchHeight)) {
                this.height = searchHeight;
            }

            // set everything we need
            this.heightType = $(el).attr('data-height-type');
            this.widthType = $(el).attr('data-width-type');
            this.resultTarget = $(el).attr('data-target');
            this.currentUserId = $(el).attr('data-user-id');

            // grab the template from the web service
            this.loadTemplate();
        },
        /**
         * Check if we have the template loaded, if not do a get request to fetch it
         * @return {void}
         */
        loadTemplate: function() {
            // is the template pre-laoded into the dom?
            if ($('#image-search').length === 1) {
                return this.launch();
            }

            // grab it
            $.get('/ws/public/template/ImageSearch/', {}, ImageSearch.load);

        },
        /**
         * Result of the ajax request, appending the content to the Dom and setting it as a dialog box
         * @context AjaxRequest
         * @param  {mixed} data The result of the ajax request
         * @return {void}
         */
        load: function(data) {
            
            var dialogWidth = 640;
            var dialogHeight = 600;
            if (isMobileDevice) {
                dialogWidth = 300;
                dialogHeight = 400;
            }

            // append it
            var html = data.content;
            $('body').append(html);

            //make it a dialogue box
            $("#image-search").dialog({
                bgiframe: true,
                autoOpen: false,
                height: dialogHeight,
                width: dialogWidth,
                title: 'Image search',
                modal: true
            });

            // grab the current user and add it to the form
            var userOption = $('<option></option>').val(ImageSearch.currentUserId).html('Only me');
            $('#image-search-user').append(userOption);

            // launch it
            ImageSearch.launch();

        },
        /**
         * Open the dialog, after ensuring all the buttons work and the content is correct
         * @return {void}
         */
        launch: function() {

            // bind the search and clear buttons
            this.bind();

            // set all the defualt settings
            this.setDefaults();

            // open it
            $('#image-search').dialog('open');


        },
        /**
         * Perform the search bindings
         * @return {void}
         */
        bind: function() {
            $('#image-search-submit').unbind('click').click(function (e) {
                e.preventDefault();
                ImageSearch.search();
            });
            $('#image-search-clear').unbind('click').click(function (e) {
                e.preventDefault();
                ImageSearch.clearResults();
                ImageSearch.setDefaults();

            });
        },
        /**
         * Bind all the result buttons and images
         * @return {void}
         */
        bindResults: function() {
            // handle a direct select
            $('.artEditImgSelect').unbind('click').click(function (e) {
                e.preventDefault();
                ImageSearch.handleSelect(this);
            });
            // handle a request for more details
            $('.image-search-image, artEditImgSelect, .artEditImgViewDetail').unbind('click').click(function (e) {
                e.preventDefault();
                ImageSearch.handleDetails(this);
            });
            // close the search details
            $('.artEditImgClose').unbind('click').click(function (e) {
                e.preventDefault();
                $('#image-search-details').hide();
            });
            // set the page to update the site as required
            $('#image-search-pager').unbind('change').change(function (e) {
                var val = $(this).val();
                $('#image-search-current-page').val(val);
                ImageSearch.search();
            });
            // handle the previous buttons
            $('#image-search-prev').unbind('click').click(function (e) {
                e.preventDefault();

                // get the current page
                var val = parseInt($('#image-search-current-page').val());

                // we don't do anyting if we are already on zero
                if (val === 0) {
                    return;
                }
                // decreate the value
                val--;

                // dop a new search
                $('#image-search-current-page').val(val);
                ImageSearch.search();
            });
            // handle the next button
            $('#image-search-next').unbind('click').click(function (e) {
                e.preventDefault();
                // get the current
                var val = parseInt($('#image-search-current-page').val());
                // increment
                val++;
                $('#image-search-current-page').val(val);
                ImageSearch.search();
            });

        },
        /**
         * Get the current id of the image/item selected and place it in the defined target
         * @param  {DomElement} e
         * @return {void}
         */
        handleSelect: function (e) {
            var id = $(e).attr('data-id');
            $(this.resultTarget).val(id);
            // call a change on the target, to ensure it gets captured
            $(this.resultTarget).change();
            $('#image-search').dialog('close');
        },
        /**
         * Display the detials overlay with the items from the domelement that has been clicked on
         * @param  {DomElement} e
         * @return {void}
         */
        handleDetails: function(e) {
            $('#image-search-details').show();

            $('#image-search-details-id').html($(e).attr('data-id'));
            $('#image-search-details-image').attr('src', '/resources/images/'+$(e).attr('data-id')+'/?type=thumb');
            $('#image-search-details-description').html($(e).attr('data-description'));
            $('#image-search-details-dimensions').html($(e).attr('data-size'));
            $('#image-search-details-timestamp').html($(e).attr('data-timestamp'));
            $('#image-search-details-filename').html($(e).attr('data-name'));
            $('#image-search-details-macro').html('%image('+$(e).attr('data-id')+')');
            $('#image-search-details-path-macro').html('%imagepath('+$(e).attr('data-id')+')');


        },
        /**
         * Clear all the results dom elements for a new search or a new pager
         * @return {void}
         */
        clearResults: function () {
            $('#image-search input, #image-select select').val('').attr('disabled', null);
            $('#image-search-results').empty();
            $('#image-search-form').show();
            $('#image-search-newsearch').hide();
            $('#image-search-current-page').val('0');
        },
        /**
         * Reset all the defaults
         */
        setDefaults: function () {
            // preset the width
            if (this.width > 0) {
                $('#image-search-width').attr('disabled', 'disabled').val(this.width);
            }

            // preset the height
            if (this.height > 0) {
                $('#image-search-height').attr('disabled', 'disabled').val(this.height);
            }

            // set the heightType
            if (this.heightType !== '' && this.heightType !== undefined) {
                $('#image-search-height-filter').attr('disabled', 'disabled').val(this.heightType);
            }

            // set the width type
            if (this.widthType !== '' && this.widthType !== undefined) {
                $('#image-search-width-filter').attr('disabled', 'disabled').val(this.widthType);
            }

            // always zero to start with
            $('#image-search-current-page').val('0');

            $('#image-search-order').val('timestampdesc');

            $('#image-search-deleted').val('0');

            $('#image-search-this-site-only').attr('checked', null);

        },
        /**
         * Perform the search with an ajaz request
         * @return {void}
         */
        search: function() {
            // show a progress spinner
            this.showProgressBar();

            // build the data we require
            var options = {};
            options.width = $('#image-search-width').val();
            options.width_filter = $('#image-search-width-filter').val();
            options.height = $('#image-search-height').val();
            options.height_filter = $('#image-search-height-filter').val();
            options.keyword = $('#image-search-keyword').val();
            options.order_by = $('#image-search-order').val();
            options.deleted = $('#image-search-deleted').val();
            options.site_only = ($('#image-search-this-site-only').is(':checked')) ? 1 : 0;
            options.page = $('#image-search-current-page').val();
            options.user_id = $('#image-search-user').val();
            options.limit = this.limit;

            // request some results
            $.post('/ws/public/ImageSearch/', options, ImageSearch.displayResults);
        },
        displayResults : function(data) {
            // get the results
            var results = data.data;
            // and the total
            var total = data.total;
            // remove the progress spinner
            $('#image-search-progress-bar').remove();

            //clear any previous results
            $('#image-search-results').empty();

            if (total === 0) {
                $('#image-search-pager-totals').html('No results found');
                $('#image-search-results').html('No results found');
                return;
            }

            // create the li for each search result
            $.each(results, function(i, d) {
                var li = ImageSearch.createLi(d);
                $('#image-search-results').append(li);
            });

            // generate the pager
            ImageSearch.generatePager(total);

            // and bind the new buttons
            ImageSearch.bindResults();
        },
        /**
         * Generate the pager and total description from the results
         * @param  {int} total
         * @return {void}
         */
        generatePager: function(total) {

            // get the current value
            var currentPage = parseInt($('#image-search-current-page').val());
            // work out what we are doing and showing
            var showing = (this.limit * currentPage) + 1;
            var to = showing + this.limit;

            // set the html
            $('#image-search-pager-totals').html("Showing "+showing+" to "+to+" of "+total);

            // clear old results
            $('#image-search-pager').empty();
            // see how many pages we need
            var pages = Math.floor(total/this.limit);
            for (var i = 0; i <= pages; i++) {
                // build the item
                var option = $('<option></option>').val(i).text((i+1));
                if (i === currentPage) {
                    option.attr('selected', 'selected');
                }

                // append it
                $('#image-search-pager').append(option);
            }

        },
        /**
         * Show the progress bar and hide other elements
         * @return {void}
         */
        showProgressBar: function() {
            // hide the form
            $('#image-search-form').hide();
            // show the new search bar
            $('#image-search-newsearch').show();

            // set the spinner
            var img = $('<img>').attr('src', '/resources/static/ajax-loader.gif').attr('style', 'margin: auto').attr('id', 'image-search-progress-bar');
            $('#image-search-results').append(img);
        },
        /**
         * Create an li element for the dom results, with the data-attribute spammed with all the details we require
         * @param  {object} data
         * @return {DomElement}
         */
        createLi: function(data) {
            // holder
            var li = $('<li></li>');
            // standard link with attributes
            var a = $('<a></a>').addClass('image-search-image artEditImgViewDetail').attr('data-id', data.id).attr('data-size', data.width+'x'+data.height).attr('data-description', data.description).attr('data-timestamp', data.timestamp).attr('data-name', data.filename);
            // select link
            var c = $('<a></a>').attr('data-id', data.id).html('Select').addClass('artEditImgSelect');
            var b = $('<a></a>').attr('data-id', data.id).addClass('artEditImgSelect');
            // secondary details link
            var a2 = a.clone().removeClass().addClass('artEditImgViewDetail').html('Details');
            // thumbnail
            var img = $('<img>').attr('src', '/resources/images/'+data.id+'/?type=thumb').addClass('artEditImg');
            b.append(img);
            li.append(b, a2, c);

            return li;
        },
        init: function() {
            $('.call-image-search').unbind('click').click(function (e) {
                e.preventDefault();

                ImageSearch.display(this);
            });
        }
    };
    window.ImageSearch = ImageSearch;
    // init function call
    $(document).ready(function () {
        ImageSearch.init();
    });

})(isMobileDevice);


/* -- Martini-Image-Upload.js -- */

/* jshint strict: true, browser: true, devel: true, jquery: true, indent: 4, curly: true */
;(function () {
    'use strict';

    /**
     * A replacement for a standad <form - input-file element, to allow for image uploads only, with controllable pre-upload valdiation
     * @type {Object}
     */
    var ImageUpload = {
        // the target input, to which a succesful image id will be added - '#imageid'
        dataTarget : '',
        // the default width - set with data-width in the file element
        defaultWidth: 0,
        // the default height - set with the data-height attribute in the file element
        defaultHeight: 0,
        // the default comaparitor for the width, data-width-type, can be > >= = < <=
        defaultWidthType : '',
        // the default comaparitor for the height, data-height-type, can be > >= = < <=
        defaultHeightType : '',

        // handle multi uploads
        isMulti: false,
        // some place holders so we can recreate the button properly
        originalClass : '',
        originalStyle : '',
        originalName : '',
        errorStack: [],
        callBack: null,

        // the id of the element clicked
        id: null,

        /**
         * Standard init, we check the browser is capable and then assign the correct action to the selector
         * @return {void}
         */
        init: function() {

            // no compat, error out.  We no longer fallback
            if (!this.checkBrowser()) {
                $('.call-image-upload').replaceWith('<div class="alert alert-error">You must use a modern browser</div>');
                return false;
            }

            // set the change trigger
            $('.call-image-upload').unbind('change').change(function (e) {
                ImageUpload.upload(e, this);
            });
        },
        /**
         * Check if a browser actually supports the actions that we need
         * @return {bool}
         */
        checkBrowser: function() {
            // if we don't have the FileReader api, then abort
            if (typeof FileReader === 'undefined') {
                return false;
            }

            return true;
        },

        /**
         * Run a series of checks, and then try and upload it via the webservice api
         * @param  {event} e  The event from the button change
         * @param  {element} el The element the change occured from
         * @return {void}
         */
        upload: function (e, el) {
            // remove all previous image errors
            $('.image-error').remove();
            this.id = $(el).attr('id');

            this.originalStyle =  $(el).attr('style');
            this.originalClass =  $(el).attr('class');
            this.originalName  =  $(el).attr('name');

            this.isMulti = $(el).attr('multiple');
            // extract the data target
            this.dataTarget = $(el).attr('data-target');

            // extract the size types
            this.defaultWidthType = $(el).attr('data-width-type');
            this.defaultHeightType = $(el).attr('data-height-type');

            // get the dfaults
            var defaultWidth = $(el).attr('data-width');
            var defaultHeight = $(el).attr('data-height');

            // check we have a valid int, and then update the default width if required
            if (!isNaN(parseInt(defaultWidth))) {
                this.defaultWidth = defaultWidth;
            }

            // check we have a valid height
            if (!isNaN(parseInt(defaultHeight))) {
                this.defaultHeight = defaultHeight;
            }

            var callBack = $(el).attr('data-callback');

            // check we have a valid attr
            if (typeof callBack !== 'undefined') {
                this.callBack = callBack;
            }

            // create with an uploading gif
            var img = $('<img />').attr('src', '/resources/static/standard/images/loading.gif').addClass('image-upload-temporary-image');


            $('#'+this.id).replaceWith(img.clone().attr('id', this.id));

            // get the actual event, a window event if null
            var e = e || window.event;

            // prevent anything from bubbling up
            e.preventDefault();
            // jQuery wraps the originalEvent, so we try to detect that here...
            var e = e.originalEvent || e;

            // get the actual file, it is stored as a FileList
            var files = e.files || e.target.files;
            var errors = []; // Failures are dumped here as booleans
            var counter = 0; // Counter is used to keep track of that pass type validation but "may" fail size validation
            var stack = files.length; // How many files have been selected?
            // iterate through the file list
            $.each(files, function (i, file) {
                // we only handle images here
                if (!file.type.match('image.*'))
                {
                    alert("Sorry, You can only upload images here.");
                    stack--; // Decrease stack, not important for our needs
                } else {

                    // get a reader
                    var reader = new FileReader();
                    reader.errors = errors;
                    // as the file is loaded, run this function
                    reader.onloadend = function() {
                        console.log('File reader onloadend');
                        console.log("Errors length %s", errors.length);
                        var fileData = reader.result;
                        var fileName = file.name;

                        // load the image into the dom so we can chekc the width/height
                        var img = new Image();
                        img.src = reader.result;

                        // onload, call back function
                        img.onload = function () {
                            // check we are valid, and exit if not
                            if (!ImageUpload.imageValidate(this, fileName))
                            {
                                errors.push(true);

                            } else {
                                // we are, upload it
                                var data = {file: fileData, name: fileName};

                                $.ajax({
                                    type: 'POST',
                                    url: '/ws/images/add/',
                                    data: data,
                                    success: ImageUpload.imageSuccess,
                                    error: ImageUpload.imageError
                                });
                            }
                            counter++;
                            if(counter == stack) { //Finished validating size
                                if(errors.length > 0 ) { //Lets see if there's any errors...
                                    var button = ImageUpload.createNewButton();
                                    $('#'+ImageUpload.id).replaceWith($(button));
                                    ImageUpload.init();
                                    return;
                                }
                            }
                        };
                    };
                    // load the file from the filesystem
                    reader.readAsDataURL(file);
                }

                if (! --files) { //This gets executed before image size is validated and only handles images tht fail type validation
                     if(errors.length > 0 ) {
                        var button = ImageUpload.createNewButton();
                        $('#'+ImageUpload.id).replaceWith(button);
                        ImageUpload.init();
                        return;
                     }
                }
            });
        },
        /**
         * We have a successful upload, so we populate the target specified earlier
         * @context AjaxUpload
         * @param  {object} data
         * @return {void}
         */
        imageSuccess : function (data) {

            // extract the id
            var imageId = data.id;
            $(ImageUpload.dataTarget).val(imageId);

            // call the change on the target, just in case
            $(ImageUpload.dataTarget).change();

            var button = ImageUpload.createNewButton();
            $('#'+ImageUpload.id).replaceWith(button);
            // rebind the cleared form
            ImageUpload.init();


            if (typeof ImageUpload.oldImage === 'object') {
                 var img = ImageUpload.oldImage.attr('src', '/resources/images/'+imageId+'/?type=mc1');
                 $(ImageUpload.imageThumb).replaceWith(img);
            }

            if (ImageUpload.callBack !== null && ImageUpload.callBack.length > 0) {
                call.fromString(ImageUpload.callBack, window, imageId, ImageUpload.id);
            }
        },
        /**
         * The image failed to upload
         * @param  {jqXHR object} data
         * @param  {string} status
         * @param  {string} error
         * @return {void}
         */
        imageError: function (data, status, error) {

            $(ImageUpload.dataTarget).before($('<div></div>').addClass('alert alert-error image-error').html('Image failed to upload - '+error));
            $(ImageUpload.imageThumb).replaceWith(this.oldImage);
            $('#'+ImageUpload.id).replaceWith(ImageUpload.oldButton);
        },
        /**
         * Perfrom the validation of the image
         * @param  {Image} img
         * @return {bool}
         */
        imageValidate: function (img, filename) {
            // hold any errors
            var errString = '';

            // check the width
            try {
                this.imageSizeValidate(img.width, this.defaultWidthType, this.defaultWidth, 'width');
            }
            catch (err) {
                errString += "<strong>" + filename + "</strong>: " + err.message;
            }

            // check the height
            try {
                this.imageSizeValidate(img.height, this.defaultHeightType, this.defaultHeight, 'height');
            }
            catch (err) {
                errString += '<br />'+ filename + ":" +  err.message;
            }

            // if we have an error, we expose it to the dom and return false
            if (errString.length > 0) {
                $(this.dataTarget).before($('<div></div>').addClass('alert alert-error image-error').html(errString));
                return false;
            }

            return true;

        },
        /**
         * Check the image file dimensions with the dimensions specified in the dom
         * @param  {int} imgDimension
         * @param  {string} symbol
         * @param  {int} target
         * @param  {string} name
         * @throws {error} If [size does not match]
         * @return {void}
         */
        imageSizeValidate: function(imgDimension, symbol, target, name) {

            // we don't have a comparitor or a valid size
            if (symbol === ''  || target === 0) {
                return '';
            }
            target = parseInt(target);
            // check we have a valid symbol
            switch (symbol)
            {
            case '>':
                if (imgDimension <= target)   {
                    throw new Error("The image "+name+" must be greater than "+target+"px ("+imgDimension+"px)");
                }
                break;
            case '>=':
                if (imgDimension < target)   {
                    throw new Error("The image "+name+" must be greater than or equal to "+target+"px ("+imgDimension+"px)");
                }
                break;
            case '=':
                if (imgDimension !== target)   {
                    throw new Error("The image "+name+" must be "+target+"px ("+imgDimension+"px)");
                }
                break;
            case '<=':
                if (imgDimension > target)   {
                    throw new Error("The image "+name+" must less than or equal to "+target+"px ("+imgDimension+"px)");
                }
                break;
            case '<':
                if (imgDimension <= target)   {
                    throw new Error("The image "+name+" must be less than "+target+"px ("+imgDimension+"px)");
                }
            }
        },
        createNewButton: function() {
            var b = $('<input />').attr('type', 'file');
            b.attr('data-width', this.defaultWidth);
            b.attr('data-height', this.defaultHeight);
            b.attr('data-callback', this.callBack);
            b.attr('data-target', this.dataTarget);
            b.attr('id', this.id);
            b.attr('data-width-type', this.defaultWidthType);
            b.attr('data-height-type', this.defaultHeightType);
            b.addClass(this.originalClass);
            b.attr('name', this.originalName);
            b.attr('style', this.originalStyle);
            if (this.isMulti) {
                b.attr('multiple', '');
            }

            return b;
        }

    };
    window.ImageUpload = ImageUpload;
    $(document).ready(function (e) {
        ImageUpload.init();
    });
}());


/* -- Martini-Jobs.js -- */

;(function($) {

    "use strict";

    var jobAlertsHandler = {
	    init: function()
	    {
			$('#subscribe-to-job-alerts-top').submit(function(e) {
			    e.preventDefault();
			    jobAlertsHandler.submitNewJobAlert($(this).attr('action'), $(this).serialize());
			});
	    	
			$('#subscribe-to-job-alerts-bottom').submit(function(e) {
			    e.preventDefault();
			    jobAlertsHandler.submitNewJobAlert($(this).attr('action'), $(this).serialize());
			});

			$('.frequency-immediately').click(function(e){
			    e.preventDefault();
			    jobAlertsHandler.setFrequencyImmediately($(this).attr('id'));
			});
			
			$('.frequency-daily').click(function(e){
			    e.preventDefault();
			    jobAlertsHandler.setFrequencyDaily($(this).attr('id'));
			});
			
			$('.frequency-paused').click(function(e){
			    e.preventDefault();
			    jobAlertsHandler.setFrequencyPaused($(this).attr('id'));
			});
			
			$('.delete-alert').click(function(e){
			    e.preventDefault();
			    jobAlertsHandler.deleteJobAlert($(this).attr('id'));
			});
			
			$('.confirm-delete').click(function(e){
			    e.preventDefault();
			    jobAlertsHandler.confirmDeleteJobAlert($(this).attr('id'));
			});
			
			$('.cancel-delete').click(function(e){
			    e.preventDefault();
			    jobAlertsHandler.cancelDeleteJobAlert($(this).attr('id'));
			});
	    },
		submitNewJobAlert: function(action, data) {
			$('.process-job-alert').html('<div class="alert jobsByEmail"><h4><i class="fa fa-spinner fa-spin"></i>  We are processing your alert</h4></div>');
		    $.ajax({
		        url: action,
		        method: "POST",
		        data: data,
		        success: function(r) {
		            if(r.success === true) {
		            	$('.jobs-by-email').replaceWith(r.message_body);
		            }
		            else
		            {
		            	$('.jobs-by-email-errors').html(r.message_body);
		            }
		        }
		    });
		    
		    $('.process-job-alert').html('');
		},
		setFrequencyImmediately: function(job_alert_id) {		    
		    $('.frequency-immediately-'+job_alert_id).addClass('btnGreen');
		    $('.frequency-immediately-'+job_alert_id).html('<i class="fa fa-spinner fa-spin"></i> Please wait');
		    $('.frequency-daily-'+job_alert_id).removeClass('btnGreen');
		    $('.frequency-daily-'+job_alert_id).html('Daily');
		    $('.frequency-paused-'+job_alert_id).removeClass('btnGreen');
		    $('.frequency-paused-'+job_alert_id).html('Paused');
		    
		    $.ajax({
		        url: "/jobs/set-frequency/",
		        method: "POST",
		        data: {job_alert_id: job_alert_id, frequency: "immediately"},
		        success: function(r) {
		            $('.frequency-immediately-'+job_alert_id).html('<i class="fa fa-check"></i> Immediately');
		        }
		    });
			
		},
		setFrequencyDaily: function(job_alert_id) {		    
		    $('.frequency-daily-'+job_alert_id).addClass('btnGreen');
		    $('.frequency-daily-'+job_alert_id).html('<i class="fa fa-spinner fa-spin"></i> Please wait');
		    $('.frequency-immediately-'+job_alert_id).removeClass('btnGreen');
		    $('.frequency-immediately-'+job_alert_id).html('Immediately');
		    $('.frequency-paused-'+job_alert_id).removeClass('btnGreen');
		    $('.frequency-paused-'+job_alert_id).html('Paused');
		    
		    $.ajax({
		        url: "/jobs/set-frequency/",
		        method: "POST",
		        data: {job_alert_id: job_alert_id, frequency: "daily"},
		        success: function(r) {
		            $('.frequency-daily-'+job_alert_id).html('<i class="fa fa-check"></i> Daily');
		        }
		    });
			
		},
		setFrequencyPaused: function(job_alert_id) {
		    $('.frequency-paused-'+job_alert_id).addClass('btnGreen');
		    $('.frequency-paused-'+job_alert_id).html('<i class="fa fa-spinner fa-spin"></i> Please wait');
		    $('.frequency-immediately-'+job_alert_id).removeClass('btnGreen');
		    $('.frequency-immediately-'+job_alert_id).html('Immediately');
		    $('.frequency-daily-'+job_alert_id).removeClass('btnGreen');
		    $('.frequency-daily-'+job_alert_id).html('Daily');
		    
		    $.ajax({
		        url: "/jobs/set-frequency/",
		        method: "POST",
		        data: {job_alert_id: job_alert_id, frequency: "paused"},
		        success: function(r) {
		            $('.frequency-paused-'+job_alert_id).html('<i class="fa fa-check"></i> Paused');
		        }
		    });
		},
		deleteJobAlert: function(job_alert_id) {
			$('.confirm-delete-'+job_alert_id).html('Confirm delete');
			$('.cancel-delete-'+job_alert_id).html('Cancel');
			$('.delete-alert-'+job_alert_id).html('');
			$('.confirm-delete-'+job_alert_id).css("visibility", "visible");
			$('.cancel-delete-'+job_alert_id).css("visibility", "visible");
			$('.delete-alert-'+job_alert_id).css("visibility", "hidden");
		},
		confirmDeleteJobAlert: function(job_alert_id) {		    
		    $.ajax({
		        url: "/jobs/delete-job-alert/",
		        method: "POST",
		        data: {job_alert_id: job_alert_id},
		        success: function(r) {
		            location.href = '/my/account/profile/jobs/';
		        }
		    });
		},
		cancelDeleteJobAlert: function(job_alert_id) {
			$('.confirm-delete-'+job_alert_id).html('');
			$('.cancel-delete-'+job_alert_id).html('');
			$('.delete-alert-'+job_alert_id).html('<i class="fa fa-times"></i> Delete');
			$('.confirm-delete-'+job_alert_id).css("visibility", "hidden");
			$('.cancel-delete-'+job_alert_id).css("visibility", "hidden");
			$('.delete-alert-'+job_alert_id).css("visibility", "visible");
		}
    };

    $(document).ready(function() {
    	jobAlertsHandler.init();
    });

})($);

/* -- Martini-Live-Events.js -- */

;(function() {
    var liveEventsFE = {
        autoOn : null,
        autoOff : null,
        domClass : '',
        init: function () {
            if ($('#updateList').length !== 1) {
                return;
            }

            // load the inital stuff
            this.pollServer();

            // get a cookie, and some parts of the dom
            var autoUpdateCookie = $.cookie('autoUpdate'),
                autoOn = $('.articleAutoUpdate span:contains("YES")'),
                autoOff = $('.articleAutoUpdate span:contains("NO")');
            this.domClass = 'articleLiveSelected';


            // mobile sites
            if (autoOn.length === 0) {
                autoOn = $('.auto-update span:contains("YES")');
                autoOff = $('.auto-update span:contains("NO")');
                this.domClass = 'auto-selected';
            }

            // store the classes
            this.autoOn = autoOn;
            this.autoOff = autoOff;


            // bind the results to clicking on the buttons
            autoOn.click(function() {
                liveEventsFE.handleClick(true);
            });
            autoOff.click(function() {
                liveEventsFE.handleClick(false);
            });

            // if we have a valid cookie
            if(autoUpdateCookie) {
                var b = (autoUpdateCookie === 'On');
                this.handleClick(b);
            }

            // init the auto update feature - turned off to start with
            $("#autoupdate").at_intervals(liveEventsFE.pollServer, { delay: 60000, name: "get_events"});

        },
        // set a temporary session cookie
        liveUpdateCookie : function (val) {
            $.cookie('autoUpdate', val);
        },
        // a click handler for the on/off buttons
        handleClick: function(bool) {
            if (bool) {
                this.liveUpdateCookie('On');
                this.autoOn.addClass(this.domClass);
                this.autoOff.removeClass();
                $('#autoupdate').show();
            } else {
                this.liveUpdateCookie('Off');
                this.autoOff.addClass(this.domClass);
                this.autoOn.removeClass();
                $('#autoupdate').hide();
            }
        },
        pollServer: function() {
            var foreignId = $('#foreign_id').val();
            var path = '/ws/public/live-event/'+foreignId+'/';

            $.ajax({
                url: path,
                type: 'GET',
                cache: false,
                success: liveEventsFE.update
            });
        },
        update: function(result) {

            if (typeof result === 'string') {
                result = $.parseJSON(result);
            }

            var events = result.events;

            var hasBrightcove = false;

            if (events.length === 0 && result.updated_events.length === 0) {
                return;
            }

            var latest = $('#latest').val();
            events = events.reverse();

            $.each(events, function (i, e) {                
                if (e.time > latest && $('#event-'+e.id).length === 0) {
                    $('#updateList').prepend(e.html);
                    // slide the content down, then fade it in - snazzy.
                    $('#event-'+e.id).slideDown(2000, function() {
                        $("#event-"+e.id).animate({ opacity: "1" }, 1000);
                    });
                }
                if (e.html.indexOf('brightcove') !== -1) {
                    hasBrightcove = true;
                }
            });

            // replace any updated live events
            $.each(result.updated_events, function(i, e) {
                if (e.time > latest) {
                    console.log('editing '+e.id);
                    var id = e.id;

                    if (e.deleted == 1) {
                        $('#event-'+id).remove();
                    }
                    else {
                        $('#event-'+id).replaceWith(e.html);
                    }
                }
                if (e.html.indexOf('brightcove') !== -1) {
                    hasBrightcove = true;
                }
            });

            // update with the latest
            $('#latest').val(result.time);
            
            if (hasBrightcove) {
                MartiniBrightcove.gatherDetails();
                MartiniBrightcove.expand();
            }
        }
    };
    // add to global
    window.liveEventsFE = liveEventsFE;

    // init it on page load
    $(document).ready(function() {
        liveEventsFE.init();
    });

})();


/* -- Martini-Panels.js -- */

/**
 * Singleton handler to load all panels from the system
 */

var panelHandler = {

    defaultStorageTime : 900, // 15 minutes

    loadPanel : function(panelName, id, params, callback)
    {
        $(function() {
            panelHandler._loadPanel(panelName, id, params, callback);
        });
    },
    _loadPanel : function(panelName, id, params, callback)
    {
        if (typeof params == 'undefined')
        {
            params = {};
        }
        if (typeof callback !== 'function')
        {
            callback = function() {};
        }

        var localCopy = null;

        var panelExtra = (typeof params.ns === 'string') ? params.ns : '';

        var panelKey = panelName + panelExtra;

        if(localCopy = panelHandler.getLocalCopy(panelKey, id))
        {
            panelHandler.loadHtml(id, localCopy);
            callback();
        }
        else
        {
            params.id = id;
            var url = '/resources/ph/'+panelName+'/';
            this.load(url, params, callback);
        }
    },
    load : function (url, params, callback)
    {
        $.ajax({
            type: 'GET',
            url: url,
            dataType: 'json',
            data: params
        })
        .success(function(result){
            panelHandler.loadHtml(params.id, result);

            var panelExtra = (typeof params.ns === 'string') ? 'new' : '';

            var panelKey = result.panelName + panelExtra;
            panelHandler.storeLocalCopy(panelKey, params.id, result);
            callback();
        })
        .error(function(result){
            // TODO:
            // fake blank panel so we do not continue hammering the
            // same panel code in case of failure
        });
    },
    loadHtml : function(id, panelData, callback)
    {
        try
        {
            $("#"+id).html(panelData.html);
        }
        catch (err)
        {
            // the dom element doesn't exist, so exit
            return;
        }

        switch(panelData.panelName)
        {
        case 'EventsLiveTravelPanel':
            panelHandler.liveTravel(panelData, id);
            break;
        case 'MostPopularDataPanel':
        case 'MostPopularDataPanelSport':
            panelHandler.mostPopular(panelData, id);
            break;
        }
        martini.attachClearOnFocus();
        martini.attachClearOnSubmit();
    },
    liveTravel : function(panelData, id)
    {
        if (panelData.html == ''){
            return;
        }

        $("#"+id).html(panelData.html.html);

        if (document.location.toString().match(/education/))
        {
            $(".whatsTravel").tabs();
            return;
        }

        // current time
        var dateObj = new Date();
        var currentTime = dateObj.getTime();

        //different times
        // morning starts at 7:30
        var morningStart = new Date();
        morningStart.setHours(7, 30, 00);

        // and ends at 9:30
        var morningEnd = new Date();
        morningEnd.setHours(9, 30, 00);

        // evening starts at at 4 30
        var eveningStart = new Date();
        eveningStart.setHours(16, 30, 00);

        // and ends at 6 30
        var eveningEnd = new Date();
        eveningEnd.setHours(18, 30, 00);


        var inTime = false;
        // check if we're in a time period'
        if ((currentTime > morningStart.getTime() &&
             currentTime < morningEnd.getTime()) ||
            (currentTime > eveningStart.getTime() &&
             currentTime < eveningEnd.getTime() ))
        {
            inTime = true;
        }


        if (panelData.html.hasImportantIncidents && inTime)
        {
            $(".whatsTravel").tabs({selected: 1});
        }
        else if (panelData.html.severeIncidents.length > 0)
        {
            $(".whatsTravel").tabs({selected: 1});
        }
        else if (panelData.html.noIncidents)
        {
                $(".whatsTravel").tabs();
        }
        else
        {
            if ($('#li-first').html() == '1')
            {
                $(".whatsTravel").tabs({selected: 1});
            }
            else
            {
                $(".whatsTravel").tabs();
            }
        }
        if (panelData.html.severeIncidents.length > 0)
        {
            $.each(panelData.html.severeIncidents, function (key, value)
            {
                var container = $('<div class="art"></div>');
                var container2 = $('<div class="artOverride"></div>');
                var link = $('<a></a>').attr('href', '/li/traffic.detail.'+value.id);
                var summarylink = $('<a></a>').attr('href', '/li/traffic.detail.'+value.id);

                // create the headline with a h4
                var headline = $('<h2></h2>');

                // the traffic em sits inside the headline
                var traffic = $('<em class="important"></em>').html('Traffic Alert');
                headline.append(traffic);

                // add the headline texti
                headline.append(value.route+', '+value.area);
                var incidentDelta = $('<p class="update"></p>').html('Last updated '+panelHandler.calcDelta(value.update_time));
                var strDescription = $('<p></p>').addClass('summary');
                strDescription.append(summarylink.html(value.message));

                var cameraLink = '';
                if (value.camera_ids != null)
                {
                    cameraLink = $('<p></p>');
                    var cameraLinkA = $('<a class="trafficCamLink"></a>').attr('href', '/li/traffic.detail.'+value.id).html('View live traffic camera');
                    cameraLink.append(cameraLinkA);
                }

                link.append(headline, incidentDelta, strDescription, cameraLink);

                container2.append(link);
                container.append(container2);

                try
                {
                    $('#trafficupdatesinarticle').append(container);
                }
                catch (err)
                {
                }

            });
        }

    },
    mostPopular: function(panelData)
    {
        var isSport = (window.location.pathname.toString().indexOf("/sport") > -1);

        $.each(panelData.html, function(g, f)
        {

            // we are in a sport section, we do not show most read
            if (f.title === 'Most read' && isSport) {
                return;
            }
            // we are not in the sport section, so we don't show Most read sport
            else if (f.title === 'Most read sport' && !isSport) {
                return;
            }
            var container = $('<div></div>');
            var ol = $('<ol></ol>');

            container.append($('<h4 class="your sectionHead"></h4>').html(f.title));

            $.each(f.data, function (i, h)
            {
                var li = $('<li></li>');
                var href = $('<a></a>').attr('href', h.link);

                if (h.show_count)
                {
                    href.html(h.headline + ' ('+h.count+')');
                }
                else
                {
                    href.html(h.headline);
                }

                li.append(href);

                if (h.show_delta_and_cat)
                {
                    var categoryLink = $('<a></a>').attr('href', h.category_link).html(h.category);
                    var deltaStr = panelHandler.calcDelta(h.date_posted);
                    var delta = $('<span></span>').html(' ('+deltaStr+' in ');
                    delta.append(categoryLink);
                    delta.append(')');
                    li.append(delta);
                }
                ol.append(li);
            });
            if (f.title == "Most recent")
            {
                var l = $('<li><a href="/today/" class="bold">More from today</a></li>');
                ol.append(l);
            }
            container.append(ol);
            $('#'+f.id+'_content').append(container);
        });
    },
    calcDelta: function(timestamp)
    {
        var n = Math.round(new Date().getTime() / 1000);
        var delta = n - timestamp;
        if (delta < 60)
        {
            return delta + ' seconds ago';
        }

        delta = parseInt(delta / 60)

        if (delta < 60)
        {
            s = ((delta == 1)) ? '':'s';
            return delta + ' minute'+s+' ago'
        }
        delta = parseInt(delta / 60)

        if (delta < 24)
        {
            s = (delta == 1) ? '':'s';
            return delta + ' hour'+s+' ago'
        }
        delta = parseInt(delta / 24)
        s = (delta == 1) ? '':'s';
        return delta + ' day'+s+' ago'
    },
    /**
     * Test if browser supports localStorage and JSON
     */
    hasLocalStorage : function()
    {
        var uid = new Date;
        var localStorageTest, jsonTest;

        try {
            // Check local storage
            localStorage.setItem(uid, uid);
            localStorageTest = localStorage.getItem(uid) == uid;
            localStorage.removeItem(uid);

            // Check JSON object
            jsonTest = JSON.parse('{"test": 1}')
            jsonTest = jsonTest.test;

            return localStorageTest && jsonTest;
        } catch(e) {
            return false;
        }
    },
    /**
     * Get local panel content copy if available
     *
     * @return object or null if not available
     */
    getLocalCopy : function(panelName, id)
    {
        if(!panelHandler.hasLocalStorage(panelName, id))
        {
            return null;
        }

        var now = Math.round((+new Date())/1000);
        var maxAge = this.defaultStorageTime;
        var item = localStorage.getItem(panelName + ";" + id);

        if(!item) {
            return null;
        }

        item = JSON.parse(item);

        // Panel can hint about max storage ange in response
        if(item.maxAge > 0)
        {
            maxAge = item.maxAge;
        }

        if((now - item.timestamp) < maxAge)
        {
            return item;
        }

        return null;
    },
    /**
     * Store local copy of panel content.
     *
     * @return boolean
     */
    storeLocalCopy : function(panelName, id, panelData)
    {
        if(!panelHandler.hasLocalStorage(panelName, id))
        {
            return false;
        }

        // Panel can hint that it's not cacheable
        if(panelData.maxAge < 0)
        {
            return false;
        }

        panelData.timestamp = Math.round((+new Date())/1000);
        panelData = JSON.stringify(panelData);
        localStorage.setItem(panelName + ';' + id, panelData);
        return true;
    },
    initUpdateHomepageJobsCarousel : function() {
        if ($('.panelJobsCarousel').length > 0) {
            $('.panelJobsCarousel').jcarousel({
                auto: 5,
                wrap: 'last',
                initCallback: panelHandler.initCarouselCallback
            });
        }
    },
    initCarouselCallback: function(carousel) {
        // Disable autoscrolling if the user clicks the prev or next button.
        carousel.buttonNext.bind('click', function() {
            carousel.startAuto(0);
        });

        carousel.buttonPrev.bind('click', function() {
            carousel.startAuto(0);
        });

        // Pause autoscrolling if the user moves with the cursor over the clip.
        carousel.clip.hover(function() {
            carousel.stopAuto();
        }, function() {
            carousel.startAuto();
        });
    }
};


/* -- Martini-QueryString.js -- */

/**
 * Examples:
 *   qs - provides two functions
 *      qa.get('action') will return any variable attached to ?action=foo in the querystring.
 *      qa.objToQueryString - will return a query string from a key:value object
 *
 */

var qs = {
    parsed : false,
    queryString : {},
    get:function(key) {
        if (!this.parsed) {
            this.init();
        }
        return (typeof this.queryString[key] == 'undefined') ? null : this.queryString[key];
    },
    init:function () {
        this.queryString = this.decode(location.search.replace(/\?/,''));
        this.parsed = true;
    },
    decode: function(data) {
        var pairs = data.split('&');
        var l = pairs.length;

        var parsedData = {};

        for ( var i = 0; i < l; i++) {
            var val = pairs[i].toString();
            if (pairs[i].indexOf("=") == -1) {
                continue;
            }

            var keyval = pairs[i].split( "=" );

            if (parsedData.hasOwnProperty(keyval[0]))  {
                if (parsedData[keyval[0]] instanceof Array) {
                    parsedData[keyval[0]].push(keyval[1]);
                } else {
                    var existingData = parsedData[keyval[0]];
                    parsedData[keyval[0]] = [];
                    parsedData[keyval[0]].push(existingData);
                    parsedData[keyval[0]].push(keyval[1]);
                }
            } else {
                parsedData[ keyval[0] ] = keyval[1];
            }
        }

        return parsedData;
    },
    objToQueryString : function(qs) {
        var pairs = [];
        $.each(qs, function (key, value) {
            // we don't set for invalid values
            if (value == null || value.length < 1) {
                return;
            }
            pairs.push(key+'='+encodeURIComponent(value.toLowerCase().replace('"','')));
        });

        return pairs.join('&');
    }
};


/* -- Martini-Select-Thumbnail-2.js -- */

;(function(isMobileDevice) {
    "use strict";
    var ImageThumbnail2 = {
        imageId : null,
        squareTarget: null,
        rectTarget: null,
        // set some default sizes for the dialog boxes
        defaultDialogWidth: 605,
        defaultDialogHeight: 535,
        defaultDialogWidthPage2: 710,
        defaultDialogHeightPage2: 475,
        thumbnailView: null,
        /**
         * Entrance function to start the process
         * @param  {Element definition}
         * @return {void}
         */
        show: function(el) {
            // get the image if
            var imageId = parseInt($(el).val());

            if (isNaN(imageId) || imageId === 0) {
                return;
                // invalid, so we don't do anything
            }

            // easier to use fields
            if (isMobileDevice) {
                this.defaultDialogWidth = 300;
                this.defaultDialogWidthPage2 = 300;
                this.defaultDialogHeight = 400;
                this.defaultDialogHeightPage2 = 400;
            }

            this.thumbnailView = $(el).attr('data-thumbnail-view');

            // store it
            this.imageId = imageId;

            // set up the targets
            this.rectTarget = $(el).attr('data-thumbnail-rect-target');
            this.squareTarget = $(el).attr('data-thumbnail-square-target');

            // load it
            this.loadTemplate();
        },
        /**
         * Check if we have the template loaded, if not do a get request to fetch it
         * @return {void}
         */
        loadTemplate: function() {
            // is the template pre-laoded into the dom?
            if ($('#image-thumbnail').length === 1) {
                return this.launch();
            }

            // grab it
            $.get('/ws/public/template/ImageThumbnailSelect2/', {}, ImageThumbnail2.load);

        },
        /**
         * Result of the ajax request, appending the content to the Dom and setting it as a dialog box
         * @context AjaxRequest
         * @param  {mixed} data The result of the ajax request
         * @return {void}
         */
        load: function(data) {

            // append it
            var html = data.content;
            $('body').append(html);

            //make it a dialogue box
            $("#image-thumbnail").dialog({
                dialogClass: 'no-close',
                bgiframe: true,
                autoOpen: false,
                height: ImageThumbnail2.defaultDialogHeight,
                width: ImageThumbnail2.defaultDialogWidth,
                title: 'Thumbnail select',
                modal: true,
                beforeClose: ImageThumbnail2.dialogBeforeClose,
                close: ImageThumbnail2.dialogClose
            });

            // launch it
            ImageThumbnail2.launch();
        },
        /**
         * On close, we assign the values to the correct hidden fields
         * We do not need to worry about validation as that is handled in dialogBeforeClose
         * @context jquery
         * @return {void}
         */
        dialogClose: function () {

            var vals = ImageThumbnail2.getFinalValues();

            var rectVal = vals.rect;
            var squareVal = vals.square;

            // wipe out the source and put it back to the default
            $('.imgThumbnailSelect img').each(function (i, e) {
                $(this).attr('src', $(this).attr('data-src'));
            });

            // assign them
            $(ImageThumbnail2.rectTarget).val(rectVal);
            $(ImageThumbnail2.squareTarget).val(squareVal);


            ImageThumbnail2.populateThumbnails(rectVal, squareVal);

            // reset the form
            $('#image-thumbnail-square-error').hide();
            $('#image-thumbnail-rect-error').hide();
            $('#image-thumbnail-square-holder').show();
            $('#image-thumbnail-rect-holder').hide();

            $('#image-thumbnail-rect-selector').html('').removeAttr('style').draggable('destroy');

            // rest the choices
            $('input:radio[name=thumbnail]:checked').attr('checked', false);
            $('#image-thumbnail-rect-value').val('');

        },
        /**
         * Perform some validation and reject the close attempt if both items are not picked
         * @return {bool}
         */
        dialogBeforeClose: function () {
            // check the values
            var squareVal = $('input:radio[name=thumbnail]:checked').val();
            var rectVal = $('#image-thumbnail-rect-value').val();
            // if either is not a valid int, then we reject the close and display the error messages
            if (isNaN(parseInt(squareVal)) || isNaN(parseInt(rectVal))) {
                $('#image-thumbnail-square-error').show();
                $('#image-thumbnail-rect-error').show();
                return false;
            }

            // validation passes
            return true;
        },
        /**
         * Bind the actions to the relevant buttons
         * @return {void}
         */
        bind: function() {
            // scroll in the second page
            $('.imgThumbnailSquare input').unbind('change').change(function (e) {
                e.preventDefault();

                // hide all error messages
                $('#image-thumbnail-square-error').hide();
                $('#image-thumbnail-rect-error').hide();



                // hide the first dib and show the second
                $('#image-thumbnail-square-holder').hide();
                $('#image-thumbnail-rect-holder').show('slide', {direction: 'right'});
                ImageThumbnail2.populateRectChoice();
            });

            // handle a click on the image
            $('.image-thumbnail-square-image').unbind('click').click(function (e) {
                e.preventDefault();

                var id = $(this).attr('data-type');

                // set the correct checjbox to being checked and then execute it
                $('#image-thumbnail-square-thumbnail-'+id).attr('checked', true).change();
            });
            // handle a click on the image
            $('.image-thumbnail-rect-image').unbind('click').click(function (e) {
                e.preventDefault();

                var id = $(this).attr('data-type');

                // set the correct checjbox to being checked and then execute it
                $('#image-thumbnail-rect-thumbnail-'+id).attr('checked', true).change();
            });

            // on the save
            $('#image-thumbnail-rect-save').unbind('click').click(function (e) {
                e.preventDefault();

                // hide the error
                $('#image-thumbnail-rect-error').hide();

                // trigger the close routine
                $('#image-thumbnail').dialog('close');
            });
        },
        /**
         * Launch the dialog window
         * @return {void}
         */
        launch: function() {
            // (re)bind the options
            this.bind();
            // open the dialog
            $("#image-thumbnail").dialog('open');

            // reset it to the default sizes
            $("#image-thumbnail").dialog('option', {height: this.defaultDialogHeight, width: this.defaultDialogWidth, position: 'center'});

            // get current values
            var currentSquareVal = parseInt($(this.rectTarget).val());


            // assign all the new sources
            for (var i = 0; i < 6; i++) {

                // set the image src, backing up the original first
                $('#image-thumbnail-square'+i).attr('data-src', $('#image-thumbnail-square'+i).attr('src'))
                                              .attr('src', '/resources/images/'+this.imageId+'/?type=highlight'+i);


                // and for the squre
                if (i === currentSquareVal) {
                    $('#image-thumbnail-square'+i).attr('checked', 'checked');
                }
            }

        },
        populateRectChoice: function() {
            var _this = this;
            var currentRectVal = parseInt($(this.squareTarget).val());



            // load the image as a thumbnail for size sake
            var bgimage = new Image();
            bgimage.src = '/resources/images/'+this.imageId+'/?type=display';

            // assign an onload function
            bgimage.onload = function () {
                _this.setUpDraggable(this);
            };

            // set up a full image so we can calculate the ratio between the two heights
            var imageDetails = new Image();
            imageDetails.src = '/resources/images/'+this.imageId+'/';
            imageDetails.onload = function () {
                _this.setUpHeight(this);
            };
        },
        setUpHeight : function (img) {
            var h = img.height;
            $('#image-thumbnail-rect-original-height').val(h);
            $('#image-thumbnail-rect-save').val('Save').attr('disabled', false);
        },
        setUpDraggable: function(img) {
            var _this = this;
            // grab the current width and height
            var w = img.width;
            var h = img.height;

            var hInt = parseInt(h);
            var wInt = parseInt(w);
            
            var scaledHeight = (w / 3) * 2;

            // if we do not need to scale anything...
            if (scaledHeight >= h || wInt === hInt) {
                $('#image-thumbnail-rect-selector').html("The full image will be used");
                $('#image-thumbnail-rect-value').val(0);
                return window.setTimeout(function () { $('#image-thumbnail').dialog('close'); }, 1000);

            }
            
            // set an initial value    
            $('#image-thumbnail-rect-value').val(0);

            // resize it to fit the new layout
            $("#image-thumbnail").dialog('option', {height: h+100,
                                                    width: w+50,
                                                    position: 'center'});

            $('#image-thumbnail-rect-current-height').val(hInt);

            // set the baste styles
            var currentOffset = $(this.rectTarget).val();

            if (isNaN(currentOffset)) {
                currentOffset = 0;
            }

            $('#image-thumbnail-rect').css("width", w+"px").css('height', h+"px");
            $('#image-thumbnail-rect-overlay').css("width", w+"px").css('height', h+"px");
            
            // set the backgrounds, etc
            $('#image-thumbnail-rect').css('background-image', 'url(/resources/images/'+this.imageId+'/?type=display)');
            $('#image-thumbnail-rect-overlay').css('background-color', 'rgba(255,0,0,0.5)');

            $('#image-thumbnail-rect-selector').css('background-image', 'url(/resources/images/'+this.imageId+'/?type=display)')
                                               .css('background-repeat', 'none')
                                               .css('width', w+"px")
                                               .css('height', scaledHeight+"px")
                                               .css('cursor', 'move');

            $("#image-thumbnail-rect-selector").draggable({ containment: "#image-thumbnail-rect-overlay",
                                                            scroll: false,
                                                            axis: 'y',
                                                            drag: function (e) {
                                                                var h = $(this).css('top');

                                                                var intH = parseInt(h.replace(/[^0-9]+/, ''));

                                                                // border offset
                                                                intH = (intH + 1);

                                                                // image background for selector
                                                                $(this).css('background-position', '0px -'+intH+'px');

                                                            },
                                                            stop: function(e){
                                                                // get the offset
                                                                var h = $(this).css('top');
                                                                // as an int
                                                                var intH = parseInt(h.replace(/[^0-9]+/, ''));

                                                                // save it
                                                                $('#image-thumbnail-rect-value').val(intH);

                                                                _this.getFinalValues(false, intH);
                                                            } });

        },
        getFinalValues: function (debug, intH) {

            var squareVal = $('input:radio[name=thumbnail]:checked').val();
            var rectVal = $('#image-thumbnail-rect-value').val();
            var origHeight = $('#image-thumbnail-rect-original-height').val();
            var currentHeight = $('#image-thumbnail-rect-current-height').val();

            // get the ratio of the thumbnail to the original
            var ratio = (parseInt(origHeight) / parseInt(currentHeight));

            //ratio = 1 + ratio;

            // and get the new offset from the full size
            rectVal = (parseInt(rectVal) * ratio);


            if (debug === true) {
                $('#image-thumbnail-debug').show();
                $('#image-debug-offset').val(intH);
                $('#image-debug-ratio').val(ratio);
                $('#image-debug-exp').val(rectVal);
                $('#image-debug-orig').val(origHeight);
                $('#image-debug-current').val(currentHeight);
            
                var imageId = this.imageId;
                var base = '/resources/images/'+imageId+'/';
                $('#image-debug-rect').attr('src', base+'?type=mc&htype='+rectVal).show();
            }

            return {rect: rectVal, square: squareVal};

        },
        /**
         * Populate some visible thumbnailswith the new image choices selected
         * @param  {int} rect
         * @param  {int} square
         * @return {void}
         */
        populateThumbnails: function(rect, square) {
            var imageId = this.imageId;
            var target = this.thumbnailView;
            // we don't have a target, so we can bail
            if ($(target).length === 0) {
                return;
            }

            // always show it
            $(target).show();

            // set the base image
            var base = '/resources/images/'+imageId+'/';

            $(target +' .main-image').attr('src', base+'?type=display').removeAttr('width').removeAttr('height');
            $(target +' .main-image-link').attr('href', base);

            $(target +' .square-image').attr('src', base+'?type=article'+square);
            $(target +' .square-image-link').attr('href', base);

            $(target +' .rect-image').attr('src', base+'?type=mc&htype='+rect);
            $(target +' .rect-image-link').attr('href', base);

        }

    };

    // assign the class to the global scope
    window.ImageThumbnail2 = ImageThumbnail2;

    $(document).ready(function () {
        $('.call-image-id-change2').change(function(e) {
            e.preventDefault();
            ImageThumbnail2.show(this);
        });
        // add a seperate onclick handler to force the change
        $('.call-image-id-force-change2').click(function (e){
            e.preventDefault();
            var target = $(this).attr('data-target');
            $(target).change();
        });
    });
})(isMobileDevice);


/* -- Martini-Select-Thumbnail.js -- */

;(function() {
    "use strict";
    var ImageThumbnail = {
        imageId : null,
        squareTarget: null,
        rectTarget: null,
        // set some default sizes for the dialog boxes
        defaultDialogWidth: 605,
        defaultDialogHeight: 535,
        defaultDialogWidthPage2: 710,
        defaultDialogHeightPage2: 475,
        thumbnailView: null,
        /**
         * Entrance function to start the process
         * @param  {Element definition}
         * @return {void}
         */
        show: function(el) {
            // get the image if
            var imageId = parseInt($(el).val());

            if (isNaN(imageId) || imageId === 0) {
                return;
                // invalid, so we don't do anything
            }

            // easier to use fields
            if (isMobileDevice) {
                this.defaultDialogWidth = 300;
                this.defaultDialogWidthPage2 = 300;
                this.defaultDialogHeight = 400;
                this.defaultDialogHeightPage2 = 400;
            }

            this.thumbnailView = $(el).attr('data-thumbnail-view');

            // store it
            this.imageId = imageId;

            // set up the targets
            this.rectTarget = $(el).attr('data-thumbnail-rect-target');
            this.squareTarget = $(el).attr('data-thumbnail-square-target');

            // load it
            this.loadTemplate();
        },
        /**
         * Check if we have the template loaded, if not do a get request to fetch it
         * @return {void}
         */
        loadTemplate: function() {
            // is the template pre-laoded into the dom?
            if ($('#image-thumbnail').length === 1) {
                return this.launch();
            }

            // grab it
            $.get('/ws/public/template/ImageThumbnailSelect/', {}, ImageThumbnail.load);

        },
        /**
         * Result of the ajax request, appending the content to the Dom and setting it as a dialog box
         * @context AjaxRequest
         * @param  {mixed} data The result of the ajax request
         * @return {void}
         */
        load: function(data) {

            // append it
            var html = data.content;
            $('body').append(html);

            //make it a dialogue box
            $("#image-thumbnail").dialog({
                dialogClass: 'no-close',
                bgiframe: true,
                autoOpen: false,
                height: ImageThumbnail.defaultDialogHeight,
                width: ImageThumbnail.defaultDialogWidth,
                title: 'Thumbnail select',
                modal: true,
                beforeClose: ImageThumbnail.dialogBeforeClose,
                close: ImageThumbnail.dialogClose
            });

            // launch it
            ImageThumbnail.launch();
        },
        /**
         * On close, we assign the values to the correct hidden fields
         * We do not need to worry about validation as that is handled in dialogBeforeClose
         * @return {void}
         */
        dialogClose: function () {
            // grab the values
            var squareVal = $('input:radio[name=thumbnail]:checked').val();
            var rectVal = $('input:radio[name=thumbnailrect]:checked').val();

            // wipe out the source and put it back to the default
            $('.imgThumbnailSelect img').each(function (i, e) {
                $(this).attr('src', $(this).attr('data-src'));
            });

            // assign them
            $(ImageThumbnail.rectTarget).val(rectVal);
            $(ImageThumbnail.squareTarget).val(squareVal);


            ImageThumbnail.populateThumbnails(rectVal, squareVal);

            // reset the form
            $('#image-thumbnail-square-error').hide();
            $('#image-thumbnail-rect-error').hide();
            $('#image-thumbnail-square-holder').show();
            $('#image-thumbnail-rect-holder').hide();

            // rest the choices
            $('input:radio[name=thumbnail]:checked').attr('checked', false);
            $('input:radio[name=thumbnailrect]:checked').attr('checked', false);

        },
        /**
         * Perform some validation and reject the close attempt if both items are not picked
         * @return {bool}
         */
        dialogBeforeClose: function () {
            // check the values
            var squareVal = $('input:radio[name=thumbnail]:checked').val();
            var rectVal = $('input:radio[name=thumbnailrect]:checked').val();

            // if either is not a valid int, then we reject the close and display the error messages
            if (isNaN(parseInt(squareVal)) || isNaN(parseInt(rectVal))) {
                $('#image-thumbnail-square-error').show();
                $('#image-thumbnail-rect-error').show();
                return false;
            }

            // validation passes
            return true;
        },
        /**
         * Bind the actions to the relevant buttons
         * @return {void}
         */
        bind: function() {
            // scroll in the second page
            $('.imgThumbnailSquare input').unbind('change').change(function (e) {
                e.preventDefault();

                // hide all error messages
                $('#image-thumbnail-square-error').hide();
                $('#image-thumbnail-rect-error').hide();

                // resize it to fit the new layout
                $("#image-thumbnail").dialog('option', {height: ImageThumbnail.defaultDialogHeightPage2,
                                                        width: ImageThumbnail.defaultDialogWidthPage2,
                                                        position: 'center'});

                // hide the first dib and show the second
                $('#image-thumbnail-square-holder').hide();
                $('#image-thumbnail-rect-holder').show('slide', {direction: 'right'});
            });

            // handle a click on the image
            $('.image-thumbnail-square-image').unbind('click').click(function (e) {
                e.preventDefault();

                var id = $(this).attr('data-type');

                // set the correct checjbox to being checked and then execute it
                $('#image-thumbnail-square-thumbnail-'+id).attr('checked', true).change();
            });
            // handle a click on the image
            $('.image-thumbnail-rect-image').unbind('click').click(function (e) {
                e.preventDefault();

                var id = $(this).attr('data-type');

                // set the correct checjbox to being checked and then execute it
                $('#image-thumbnail-rect-thumbnail-'+id).attr('checked', true).change();
            });

            // on the save
            $('.imgThumbnailRect input').unbind('change').change(function (e) {
                e.preventDefault();

                // hide the error
                $('#image-thumbnail-rect-error').hide();

                // trigger the close routine
                $('#image-thumbnail').dialog('close');
            });
        },
        /**
         * Launch the dialog window
         * @return {void}
         */
        launch: function() {
            // (re)bind the options
            this.bind();
            // open the dialog
            $("#image-thumbnail").dialog('open');

            // reset it to the default sizes
            $("#image-thumbnail").dialog('option', {height: this.defaultDialogHeight, width: this.defaultDialogWidth, position: 'center'});

            // get current values
            var currentSquareVal = parseInt($(this.rectTarget).val());
            var currentRectVal = parseInt($(this.squareTarget).val());

            // assign all the new sources
            for (var i = 0; i < 6; i++) {



                // set the image src, backing up the original first
                $('#image-thumbnail-square'+i).attr('data-src', $('#image-thumbnail-square'+i).attr('src'))
                                              .attr('src', '/resources/images/'+this.imageId+'/?type=highlight'+i);

                $('#image-thumbnail-rect'+i).attr('data-src', $('#image-thumbnail-rect'+i).attr('src'))
                                            .attr('src', '/resources/images/'+this.imageId+'/?type=mc&htype='+i);

                //pre-check if we are editing or already have a value
                if (i === currentRectVal) {
                    $('#image-thumbnail-rect'+i).attr('checked', 'checked');
                }
                // and for the squre
                if (i === currentSquareVal) {
                    $('#image-thumbnail-square'+i).attr('checked', 'checked');
                }
            }
        },
        /**
         * Populate some visible thumbnailswith the new image choices selected
         * @param  {int} rect
         * @param  {int} square
         * @return {void}
         */
        populateThumbnails: function(rect, square) {
            var imageId = this.imageId;
            var target = this.thumbnailView;
            // we don't have a target, so we can bail
            if ($(target).length === 0) {
                return;
            }

            // always show it
            $(target).show();

            // set the base image
            var base = '/resources/images/'+imageId+'/';

            $(target +' .main-image').attr('src', base+'?type=display').removeAttr('width').removeAttr('height');
            $(target +' .main-image-link').attr('href', base);

            $(target +' .square-image').attr('src', base+'?type=article'+square);
            $(target +' .square-image-link').attr('href', base);

            $(target +' .rect-image').attr('src', base+'?type=mc&htype='+rect);
            $(target +' .rect-image-link').attr('href', base);

        }

    };

    // assign the class to the global scope
    window.ImageThumbnail = ImageThumbnail;

    $(document).ready(function () {
        $('.call-image-id-change').change(function(e) {
            e.preventDefault();
            ImageThumbnail.show(this);
        });
        // add a seperate onclick handler to force the change
        $('.call-image-id-force-change').click(function (e){
            e.preventDefault();
            var target = $(this).attr('data-target');
            $(target).change();
        })
    });
})();


/* -- Martini-SiteTree.js -- */

var siteTree = {};

siteTree.openNodes = null;

siteTree.init = function()
{
    // close everything first
    $('.icon-plus-sign').siblings('ul').hide();
    
    
    // assign the onclick handler
    $('#maintree .icon-plus-sign').click( function (e)
    {
        
         e.preventDefault();
         siteTree.handleClick(this);

    });

    // get the currently open cookies
    this.openNodes = $.parseJSON($.cookie('current_nodes'));

    // check we have something valid
    if (this.openNodes === null)
    {
        return;
    }
    
    // iterate through each
    $.each(this.openNodes, function (key, value)
    {
        // get the first child image
        iEl = siteTree.getFirstObject($('#'+key), 'i');

        // activate the click
        try
        {
            iEl.click();
        }
        catch (Err)
        {
        }
    });
};

/**
 * Take a parent, and search through the children for the first (and only) matching dom element
 * @params jQueryDomElement parent
 * @params string type
 */
siteTree.getFirstObject = function(parent, type)
{

    var el = null;
    // get the children and iterate
    parent.children(type).each(function (i, k)
    {
        // get the first module
        if (i === 0)
        {
            el =  $(this);
        }
    })

    return el;
}

/**
 * Hanlde the onClick for an image to open and shut the child tree elements
 * @params HTMLDOMElement el
 */
siteTree.handleClick = function (el)
{
    // get the image that has been clicked
    // and toggle the correct classes
    $(el).toggleClass('icon-plus-sign').toggleClass('icon-minus-sign').siblings('ul').toggle();

    // find it's immediate parent li
    parentLi = $(el).closest('li');

    // get the parents first ul child
    childUl = this.getFirstObject(parentLi, 'ul');
    
    
    // switch the image
    isOpen = childUl.is(':visible');
    
    // ensure we have an
    openNodes = (this.openNodes === null) ? {} : this.openNodes;

    // either set to true, or delete
    if (isOpen)
    {
        openNodes[parentLi.attr('id')] = 1;
    }
    else
    {
        delete openNodes[parentLi.attr('id')];
    }
    // to string
    nodeStr = JSON.stringify(openNodes);
    // save as a cookie
    $.cookie('current_nodes', nodeStr, {expires: 30})

};


/* -- Martini-UATU-recommend.js -- */

/* jshint strict: true, browser: true, devel: true, jquery: true, indent: 4, curly: true */
;(function () {
    'use strict';

    var recommend = {
    	load : function(data) {

            // we don't have anything to show
            if (typeof data.title === 'undefined') {
                $('#uatu-recommendations').hide();
                return;
            }

    		var li = $('#uatu-list li:first-child');

            $('#uatu-title').html(data.title);

            $.each(data.articles, function (r, i) {
                // clone the content
                var el = li.clone().show();

                el.find('a').attr('href', i.url);
                el.find('h2').html(i.headline);

                el.find('img').attr('src', i.imagePath+'?type=mc&htype='+i.rectThumbnailType).attr('alt', i.headline);

                el.removeAttr('style');

                $('#uatu-list').append(el);

            });

            li.remove();

    	}
    };

    window.martiniRecommend = recommend;

})();


/* -- Martini-Updated-Most-Read.js -- */

/* jshint strict: true, browser: true, devel: true, jquery: true, indent: 4, curly: true */
;(function () {
    'use strict';

    var updatedMostRead = {
    	init: function () {
    		// abort if no dom option
    		if ($('#MostRead').length === 0) {
    			return;
    		}

    		$.get("/resources/ph/MostPopularDataPanel/?id=elt_MostPopularDataPanel", {}, updatedMostRead.load);

    	},
    	load : function(data) {

            var limit = $('#MostRead').attr('data-limit');
            var format = $('#MostRead').attr('data-format');

            if (format === 'noimage-highlightfirst') {
    		    var highlight = $('#MostRead li:first-child');
    		    var li = $('#MostRead li:nth-child(2)');
            } else {
    		    var li = $('#MostRead li:first-child');
            }

            // no most read
            if (typeof data.html['Most read'] === 'undefined') {
            	return;
            }

            $.each(data.html['Most read'].data, function (r, i) {
                // filter, we only show 18
                if (r >= limit) {
                    return;
                }
                // clone the content
                if (r === 0 && format === 'noimage-highlightfirst') {
                    var el = highlight.clone().show();
                    el.find('a h4').html(i.headline);
                    var imgType = 'mc1';
                } else {
                    var el = li.clone().show();
                    var imgType = 'mc';
                }

                var thumbType = (typeof i.rectThumbnailType === 'undefined') ? i.thumbnailType : i.rectThumbnailType;

                el.find('a').attr('href', i.link);
                el.find('h2').html(i.headline);
                el.find('h4 a').html(i.headline);
                if (i.imageUri !== '') {

                    el.find('img').attr('src', i.imageUri+'?type='+imgType+'&htype='+thumbType).attr('alt', i.headline);
                }
                else {
                    el.find('img').remove();
                }

                if (format === '3cols') {
                	updatedMostRead.threeCols(el, r);
                } else if (format === 'withAdvertising') {
                	updatedMostRead.withAdvertising(el, r);
                } else if (format === 'noimage' || format === "noimage-highlightfirst") {
                    $('#MostRead').append(el);
                }

            });

            li.remove();

    	},
    	withAdvertising: function(element, currentPos) {
    		if (currentPos === 0) {
    			$('#MostReadAd').before(element);
    		} else {
    			$('#MostReadAd').after(element);
    		}
    	},
    	threeCols: function(element, currentPos) {
    		// append it to the correct column
            var remainder = (currentPos % 3);

            if (remainder === 1){
                $('.mostreadb').append(element);
            }
            else if (remainder === 2) {
                $('.mostreadc').append(element);
            }
            else {
                $('.mostread').append(element);
            }
    	}

    };

    $(document).ready(function () {
    	updatedMostRead.init();
    });

})();


/* -- Martini-UploadVideo.js -- */

// installation notes:
// TO have this on a page, you MUST have a div call video-filelist
// The form must contain the following:
//  submit - a button element, with the id of video-submit.  It MUST NOT be a submit button, as the script witl submit the form afterwards
//  hidden - a hidden form element with the id of video-uploaded-id
//  button - a button element with the id of pickfiles.  This will be used for the file dialog
//  div - an element with the id of container.  This MUST contain the following:
//      div - an element with the id of video-filelist.  On selection, this will show a list of the file(s) available
//
// Changing pickfiles or container simply doesn't work.  The code is fucked.

$(document).ready( function() {

    function getInternetExplorerVersion() {
        var rv = -1; // Return value assumes failure.
        if (navigator.appName == 'Microsoft Internet Explorer') {
            var ua = navigator.userAgent;
            var re = new RegExp("MSIE ([0-9]{1,}[.0-9]{0,})");
            if (re.exec(ua) !== null)
                rv = parseFloat(RegExp.$1);
        }
        return rv;
    }

    var rTimes = 'html5';//,flash,silverlight,html4';

    var ieVersion = getInternetExplorerVersion();

    if (ieVersion > 5 && ieVersion < 9) {
        rTimes = 'flash';
    }

    var uri = escape(document.location.pathname);

    var formId = '';
    if ($('#videouploadform').length === 1) {
        formId = 'videouploadform';
    } else {
        formId = 'form1';
    }

    var uploader = new plupload.Uploader({
        runtimes : rTimes,
        browse_button : 'pickfiles', // you can pass in id...
        //container: $('container'), // ... or DOM Element itself
        max_file_size : '256mb',
        multi_selection: false,
        url : '/ws/public/video/?path='+uri,
        flash_swf_url : '/resources/static/plupload/Moxie.swf',
        silverlight_xap_url : '/resources/static/plupload/Moxie.xap',
        filters : [

            {title : "Video files", extensions : "mp4,flv,3g2,3gp,asf,avi,dv,mov,mpeg,mpg,qt,wmv,m4v"}

        ],
        init: {
            PostInit: function() {
                // clear the video file list
                $('#video-filelist').html('');

                $('#uploadfiles').click(function(e) {
                    e.preventDefault();
                    uploader.start();
                });
            },

            FilesAdded: function(up, files) {
                $.each(files, function(i, file) {

                    var fileDisplay = $('<div></div>').attr('id', file.id);
                    var percentageHolder = $('<b></b>');
                    var fileInfo = file.name + ' ('+plupload.formatSize(file.size)+') ';

                    var remove = $('<a></a>').attr('id', 'cancellink').html(' (remove)').attr('href', '#').click(function (e) {
                        e.preventDefault();

                        $('#'+file.id).remove();
                        $('#pickfiles').show();
                        $('#container p.help-block').show();
                    });

                    var listEl = fileDisplay.append(fileInfo, percentageHolder, remove);

                    $('#video-filelist').append(listEl);
                });

                $('#pickfiles').hide();
                $('#container p.help-block').hide();

            },

            BeforeUpload: function(up, file) {
                $('#cancellink').remove();

                var progressContainer = $('<div></div>').addClass('progress progress-striped active');
                var progressBar = $('<div></div>').addClass('bar').attr('style', 'width: 0px;').attr('id', 'progressBar');


                progressContainer.append(progressBar);

                $('#'+file.id).after(progressContainer);
            },

            UploadProgress: function(up, file) {

                $('#' + file.id + " b").html(file.percent + "%");
                $('#progressBar').attr('style', 'width: '+file.percent + "%");

                if (file.percent === 100) {
                    $('#' + file.id + " b").html("Processing on the server");
                }
            },

            Error: function(up, err) {
                $('#video-submit').attr('disabled', false);

                var errorMsg = $('<div></div>').html('Error: '+err.message);

                $('#video-filelist').append(errorMsg);

                $('#pickfiles').show();
                $('#container .help-block').show();
            },

            FileUploaded: function(a, b, jsonresponse) {
                // get the response
                var resp = $.parseJSON(jsonresponse.response);
                $('#video-uploaded-id').val(resp.queueId);
                $('#video-submit').attr('disabled', false);
                $('#video-filelist').html("File uploaded");
                $('#video-browse').remove();
                $('#'+formId).submit();
            }
        }
    });

    uploader.init();

    $('.facebookSelect a').click(function(){
        $('#image'+$(this).parent().attr('id').substr(8)).val('');
        window.open('/my/facebookgallerydisplay/?image='+$(this).parent().attr('id').substr(8), '_blank', 'height=650,width=720,resizable=no,menubar=no,status=no');
        return false;
    });


    $('#article_id').change(function () {

        var articleId = parseInt($(this).val());

        if (isNaN(articleId))
        {
            articleMsg(this, 'Invalid article id', 'alert-error');
            $(this).val('');
            return;
        }

        var _this = this;

        $('#video-submit').attr('disabled', 'disabled');

        articleMsg(this, 'Validating article id', 'alert-warning');

        $.ajax({
            url: '/ws/article/'+articleId+'/uri/',
            success: function (){articleMsg(_this, 'Validated', 'alert-success'); } ,
            error: function (){articleMsg(_this, 'Invalid article', 'alert-error'); $(_this).val(''); } 
        });

    });

    function articleMsg(el, html, alertClass)
    {
        var errEl = $('<div></div>').addClass('alert').html(html).addClass(alertClass);
        $(el).after(errEl);
            
        window.setTimeout(function () { $('.alert').fadeOut();}, 5000);

        $('#video-submit').attr('disabled', false);

    }

    // bind the submit to actually do it
    $('#video-submit').click(function(e) {

        // we don't do anythign with the click
        e.preventDefault();

        var mandatoryArticleId = ($(this).attr('data-mandatory-article-id') === 'true');

        if (!videoUploadFormValidate(mandatoryArticleId)) {

            window.setTimeout(function () { $('.alert').fadeOut();}, 5000);

            return false;
        }

        // check if we have anything in the queue
        var len = uploader.files.length;

        // if there are no video files, or it is complete.
        if (uploader.files[0].status == 5) {
            return $('#'+formId).submit();
        }

        // disable the start button so it can't be clicked again
        $(this).attr('disabled', 'disabled');

        // start the uploader, which will then submit the form on success
        uploader.start();

    });

    var videoUploadFormValidate = function (mandatoryArticleId) {


        var validFields = {movie_desc: 'description', movie_title: 'title', movie_name: 'name'};

        if (mandatoryArticleId)
        {
            validFields.article_id = 'Related article ID';
        }
        var minimumFields = {movie_title: 30, movie_desc: 105};
        var errEl = $('<div></div>').addClass('alert alert-error');

        var valid = true;

        // check for a video
        if (uploader.files.length === 0 && $('#video-uploaded-id').val() === '') {

            var localErr = errEl.clone();

            $('#container').before(localErr.html('You must upload a video'));

            valid = false;
        }

        // check for the raw content being present
        $.each(validFields, function(key, error) {
            if ($('#'+key).val() === '') {
                var localErr = errEl.clone();
                $('#'+key).before(localErr.html('You must enter a '+error));

                valid = false;
            }
        });

        // check that limits aren't over shot
        $.each(minimumFields, function(key, limit) {
            if ($('#'+key).val().length > limit) {
                var localErr = errEl.clone();
                $('#'+key).before(localEl.html('You have entered more than the '+limit+' limit'));
                valid = false;
            }
        });

        return valid;

    };

});

/** PL upload replacement.  Uses direct xhr
    got as far as progress bar before fix found.
    Storing here in case we need it again the future
var videoUpload = {
    init: function() {
        $('#pickfiles2').change(function(e){
                videoUpload.upload(e, this);
        });

    },
    upload: function (e, el) {
        // prevent anything from bubbling up
        e.preventDefault();

        // the path uploaded from
        var uri = escape(document.location.pathname);

        // extract the file
        var files = el.files;

        var file = files[0];

        // and the file data
        var fileName = file.name;


        var url = '/ws/public/video/?path='+uri;

        // use the formdata api
        var formData = new FormData();

        formData.append('file', file);

        // we're about to send
        this.beforeSend();

        var xhr = new XMLHttpRequest();
        xhr.open('POST', url, true);
        xhr.onload = this.videoSuccess;
        xhr.upload.addEventListener("progress", videoUpload.progressBar, false);
        xhr.onerror = this.videoError;

        xhr.send(formData);

    },
    progressBar: function(progress) {
        // calculate upload progress
        var percentage = Math.floor((progress.position / progress.totalSize) * 100);
        console.log(percentage, progress.position, progress.totalSize);
        $('#progressBar').attr('style', 'width: '+percentage + "%; color: black").html(percentage+'%');

        if (percentage === 100) {
            $('#progressBarHolder').html("Processing on the server");
        }
    },
    beforeSend: function() {
        console.log('showing div');
        $('#progressBarHolder').show();
        $('#pickfiles2').hide();
    },
    videoSuccess: function(data) {
        $('#video-uploaded-id').val(data.queueId);
        $('#progressBarHolder').html("File uploaded");
        $('#video-browse').remove();
        $('#'+formId).submit();
    },
    videoError: function(data, a, b, c) {
        console.log(data, a, b, c);
    }
};
videoUpload.init();
*/


/* -- Martini-dropit.js -- */


/* ========================================================================
 * Martini Dropit
 * for drop down boxes
 * Modified version of
 * Bootstrap: dropdown.js v3.1.0 http://getbootstrap.com/javascript/#dropdowns
 * ======================================================================== */

(function($) {
    'use strict';

    var backdrop = '.dropdown-backdrop'
    var dodrop = '[data-drop-trigger]';
    var Dropit = function (el) {
        $(el).on('click', dodrop, this.toggle)
    };


    Dropit.prototype.toggle = function (e) {
        var $this    = $(this);
        var $parent = $this.parent();
        var isActive = $parent.hasClass('dropIt');

        //e.preventDefault();

        clearMenus();


        if (!isActive) {
            if ('ontouchstart' in document.documentElement) {
              // if mobile we use a backdrop because click events don't delegate
              $('<div class="dropdown-backdrop"/>').insertAfter($this).on('click', clearMenus)
            }

            if (e.isDefaultPrevented()) return;

            $parent.toggleClass("dropIt");
            $this.focus();
            $parent.trigger('dropItShow');
        }

        return false;

    }

    Dropit.prototype.keydown = function (e) {
      if (!/(38|40|27)/.test(e.keyCode)) return

      var $this = $(this)

      e.preventDefault()
      e.stopPropagation()

      var $parent  = $this.parent()
      var isActive = $parent.hasClass('open')

      if (!isActive || (isActive && e.keyCode == 27)) {
        if (e.which == 27) $parent.find(dodrop).focus()
        return $this.click()
      }
    }

    function clearMenus(e) {
      $(backdrop).remove()
      $(dodrop).each(function () {
        var $this = $(this);
        var $parent = $this.parent();
        if (!$parent.hasClass('dropIt')) return;
        $parent.removeClass('dropIt');
        $parent.trigger('dropItHide');
      })
    }

    $.fn.dropit = function(options) {

        return this.each(function() {

            var $this = $(this);
            var data  = $this.data('martini.dropit')

            if (!data) $this.data('martini.dropit', (data = new Dropit(this)))
            if (typeof option == 'string') data[option].call($this)

            //$this.on('click', drop(e))
        });
    };

    $.fn.dropit.Constructor = Dropit

    //$(document).on('click.martini.dropit.data-api', dodrop, Dropit.prototype.toggle)
    $(document)
    .on('click.martini.dropit.data-api', clearMenus)
    .on('click.martini.dropit.data-api', '.dropPanel form', function (e) { e.stopPropagation() })
    .on('click.martini.dropit.data-api', dodrop, Dropit.prototype.toggle)
    .on('keydown.martini.dropit.data-api', dodrop, Dropit.prototype.keydown)


}(jQuery));


/* -- Martini-fotorama.js -- */

/* fotorama gallery */

;(function() {
    "use strict";

    var MartiniFotorama = {
        isFullScreen: false,
        advertLoaded: false,
        targetId : null,
        baseUrl : '',
        initialLoad : true,
        init: function() {
            // does it exist?
            if( $('.fotorama').length === 0){
                return;
            }
            // remove the initial image
            $('#seoimage-fotorama').hide().remove();


            // set up a laod handler before we load it
            this.handleLoad();

            if ($('#gallery-base-url').length === 1) {
                this.baseUrl = $('#gallery-base-url').val();
            }


            // initiliase the div
            $('.fotorama').fotorama();

            // set some actions for when a new image is selected from either the carousel or the arrows
            this.handleNewImageShown();

            // set some actions for entering and exiting a full screen
            this.handleFullScreen();

            // this appears to no longer be necessary
            //this.addArrowScrolling();
        },
        handleLoad: function() {
            $('.fotorama').on('fotorama:ready', function (e, fotorama) {
                if (MartiniFotorama.targetId === null) {
                    return;
                }
                fotorama.show(MartiniFotorama.targetId);
            });
        },
        handleNewImageShown: function() {
            $('.fotorama').on('fotorama:showend', function (e, fotorama, extra) {
                if (MartiniFotorama.isFullScreen) {
                    //console.log('refreshing full screen advert');
                    //googletag.pubads().refresh([dfpHolder.]);

                } else {
                    //console.log('refreshing mpu advert');
                    //googletag.pubads().refresh([dfpHolder.DFP_premium_mpu]);
                }
                if (MartiniFotorama.supportHistory() && MartiniFotorama.baseUrl !== '') {

                    // only trigger on subsequent loads - not on the first, which is the same as page-load
                    if (MartiniFotorama.initialLoad === true) {
                        MartiniFotorama.initialLoad = false;
                        return;
                    }
                    

                    var url = MartiniFotorama.baseUrl + fotorama.activeIndex +'/';

                    history.pushState(this.stateObj, 'Page Title', url);

                    // obfuscated code call for omniture
                    s.t();
                } else if (MartiniFotorama.baseUrl !== '') {
                    // no push state but still upate omniture
                    s.t();
                }
            });
        },
        /**
         * Set up the hidingf of thuimbnails and instructions on entering or exiting the full screen
         */
        handleFullScreen: function() {
            // register a document handler for the events charged by fotorama
            $(document).on('fotorama:fullscreenenter fotorama:fullscreenexit', function (e, fotorama) {

                // set a boolean for if we are entering of exiting a full screen
                MartiniFotorama.isFullScreen = (e.type === 'fotorama:fullscreenenter');

                if (MartiniFotorama.isFullScreen && MartiniFotorama.advertLoaded === false) {
                    googletag.display('DFP_gallery_leaderboard');
                    MartiniFotorama.advertLoaded = true;
                }

                // we show or hide the nav here - false or "thumbs" is passed
                fotorama.setOptions({nav: e.type === 'fotorama:fullscreenexit' && 'thumbs'});
                // hide or show the various classes - will start with shown, so will toggle to hide on entering full screen
                // being extremely explicit to avoid a confusion bug
                if (e.type === 'fotorama:fullscreenenter') {
                    $('.sponsorreload').show();
                } else {
                    $('.sponsorreload').hide();
                }


                // hide the instructions
                $('.fotorama__instructions').toggle();

            });
        },
        goTo: function(id) {
            id = parseInt(id);

            // not a valid id, abort
            if (isNaN(id)) {
                return;
            }


            this.targetId = 'slide'+id;
        },
        /**
         * Make the arrows work
         */
        addArrowScrolling: function() {

            // we don't have any to scroll, so we can bale
            if($('.fotorama__nav-wrap').length === 0) {
                return;
            }

            // we don't have either button, we can abort as well
            if( $('.fotorama__navprev').length === 0 || $('.fotorama__navnext').length === 0 ) {
                return;
            }


            // get the arrows
            var navPrev = $('.fotorama__navprev');
            var navNext = $('.fotorama__navnext');

            // get the dome items for the navigation
            var navWrap = $('.fotorama__nav-wrap');
            var navShaft = $('.fotorama__nav__shaft');
            var navThumbs = $('.fotorama__nav__shaft').children('div');

            // caculate the widths
            var navShaftWidth = navShaft.outerWidth();
            var navWrapWidth = navWrap.outerWidth();
            var navThumbWidth = navThumbs.outerWidth();

            // we don't have enough content, so get rid of the buttons
            if( navShaftWidth <= navWrapWidth ) {
                // remove the divs from displaying at all
                $('.fotorama__navprev').hide();
                $('.fotorama__navnext').hide();
                return;
            }

            // wwork out how many steps (thumbnails in view) we have
            var steps = Math.floor(navShaftWidth/navWrapWidth);

            // how many thumbs that is
            var thumbsPerWrap = Math.floor(navWrapWidth/navThumbWidth);

            // next step
            var increment = thumbsPerWrap*navThumbWidth;

            // where we were last
            var lastPosition = -( (steps)*increment);

            // what we have left
            var remainder = navShaftWidth - (steps*thumbsPerWrap*navThumbWidth);

            // set up click handlers to move the main shaft
            navPrev.on('click', function (event) {
                event.preventDefault();
                MartiniFotorama.fotoramaApi.moveShaft(increment, lastPosition, remainder);
            });

            navNext.on('click', function (event) {
                event.preventDefault();
                MartiniFotorama.fotoramaApi.moveShaft('-'+increment, lastPosition, remainder);
            });

        },
        /**
         * Check if the current browser supports url mangling
         */
        supportHistory: function() {
            // check if we support the necessary items
            if (Boolean(window.history && window.history.pushState && window.history.replaceState)) {
                return true;
            }

            return false;
        }


    };

    window.MartiniFotorama = MartiniFotorama;
})();
$(document).ready(function() {
    MartiniFotorama.init();
});


/* -- Martini-timestamps.js -- */

;(function() {
    "use strict";


    var tsToDelta = {
        isHomePage: function () {
            return (document.location.pathname == '/' || document.location.pathname == '/newhomepage/');
        },
        init: function() {
            this.updateDom();

            // call ourselves again in a minute
            window.setTimeout(function() { tsToDelta.init(); }, 60000);
        },
        // update the dom with the nwe timestamp
        updateDom: function() {
            $('.formatTimeStamp').each(function (e) { tsToDelta.updateFormats(this); });
        },
        // get the current timestamp and format it the way we want
        updateFormats: function(el) {

            // get the timestamp
            var ts = $(el).attr('data-timestamp');
            var format = $(el).attr('data-format');

            var showTime = ($(el).attr('data-show-time') === 'true');
            var showTimeToday = ($(el).attr('data-show-time') === 'today-only');

            // if we want to shwo time today, we override the showTime variable
            if (showTimeToday) {
                showTime = this.showTimeForToday(showTime, ts);
            }

            ts = parseInt(ts);

            var dateStr = '';

            // try and format it
            try {
                var diff = this.calcDelta(ts);
                // pluralise it
                if (diff[0] !== 1) {
                    diff[1] += 's';
                }
                // build the date string
                dateStr = diff[0]+' '+diff[1]+' ago';
            }
            catch (err) {
                dateStr = this.formatDate(ts, showTime, format);
            }

            // if we have no string AND we have nothing else in the dom, we can delete the parent
            if (dateStr === '' && $(el).siblings().length === 0) {
                $(el).parent().remove();
            } // else we just delete the time portion
            else if (dateStr === '') {
                var p = $(el).parent();
                $(el).remove();

                p.html(p.html().replace('in', 'from')); 
            }

            $(el).html(dateStr);

        },
        calcDelta: function(ts) {
            // grab the current time (which is in milliseconds in js)
            var currentTime = Math.round(+new Date()/1000);

            // get the delta
            var delta = parseInt(currentTime - ts);

            // seconds
            if (delta < 60) {
                return [delta, 'second'];
            }

            // minutes
            delta = parseInt(delta / 60);

            if (delta < 60) {
                return [delta, 'minute'];
            }

            // hours
            delta = parseInt(delta / 60);

            if (delta < 6) {
                return [delta, 'hour'];
            }

            // // days
            // delta = parseInt(delta / 24);

            // // Days
            // if (delta < 7) {
            //     return [delta, 'day'];
            // }

            throw new Error("too old");
        },
        formatDate: function(ts, showTime, format) {

            var dateString = '';
            switch(format)
            {
            case 'homepage':
                return '';
            case 'article-display':
                dateString = this.getDateString(ts, false);
                break;
            default:
                dateString = this.getDateString(ts, true);
                break;
            }

            var timeString = '';

            if (showTime) {
                var date = new Date(ts*1000);
                var mins = date.getMinutes();
                if (mins < 10) {
                    mins = "0"+mins;
                }
                var hours = date.getHours();
                if (hours < 10) {
                    hours = "0"+hours;
                }
                timeString = hours + ':' + mins + ' ';
            }

            return timeString + dateString;
        },
        showTimeForToday: function(showTime, ts) {

            var today = new Date();
            var todayDate = today.getFullYear().toString()+' '+today.getMonth().toString()+' '+today.getDate().toString();

            var posted = new Date(ts * 1000);
            var postedDate = posted.getFullYear().toString()+' '+posted.getMonth().toString()+' '+posted.getDate().toString();
            if (postedDate === todayDate) {
                showTime = true;
            }

            return showTime;
        },
        getDateString: function(ts, shortTs) {
            // change ts to milliseconds
            var date = new Date(ts*1000);

            var monthNames = [ "January", "February", "March", "April", "May", "June", "July", "August", "September", "October", "November", "December" ];
            var dayNames = ["Sunday", "Monday", 'Tuesday', "Wednesday", "Thursday", "Friday", "Saturday"];

            if (shortTs === true) {
                var shortMonth = [];
                var shortDay = [];
                $.each(monthNames, function (i, name) {
                    shortMonth[i] = name.substring(0, 3);
                });

                $.each(dayNames, function (i, name) {
                    shortDay[i] = name.substring(0, 3);
                });

                monthNames = shortMonth;
                dayNames = shortDay;
            }

            // build a date string
            var dateString = dayNames[date.getDay()] + ' ' + date.getDate() + ' ' + monthNames[date.getMonth()];
            if (!shortTs) {
                dateString += ' ' + date.getFullYear();
            }

            return dateString;
        }

    };

    // start it up when the dom is ready
    $(document).ready(function () {
        tsToDelta.init();
    });
})();


/* -- Martini.PromoCarousel.js -- */

var PromoCarousel = {

    // start position for the carousel
    currentPosition: 0,

    // store the maaximum possible
    maxPositions: 3,
    
    // holds the setTimeout reference
    promoTimeout : null,

    // css reference for the identifiers
    pattern: ".promoStrip p",
    linkPattern: ".promoStrip p a",
    holder: ".promoStrip",

    init: function () {

        // if we don't have any
        this.maxPositions = $(this.pattern).length;

        // we have none at all.
        if (this.maxPositions === 0 || $.cookie('seenPromo') === '1') {
            return;
        }
        
        this.attachHandlers();
       
        this.checkHeight();

        // hide all initial items
        $(this.pattern).hide();
        $(this.pattern).first().show();

        window.setTimeout('$(\''+this.holder+'\').slideDown(\'slow\');', 3000);
        this.start();

    },

    attachHandlers : function() {
        
        // Close button
        $(this.holder+' .closeButton').on('click', function(event){
            event.preventDefault();
            $(this).parent(PromoCarousel.holder).slideUp('slow'); 
            $.cookie('seenPromo', '1', {expires: 1, 'path': '/'});
        });  
        
        $(this.pattern).mouseover(function (e) {
            clearTimeout(PromoCarousel.promoTimeout);
        }).mouseout(function (e) {
            PromoCarousel.start();
        });
    },
    checkHeight: function () {
        var height = "40px";

        // check the elements with links
        $(this.linkPattern).each(function (a, el) {
            // see if the html is longer than 150 chars
            if ($(el).html().length > 160) {
                height = "60px";
            }

            // mark this as checked
            $(el).parent().attr('data-checked', 1);
        });

        // check the elements with no links
        $(this.pattern).each(function (a, el) {
            // has this item been checked?
            if ($(el).attr('data-checked') == 1) {
                return;
            }
            // check it
            if ($(el).html().length > 160) {
                height = "60px";
            }
        });
        
        // set the new height
        $(this.holder).css('height', height);

    },
    start: function () {
        this.promoTimeout = window.setTimeout('PromoCarousel.rotatePromo()', 5000);
    },
    incrementCurrentPosition: function() {
        i = this.currentPosition;
        i++;
        if (i == this.maxPositions) {
            i = 0;
        }
        this.currentPosition = i;
    },
    // hide all the promos 
    rotatePromo : function() {
        
        i = this.currentPosition;
        this.incrementCurrentPosition();
        
        if ($(this.pattern).length === 1) {
            return;
        }

        $(this.pattern).hide();

        var el = $(this.pattern).get(i);

        $(el).slideDown('slow');

        this.start();
    }
}


/* -- Martini.js -- */

var martini = {
    init: function()
    {
        this.attachClearOnFocus();
        this.attachClearOnSubmit();
        this.attachDatePicker();
        this.attachTextLimit();
        this.addOriginatorToRegistrationForms();
        this.loadBisDir();
        this.checkCookieSupport();
        this.redirectToMobile();
        PromoCarousel.init();
        // set up article bump and delete
        $('.inlineBump').click (function(e) { e.preventDefault(); martini.bumpArticle(this); });
        $('.inlineDelete').click (function(e) { e.preventDefault(); martini.deleteArticle(this); });

        $('.loginModalRequest').click (function (e) { e.preventDefault(); martini.createModalLoginBox(); });
        $('.loginModalRequestCommentSort').click (function (e) { e.preventDefault(); martini.createModalLoginBox('comment-sort'); });
        $('.loginModalRequestCommentAlerts').click (function (e) { e.preventDefault(); martini.createModalLoginBox('comment-alerts'); });
    },
    addOriginatorToRegistrationForms: function() {

        var url = encodeURIComponent(location.href);
        var newAction = "/my/account/register/step1/?originatingUrl="+url;

        $('form[action="/my/account/register/step1/"]').attr('action', newAction);
        $('a[href="/my/account/register/step1/"]').attr('href', newAction);
    },
    attachDatePicker: function()
    {
        $('.datepicker').each(function ()
        {
            $(this).datepicker({"dateFormat": 'dd-M-yy'});

            if ($(this).is('.futureonly'))
            {
                $(this).datepicker('option', 'minDate', new Date());
            }
        });

        $('.datetimepicker').each(function ()
        {
            $(this).datetimepicker({"dateFormat": 'dd-M-yy'});

            if ($(this).is('.futureonly'))
            {
                $(this).datetimepicker('option', 'minDate', new Date());
            }
        });
    },
    attachTextLimit: function()
    {
        $('textarea').each( function (a, b) { martini.setTextLimit(this); } );
        $('input').each( function (a, b) { martini.setTextLimit(this); } );
    },
    setTextLimit: function(ele)
    {
        f = $(ele);

        i = parseInt(f.attr('limit'));

        if ( i < 1 || isNaN(i))
        {
            return;
        }

        f.countable({maxLength: i});
    },
    attachClearOnFocus: function()
    {
        $('.clearOnFocus').unbind('focus').each(function ()
        {
            $(this).data('v', $(this).val());
            $(this).focus( function ()
            {
                if ($(this).data('v') === $(this).val())
                {
                    $(this).val('');
                }
            } );
        });
    },

    attachClearOnSubmit: function()
    {
        $('form').submit( function ()
        {
            $('.clearOnFocus').each( function ()
            {
                if ($(this).data('v') === $(this).val())
                {
                    $(this).val('');
                }
            } );

        });
        return true;
    },
    attachABTestCookies: function()
    {
        // set the initial
        this.updateABTestDom($.cookie('abVariant'));

        $('.setABCookie').click(function(e)
        {
            e.preventDefault();
            value = $(this).attr('href');
            $.cookie('abVariant', value, {expires: 40, 'path': '/'});
            martini.updateABTestDom(value);
        });
    },
    updateABTestDom: function(b)
    {
        var html = (b == 1) ? 'New content' : 'Original content';
        $('#currentABStatus').html(html);

    },
    loadBisDir: function()
    {
        if ($('#businessdircontext').length < 1)
        {
            return;
        }

        context = $('#businessdircontext').html();

        if (context.length < 1 || context === 'Unknown')
        {
            url = '/trade_directory/supers/5';
        }
        else
        {
            url = '/trade_directory/context/'+context;
        }

        $.ajax(
            {
                type: 'GET',
                dataType: 'json',
                url: url,
                success: function(e) {
                context = $('#businessdircontext').html();
                // if we have no results and we have a valid context, load it again
                // with no context, activating the random
                if (e == null && context != '' && context != 'Unknown')
                {
                    // clear the context from the dom
                    $('#businessdircontext').html('');
                    martini.loadBisDir();
                    return;
                }

                $.each(e, function(g, f)
                {
                    category = f.categories.shift();

                    subcon = $('<li></li>');

                    // create the title
                    titleEl = $('<h5></h5>');
                    url = '/trade_directory/advert/'+category.tokenisedname+'/'+f.id+'/'+encodeURI(f.serviceName)+'?ref=ft';
                    titleLink = titleEl.append($('<a></a>').attr('href', url).html(f.serviceName));

                    // create the logo
                    if (f.logoId !== null)
                    {
                        logoUrl = '/resources/images/'+f.logoId+'/';
                        logoImg = $('<img />').attr('src', logoUrl).attr('alt', f.serviceName).attr('width', 60);
                        logoLink = $('<a></a>').attr('href', url);
                        logoLine = $('<p></p>').addClass('bizColLogo');
                        logoLink.append(logoImg);
                        logoLine.append(logoLink);
                        subcon.append(logoLine);
                    }

                    // create the more link
                    moreLink = $('<a></a>').attr('href', url).html('View details Â»');
                    moreLine = $('<p></p>').addClass('bizColDetails').append(moreLink);

                    // create the category
                    catLinkUrl = '/trade_directory/search/category/'+category.tokenisedname+'?ref=ft';
                    catLink = $('<a></a>').attr('href', catLinkUrl).html('More '+category.displayname);
                    cat = $('<p></p>').addClass('bizColMore').append(catLink);

                    subcon.append(titleLink);
                    subcon.append(moreLine);
                    subcon.append(cat);

                    $('#bizdir_content').prepend(subcon);
                });
            },
            error: function (a, b, c) {$('#businessdircontext').remove();}
        });

    },
    createGMTDate : function()
    {
        var tdate = new Date();
        var today = new Date(tdate.toGMTString());
        var w = today.getDay();
        var m = today.getMonth();
        var y = today.getFullYear();
        var d = today.getDate();
        var day = new Array('Sunday','Monday','Tuesday','Wednesday','Thursday','Friday','Saturday');
        var month = new Array('January','February','March','April','May','June','July','August','September','October','November','December');

        // get a textural representation of the month
        m = month[m];
        w = day[w];

        // format a date string
        var newDate = w + ', ' + d + ' ' + m + ' ' + y;

        // write it to the correct id
        $('#site-time').html(newDate);
    },
    handleVote: function(voteId)
    {
        var checkedVal = null;
        if ($("#m_"+voteId).val() == 1)
        {

            checkedVal = $("#f"+voteId+" input[type='checkbox']:checked").each( function (i,v) {  } );
            checked = [];
            for (i=0;i<5;i++)
            {
                checked[i] = $(checkedVal[i]).val();
            }
            checkedVal = checked.join(',');
        }
        else
        {
            checkedVal = $("#f"+voteId+" input[type='radio']:checked").val();
        }

        var uInfo = BrowserOptions.getOptions();
        uInfo.vid = voteId;
        uInfo.option = checkedVal;

        $.ajax(
        {
            type: 'POST',
            url: '/resources/ballot/?id=vote_'+voteId+'&ballot_id='+voteId,
            data: uInfo,
            success: function (string)
                     {

                        // split it into the id and the html
                        var strings = string.split('|||');

                        var id = strings[0];
                        var html = strings[1];
                        // set the id to the html
                        $('#'+id).html(html);
                    }
        });
    },
    nextFacebook: function(e)
    {
        if(typeof e == 'object' && typeof e.preventDefault == 'function')
        {
            e.preventDefault();
        }
        var url = '';
        if (qs.get('originatingUrl')) {
            url = qs.get('originatingUrl');
        } else {
            url = encodeURIComponent(location.href);
        }
        window.open('/my/facebook/login/?originatingUrl='+url, '_blank', 'height=400,width=680px,location=no,menubar=no,resizable=yes,scrollbars=yes,status=no,titlebar=no');
    },
    nextGoogle: function(e)
    {
        if (typeof e == 'object' && typeof e.preventDefault == 'function')
        {
            e.preventDefault();
        }
        var url = '';
        if (qs.get('originatingUrl')) {
            url = qs.get('originatingUrl');
        } else {
            url = encodeURIComponent(location.href);
        }
        window.open('/my/openId/?provider=google&originatingUrl='+url, '_blank', 'height=400,width=880px,location=no,menubar=no,resizable=yes,scrollbars=no,status=no,titlebar=no');
    },

    bumpArticle : function(ele) {
        var articleId = $(ele).attr('data-id');
        var url = '/ws/article/'+articleId+'/bump/';

        $.get(url, {type : 'json'}, function(data) {
            var article = data[0];
            var articleId = article.id;
            var div = $('#article_'+articleId);
            $('#article_'+articleId).remove();
            $('#trafficupdatesinarticle').after(div);
        });
    },
    deleteArticle: function(ele) {
        var articleId = $(ele).attr('data-id');
        var url = '/ws/article/'+articleId+'/delete/';
        if (confirm("Are you sure you want to delete this article?")) {
            $.get(url, {}, function() {
                $('#article_'+articleId).remove();
            });
        }
    },
    redirectToMobile: function() {
        var cookieName = 'displayPreference';
        $('#mobileSiteLink').click(function (e) {
            e.preventDefault();
            var cookieDomain = $(this).attr('data-cookie-domain');
            $.cookie(cookieName, '', {expires: -1, domain: cookieDomain});
            var url = $(this).attr('href');

            document.location = url;
        });
    },
    displayCookieWarning: function()
    {
        // check if they have seen this warning already
        if ($.cookie('nqreu') == 'warned') {
            return;
        }

        // bring in the warning page
        $('#cookiewarning,#cookiewarningiframe').animate({'right':'0px'},300);

        $('#cookieclose').click(function (e) {
            e.preventDefault();
            $('#cookiewarning,#cookiewarningiframe').stop(true).animate({'right':'-430px'},100);
            $('#cookiewarning,#cookiewarningiframe').remove();
        });

        // set a cookie saying it's been showed.
        $.cookie('nqreu', 'warned', {path: '/', expires: 2000});
    },
    displayUserWarning: function(message)
    {
        if(document.location.pathname == "/my/account/termsandconditions/")
        {
            return;
        }

        var html = $('<div class="modal fade"><div class="modal-dialog"><div class="modal-content"><div class="modal-body"></div><div class="modal-footer"><button type="button" class="btn btn-default">Close</button></div></div></div></div>');
        if (message) {
            $(".modal-body", html).append('<p>' + message + '</p>');
        }
        $(".modal-body", html).append('<label><input type="checkbox" id="tcsConfirm" /> I confirm I have read and understood the message</label>');

        $("a", html).attr("target", "_blank");

        $(html).modal({
            backdrop: 'static'
        });

        $("button", html).on('click', function (e) {
            if ($("#tcsConfirm", html).is(":checked")) {
                $.get('/my/warningAcknowledge/?' + (new Date().getTime()), {}, function() {}, 'json');
            }

            $(html).modal('hide');
        })
    },
    checkCookieSupport: function () {
        // Internet Explorter LIES about navigation.cookieEnabled
        // var cookieEnabled = (navigator.cookieEnabled) ? true : false

        // set a cookie
        document.cookie = "testcookie=1";
        cookieEnabled = (document.cookie.indexOf("testcookie") != -1) ? true : false;

        // delete the cookie
        document.cookie = "testcookie=;expires=Thu, 01 Jan 1970 00:00:01 GMT";
        return cookieEnabled;
    },
    createModalRegisterBox: function () {
        var pathArray = window.location.pathname.split( '/' );
        var is_show_modal = false;
        switch (pathArray[1]) {
            case "news":
                var section = "news";
                is_show_modal = true;
                var registerModalContent = $('<div class="modal fade"><div class="modal-dialog"><div class="modal-content"><div class="modal-header"><button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button><h4 class="modal-title">Get '+section+' email alerts sent to your inbox</h4></div><div class="modal-body" id="registerModal"><form action="/my/account/register/news-alerts/part-user-ajax/" method="post" id="modal-user-register" class="regForm genericForm clearfix"><div id="modal-user-register-errors"><div style="display: none;" class="validateError"><h4>Sorry, it seems some details are missing or incorrect.</h4><ul></ul></div></div><ul><li><label for="regemail"><span>*</span>Email</label><input type="text" value="" name="regemail" id="regemail"></li></ul><button value="Next" class="btn btnGeneric" name="submit" type="submit" id="submit-email-alert-registration">Subscribe now &raquo;</button><p class="genericHelper">Fields marked with * are mandatory.</p></form></div></div></div></div>');
                break;
            case "sport":
                var section = "sport";
                is_show_modal = true;
                var registerModalContent = $('<div class="modal fade"><div class="modal-dialog"><div class="modal-content"><div class="modal-header"><button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button><h4 class="modal-title">Get '+section+' email alerts sent to your inbox</h4></div><div class="modal-body" id="registerModal"><form action="/my/account/register/sport-alerts/part-user-ajax/" method="post" id="modal-user-register" class="regForm genericForm clearfix"><div id="modal-user-register-errors"><div style="display: none;" class="validateError"><h4>Sorry, it seems some details are missing or incorrect.</h4><ul></ul></div></div><ul><li><label for="regemail"><span>*</span>Email</label><input type="text" value="" name="regemail" id="regemail"></li></ul><button value="Next" class="btn btnGeneric" name="submit" type="submit" id="submit-email-alert-registration">Subscribe now &raquo;</button><p class="genericHelper">Fields marked with * are mandatory.</p></form></div></div></div></div>');
                break;
            case "":
                var section = "news";
                is_show_modal = true;
                var registerModalContent = $('<div class="modal fade"><div class="modal-dialog"><div class="modal-content"><div class="modal-header"><button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button><h4 class="modal-title">Get '+section+' email alerts sent to your inbox</h4></div><div class="modal-body" id="registerModal"><form action="/my/account/register/news-alerts/part-user-ajax/" method="post" id="modal-user-register" class="regForm genericForm clearfix"><div id="modal-user-register-errors"><div style="display: none;" class="validateError"><h4>Sorry, it seems some details are missing or incorrect.</h4><ul></ul></div></div><ul><li><label for="regemail"><span>*</span>Email</label><input type="text" value="" name="regemail" id="regemail"></li></ul><button value="Next" class="btn btnGeneric" name="submit" type="submit" id="submit-email-alert-registration">Subscribe now &raquo;</button><p class="genericHelper">Fields marked with * are mandatory.</p></form></div></div></div></div>');
                break;
            default:
                // Do nothing.
        }

        if (is_show_modal === true)
        {
	        $( registerModalContent ).modal();
	        
	        $( registerModalContent ).on('submit', '#modal-user-register', function(event){
	            $.ajax({
	                url: $(this).attr('action'),
	                method: "POST",
	                data: $(this).serialize(),
	                success: function(response) {

	                    if(response.success === true) 
	                    {
	                        $('#modal-user-register').replaceWith('<p><strong>Thank you.</strong></p>\
	                            <p>Please check your inbox to verify your email address.</p>');
	                    }
	                    else
	                    {
	                    	var error_message = "<h4>Sorry, it seems some details are missing or incorrect.</h4>";

	                    	var key;
	                    	for (key in response.errors)
	                    	{
	                    		error_message += '<p>' + response.errors[key] + '</p>';
	                    	}

	                    	$('#modal-user-register-errors').html('<div class="validateError">' + error_message + '</div>');
	                    }
	                }
	            });
	            event.preventDefault();
	        });
        }
    },
    createModalLoginBox: function(modaltype) {
        var url = encodeURIComponent(location.href);
        
    	switch (modaltype) {
	    	case "comment-sort":
	    		var loginModalContent = $('<div class="modal fade"><div class="modal-dialog"><div class="modal-content"><div class="modal-header"><button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button><h4 class="modal-title">Log in to enable comment sorting</h4></div><div class="modal-body" id="loginModal"><form action="" method="post" class="regForm genericForm clearfix"><ul><li><label for="regemail"><span>*</span>Email</label><input type="text" value="" name="email" id="regemail"></li><li><label for="regpassword"><span>*</span>Password</label><input type="password" value="" name="password" id="regpassword"></li></ul><button value="Sign in" class="btn btnGeneric" name="submit" type="submit">Log in</button><p class="genericHelper">Not got an account? <a href="/my/account/register/step1/?regType=comment_sort&originatingUrl='+url+'">Create an account</a></p><p class="genericHelper">Need help? <a href="/my/account/faq/">FAQ</a></p><p class="genericHelper">Fields marked with * are mandatory.</p></form></div></div></div></div>');
	    		break;
	    	case "comment-alerts":
	    		var loginModalContent = $('<div class="modal fade"><div class="modal-dialog"><div class="modal-content"><div class="modal-header"><button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button><h4 class="modal-title">Log in to get comment alerts</h4></div><div class="modal-body" id="loginModal"><form action="" method="post" class="regForm genericForm clearfix"><ul><li><label for="regemail"><span>*</span>Email</label><input type="text" value="" name="email" id="regemail"></li><li><label for="regpassword"><span>*</span>Password</label><input type="password" value="" name="password" id="regpassword"></li></ul><button value="Sign in" class="btn btnGeneric" name="submit" type="submit">Log in</button><p class="genericHelper">Not got an account? <a href="/my/account/register/step1/?regType=comment_alerts&originatingUrl='+url+'">Create an account</a></p><p class="genericHelper">Need help? <a href="/my/account/faq/">FAQ</a></p><p class="genericHelper">Fields marked with * are mandatory.</p></form></div></div></div></div>');
	    		break;
	    	default:
	    		var loginModalContent = $('<div class="modal fade"><div class="modal-dialog"><div class="modal-content"><div class="modal-header"><button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button><h4 class="modal-title">Log in and keep up to date with news and information</h4></div><div class="modal-body" id="loginModal"><form action="" method="post" class="regForm genericForm clearfix"><ul><li><label for="regemail"><span>*</span>Email</label><input type="text" value="" name="email" id="regemail"></li><li><label for="regpassword"><span>*</span>Password</label><input type="password" value="" name="password" id="regpassword"></li></ul><button value="Sign in" class="btn btnGeneric" name="submit" type="submit">Log in</button><p class="genericHelper">Not got an account? <a href="/my/account/register/step1/">Create an account</a></p><p class="genericHelper">Need help? <a href="/my/account/faq/">FAQ</a></p><p class="genericHelper">Fields marked with * are mandatory.</p></form></div></div></div></div>');
    	}
        
        $( loginModalContent ).modal();
    }
};


/* -- bootstrap-alert.js -- */

/* ========================================================================
 * Bootstrap: alert.js v3.1.1
 * http://getbootstrap.com/javascript/#alerts
 * ========================================================================
 * Copyright 2011-2014 Twitter, Inc.
 * Licensed under MIT (https://github.com/twbs/bootstrap/blob/master/LICENSE)
 * ======================================================================== */


+function ($) {
  'use strict';

  // ALERT CLASS DEFINITION
  // ======================

  var dismiss = '[data-dismiss="alert"]'
  var Alert   = function (el) {
    $(el).on('click', dismiss, this.close)
  }

  Alert.prototype.close = function (e) {
    var $this    = $(this)
    var selector = $this.attr('data-target')

    if (!selector) {
      selector = $this.attr('href')
      selector = selector && selector.replace(/.*(?=#[^\s]*$)/, '') // strip for ie7
    }

    var $parent = $(selector)

    if (e) e.preventDefault()

    if (!$parent.length) {
      $parent = $this.hasClass('alert') ? $this : $this.parent()
    }

    $parent.trigger(e = $.Event('close.bs.alert'))

    if (e.isDefaultPrevented()) return

    $parent.removeClass('in')

    function removeElement() {
      $parent.trigger('closed.bs.alert').remove()
    }

    $.support.transition && $parent.hasClass('fade') ?
      $parent
        .one($.support.transition.end, removeElement)
        .emulateTransitionEnd(150) :
      removeElement()
  }


  // ALERT PLUGIN DEFINITION
  // =======================

  var old = $.fn.alert

  $.fn.alert = function (option) {
    return this.each(function () {
      var $this = $(this)
      var data  = $this.data('bs.alert')

      if (!data) $this.data('bs.alert', (data = new Alert(this)))
      if (typeof option == 'string') data[option].call($this)
    })
  }

  $.fn.alert.Constructor = Alert


  // ALERT NO CONFLICT
  // =================

  $.fn.alert.noConflict = function () {
    $.fn.alert = old
    return this
  }


  // ALERT DATA-API
  // ==============

  $(document).on('click.bs.alert.data-api', dismiss, Alert.prototype.close)

}(jQuery);


/* -- bootstrap-modal.js -- */

/* ========================================================================
 * Bootstrap: modal.js v3.1.1
 * http://getbootstrap.com/javascript/#modals
 * ========================================================================
 * Copyright 2011-2014 Twitter, Inc.
 * Licensed under MIT (https://github.com/twbs/bootstrap/blob/master/LICENSE)
 * ======================================================================== */


+function ($) {
  'use strict';

  // MODAL CLASS DEFINITION
  // ======================

  var Modal = function (element, options) {
    this.options   = options
    this.$element  = $(element)
    this.$backdrop =
    this.isShown   = null

    if (this.options.remote) {
      this.$element
        .find('.modal-content')
        .load(this.options.remote, $.proxy(function () {
          this.$element.trigger('loaded.bs.modal')
        }, this))
    }
  }

  Modal.DEFAULTS = {
    backdrop: true,
    keyboard: true,
    show: true
  }

  Modal.prototype.toggle = function (_relatedTarget) {
    return this[!this.isShown ? 'show' : 'hide'](_relatedTarget)
  }

  Modal.prototype.show = function (_relatedTarget) {
    var that = this
    var e    = $.Event('show.bs.modal', { relatedTarget: _relatedTarget })

    this.$element.trigger(e)

    if (this.isShown || e.isDefaultPrevented()) return

    this.isShown = true

    this.escape()

    this.$element.on('click.dismiss.bs.modal', '[data-dismiss="modal"]', $.proxy(this.hide, this))

    this.backdrop(function () {
      var transition = $.support.transition && that.$element.hasClass('fade')

      if (!that.$element.parent().length) {
        that.$element.appendTo(document.body) // don't move modals dom position
      }

      that.$element
        .show()
        .scrollTop(0)

      if (transition) {
        that.$element[0].offsetWidth // force reflow
      }

      that.$element
        .addClass('in')
        .attr('aria-hidden', false)

      that.enforceFocus()

      var e = $.Event('shown.bs.modal', { relatedTarget: _relatedTarget })

      transition ?
        that.$element.find('.modal-dialog') // wait for modal to slide in
          .one($.support.transition.end, function () {
            that.$element.focus().trigger(e)
          })
          .emulateTransitionEnd(300) :
        that.$element.focus().trigger(e)
    })
  }

  Modal.prototype.hide = function (e) {
    if (e) e.preventDefault()

    e = $.Event('hide.bs.modal')

    this.$element.trigger(e)

    if (!this.isShown || e.isDefaultPrevented()) return

    this.isShown = false

    this.escape()

    $(document).off('focusin.bs.modal')

    this.$element
      .removeClass('in')
      .attr('aria-hidden', true)
      .off('click.dismiss.bs.modal')

    $.support.transition && this.$element.hasClass('fade') ?
      this.$element
        .one($.support.transition.end, $.proxy(this.hideModal, this))
        .emulateTransitionEnd(300) :
      this.hideModal()
  }

  Modal.prototype.enforceFocus = function () {
    $(document)
      .off('focusin.bs.modal') // guard against infinite focus loop
      .on('focusin.bs.modal', $.proxy(function (e) {
        if (this.$element[0] !== e.target && !this.$element.has(e.target).length) {
          this.$element.focus()
        }
      }, this))
  }

  Modal.prototype.escape = function () {
    if (this.isShown && this.options.keyboard) {
      this.$element.on('keyup.dismiss.bs.modal', $.proxy(function (e) {
        e.which == 27 && this.hide()
      }, this))
    } else if (!this.isShown) {
      this.$element.off('keyup.dismiss.bs.modal')
    }
  }

  Modal.prototype.hideModal = function () {
    var that = this
    this.$element.hide()
    this.backdrop(function () {
      that.removeBackdrop()
      that.$element.trigger('hidden.bs.modal')
    })
  }

  Modal.prototype.removeBackdrop = function () {
    this.$backdrop && this.$backdrop.remove()
    this.$backdrop = null
  }

  Modal.prototype.backdrop = function (callback) {
    var animate = this.$element.hasClass('fade') ? 'fade' : ''

    if (this.isShown && this.options.backdrop) {
      var doAnimate = $.support.transition && animate

      this.$backdrop = $('<div class="modal-backdrop ' + animate + '" />')
        .appendTo(document.body)

      this.$element.on('click.dismiss.bs.modal', $.proxy(function (e) {
        if (e.target !== e.currentTarget) return
        this.options.backdrop == 'static'
          ? this.$element[0].focus.call(this.$element[0])
          : this.hide.call(this)
      }, this))

      if (doAnimate) this.$backdrop[0].offsetWidth // force reflow

      this.$backdrop.addClass('in')

      if (!callback) return

      doAnimate ?
        this.$backdrop
          .one($.support.transition.end, callback)
          .emulateTransitionEnd(150) :
        callback()

    } else if (!this.isShown && this.$backdrop) {
      this.$backdrop.removeClass('in')

      $.support.transition && this.$element.hasClass('fade') ?
        this.$backdrop
          .one($.support.transition.end, callback)
          .emulateTransitionEnd(150) :
        callback()

    } else if (callback) {
      callback()
    }
  }


  // MODAL PLUGIN DEFINITION
  // =======================

  var old = $.fn.modal

  $.fn.modal = function (option, _relatedTarget) {
    return this.each(function () {
      var $this   = $(this)
      var data    = $this.data('bs.modal')
      var options = $.extend({}, Modal.DEFAULTS, $this.data(), typeof option == 'object' && option)

      if (!data) $this.data('bs.modal', (data = new Modal(this, options)))
      if (typeof option == 'string') data[option](_relatedTarget)
      else if (options.show) data.show(_relatedTarget)
    })
  }

  $.fn.modal.Constructor = Modal


  // MODAL NO CONFLICT
  // =================

  $.fn.modal.noConflict = function () {
    $.fn.modal = old
    return this
  }


  // MODAL DATA-API
  // ==============

  $(document).on('click.bs.modal.data-api', '[data-toggle="modal"]', function (e) {
    var $this   = $(this)
    var href    = $this.attr('href')
    var $target = $($this.attr('data-target') || (href && href.replace(/.*(?=#[^\s]+$)/, ''))) //strip for ie7
    var option  = $target.data('bs.modal') ? 'toggle' : $.extend({ remote: !/#/.test(href) && href }, $target.data(), $this.data())

    if ($this.is('a')) e.preventDefault()

    $target
      .modal(option, this)
      .one('hide', function () {
        $this.is(':visible') && $this.focus()
      })
  })

  $(document)
    .on('show.bs.modal', '.modal', function () { $(document.body).addClass('modal-open') })
    .on('hidden.bs.modal', '.modal', function () { $(document.body).removeClass('modal-open') })

}(jQuery);


/* -- bootstrap-tab.js -- */

/* ========================================================================
 * Bootstrap: tab.js v3.1.1
 * http://getbootstrap.com/javascript/#tabs
 * ========================================================================
 * Copyright 2011-2014 Twitter, Inc.
 * Licensed under MIT (https://github.com/twbs/bootstrap/blob/master/LICENSE)
 * ======================================================================== */


+function ($) {
  'use strict';

  // TAB CLASS DEFINITION
  // ====================

  var Tab = function (element) {
    this.element = $(element)
  }

  Tab.prototype.show = function () {
    var $this    = this.element
    var $ul      = $this.closest('ul:not(.dropdown-menu)')
    var selector = $this.data('target')

    if (!selector) {
      selector = $this.attr('href')
      selector = selector && selector.replace(/.*(?=#[^\s]*$)/, '') //strip for ie7
    }

    if ($this.parent('li').hasClass('active')) return

    var previous = $ul.find('.active:last a')[0]
    var e        = $.Event('show.bs.tab', {
      relatedTarget: previous
    })

    $this.trigger(e)

    if (e.isDefaultPrevented()) return

    var $target = $(selector)

    this.activate($this.parent('li'), $ul)
    this.activate($target, $target.parent(), function () {
      $this.trigger({
        type: 'shown.bs.tab',
        relatedTarget: previous
      })
    })
  }

  Tab.prototype.activate = function (element, container, callback) {
    var $active    = container.find('> .active')
    var transition = callback
      && $.support.transition
      && $active.hasClass('fade')

    function next() {
      $active
        .removeClass('active')
        .find('> .dropdown-menu > .active')
        .removeClass('active')

      element.addClass('active')

      if (transition) {
        element[0].offsetWidth // reflow for transition
        element.addClass('in')
      } else {
        element.removeClass('fade')
      }

      if (element.parent('.dropdown-menu')) {
        element.closest('li.dropdown').addClass('active')
      }

      callback && callback()
    }

    transition ?
      $active
        .one($.support.transition.end, next)
        .emulateTransitionEnd(150) :
      next()

    $active.removeClass('in')
  }


  // TAB PLUGIN DEFINITION
  // =====================

  var old = $.fn.tab

  $.fn.tab = function ( option ) {
    return this.each(function () {
      var $this = $(this)
      var data  = $this.data('bs.tab')

      if (!data) $this.data('bs.tab', (data = new Tab(this)))
      if (typeof option == 'string') data[option]()
    })
  }

  $.fn.tab.Constructor = Tab


  // TAB NO CONFLICT
  // ===============

  $.fn.tab.noConflict = function () {
    $.fn.tab = old
    return this
  }


  // TAB DATA-API
  // ============

  $(document).on('click.bs.tab.data-api', '[data-toggle="tab"], [data-toggle="pill"]', function (e) {
    e.preventDefault()
    $(this).tab('show')
  })

}(jQuery);


/* -- bootstrap-transition.js -- */

/* ========================================================================
 * Bootstrap: transition.js v3.1.1
 * http://getbootstrap.com/javascript/#transitions
 * ========================================================================
 * Copyright 2011-2014 Twitter, Inc.
 * Licensed under MIT (https://github.com/twbs/bootstrap/blob/master/LICENSE)
 * ======================================================================== */


+function ($) {
  'use strict';

  // CSS TRANSITION SUPPORT (Shoutout: http://www.modernizr.com/)
  // ============================================================

  function transitionEnd() {
    var el = document.createElement('bootstrap')

    var transEndEventNames = {
      'WebkitTransition' : 'webkitTransitionEnd',
      'MozTransition'    : 'transitionend',
      'OTransition'      : 'oTransitionEnd otransitionend',
      'transition'       : 'transitionend'
    }

    for (var name in transEndEventNames) {
      if (el.style[name] !== undefined) {
        return { end: transEndEventNames[name] }
      }
    }

    return false // explicit for ie8 (  ._.)
  }

  // http://blog.alexmaccaw.com/css-transitions
  $.fn.emulateTransitionEnd = function (duration) {
    var called = false, $el = this
    $(this).one($.support.transition.end, function () { called = true })
    var callback = function () { if (!called) $($el).trigger($.support.transition.end) }
    setTimeout(callback, duration)
    return this
  }

  $(function () {
    $.support.transition = transitionEnd()
  })

}(jQuery);


/* -- jquery-fotorama.js -- */

/*!
 * Fotorama 4.5.1 | http://fotorama.io/license/
 */
(function (window, document, location, $, undefined) {
  "use strict";
var _fotoramaClass = 'fotorama',

    font-family: 'FontAwesome';
    src: url('../../fonts/fontawesome-webfont.eot');
    src: url('../../fonts/fontawesome-webfont.eot?') format('embedded-opentype'),
         url('../../fonts/fontawesome-webfont.woff') format('woff'),
         url('../../fonts/fontawesome-webfont.ttf') format('truetype'),
         url('../../fonts/fontawesome-webfont.svg') format('svg');
    font-weight: normal;
    font-style: normal;
}
/* Font Live - for live panels - http://fontello.com/ */
@font-face {
    font-family: 'FontLive';
    src: url('../../fonts/live.eot');
    src: url('../../fonts/live.eot') format('embedded-opentype'),
         url('../../fonts/live.woff') format('woff'),
         url('../../fonts/live.ttf') format('truetype'),
         url('../../fonts/live.svg') format('svg');
    font-weight: normal;
    font-style: normal;
}

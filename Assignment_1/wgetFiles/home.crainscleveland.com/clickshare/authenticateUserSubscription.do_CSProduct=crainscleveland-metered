if (navigator.userAgent.match(/MSIE\s([0-9])\./) != null) {
	if (navigator.userAgent.match(/MSIE\s([0-9])\./)[1] < 8) {
		isltIE8 = true;
	} else {
		isltIE8 = false;
	}
}
if (!isltIE8) {
	$(window).load(function(){ 
		$('.scrollVertical').mCustomScrollbar({ //Vertical Scroll - Fixed content
			scrollButtons:{	enable:true },
			theme:"dark-thin"
		});
		$('.scrollHorizontal').mCustomScrollbar({ //Horizontal scroll - Fixed content
			horizontalScroll:true,
			scrollButtons:{	enable:true },
			theme:"dark-thin"
		});
	});
}

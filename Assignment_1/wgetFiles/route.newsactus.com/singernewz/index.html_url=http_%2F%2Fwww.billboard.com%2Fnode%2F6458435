            var set_article_size = function()
            {
                var columns = 4; 
                var margin = 10;
                var padding_and_border = 10;
				var noRight = 0;
							
                var available_width = $(".article").width();
                //if (available_width < 1000) { columns = 1; }
                if (available_width < 955) { 
					noRight = 1; columns = 2; margin = 10; padding_and_border = 8;
				}
                				
                $(".more-articles a").css({
                    "width": Math.floor((available_width - ((margin+padding_and_border)*columns*2))/columns) - 1,
                    "margin": margin
                });
				
				if(noRight == 1) {
                    $(".more-articles a").css("height", "200px");
                    $(".left-part").css("width", available_width);
                    $(".right-part").css("display", "none");
                } else {
                    $(".more-articles a").css("height", "auto");
                    $(".left-part").css("width", "728px");
                    $(".right-part").css({
                        "width": available_width - 780,
						"height": "auto",
                        "display": "block"
                    });
                }
            }; set_article_size();
            $(window).on("resize", set_article_size);
        </script>
		


	





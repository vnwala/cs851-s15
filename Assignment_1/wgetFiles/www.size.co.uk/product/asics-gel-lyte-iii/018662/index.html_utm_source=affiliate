function myExampleShowTab(Id){

var tabClass = jQuery("#ReviewsTab").attr('class');
var tabNo = tabClass.split(' ')[0].split('-')[1];
parentContainer = jQuery('#ReviewsTab').parent().parent().parent();
jQuery(parentContainer).find('.tab-content').hide();
jQuery(parentContainer).find('#content-' + tabNo).show();
jQuery('#ReviewsTab').parent().parent().attr('class', 'product-tabs tab-selected-'+tabNo);
reviewOffset = jQuery("#content-2").offset().top;
jQuery(window).scrollTop(reviewOffset);

//reviewPanelChange("reviews"); 
return false;
}


function reviewPanelChange(clickedTab) {


 panelToShowId = clickedTab +'Panel';
 panelToShow = jQuery('#'+panelToShowId);

 panelToShow.siblings('div').fadeOut("fast", function() {
 panelToShow.fadeIn("fast");
 reviewOffset = jQuery("#reviewsPanel").offset().top;
 jQuery(window).scrollTop(reviewOffset);
 });
 
 

 } 


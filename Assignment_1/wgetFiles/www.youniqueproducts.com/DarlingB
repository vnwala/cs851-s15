            $(".changemarket").on({
                change: function(e) {
                    if($(this).is(":checked")) {
                        market = parseInt($(this).val());
                        API.call("/user/setMarket/"+market, {}, function(result) {
                            //$('#changemarket').addClass('loader');
                            window.location.reload();
                        });
                    }
                }
            });
        });

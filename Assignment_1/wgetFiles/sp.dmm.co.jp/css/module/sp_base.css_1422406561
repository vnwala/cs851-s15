body[class*="dm-ios-5"] #dm-content02.dm-on {
-webkit-animation: none;
}
body[class*="dm-ios-5"] #dm-header02.dm-off > .hd-wrapwrap,
body[class*="dm-ios-5"] #dm-content02.dm-off {
-webkit-animation: none;
}


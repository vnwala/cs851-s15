<script type="text/javascript">

  var emailSmartLink_embed_atf, smsSmartLink_embed_atf;
  amznJQ.onReady('jQuery', function() {
    var kcpAppBaseObj_embed_atf = jQuery('.kcpAppBaseBox_embed_atf');
    var kcpAppDialogObj_embed_atf = jQuery('.kcpAppsPopOverDialog_embed_atf');
    var escChar = String.fromCharCode(0x5c);
    function isValidEmail(emailAddress) {
        //If email not supported or invalid email address return false
        if ( parseInt("") || (emailAddress && typeof emailAddress !== "string" )) {
            return false;
        }

        // validating only from @ to end
        var validator = new RegExp(escChar + "w+@[" + escChar +"w" + escChar + "-+_]+?" + escChar + ".[" + escChar + "w" + escChar + "-+_]+");
        return emailAddress.match( validator ) != null;
    }
    
    function isValidPhone(phoneNumber) {
        //If sms not supported or no input return false
        if ( parseInt("") || !phoneNumber ) {
            return false;
        }

        var validator = new RegExp("^([0-9" + escChar + "(" + escChar + ")" + escChar + "/" + escChar + "+" + escChar + "-]*)$");
        var digitsOnly = phoneNumber.replace(/D/g, '');
        return digitsOnly.length >= 9 && digitsOnly.length < 20 && phoneNumber.match(validator) != null; 
    }
    
    function appendCountryCode(phoneNumber) {
        var countryCode = "";
        if ( countryCode && isValidPhone(phoneNumber) && !phoneNumber.match(/^＼+/g) ) {
            phoneNumber = countryCode + phoneNumber;
        }
        return phoneNumber;
    }
    
    var successMessage = "アプリ情報を送信中です。Eメールをご確認ください。";
    var processError = "現時点では、リクエストを処理することができません。しばらくしてからもう一度試してください。";
    var popupOptions = {
        title: "無料 Kindle 読書アプリを入手",
        localContent: "#kcpAppsPopOverDialog_embed_atf",
        closeEventInclude: "CLICK_OUTSIDE",
        modal: "true",
        closeText: "",
        width: 650,
        onShow: function() {
            jQuery.ajax({
                url: '/gp/digital/fiona/ajax/reftagCall.html/ref=kcpapp_embed_atf',
                dataType: 'json',
                cache: false,
                type: 'GET'
            });
        }
    }

    kcpAppDialogObj_embed_atf = kcpAppBaseObj_embed_atf; //Replace the dialogObj with baseObj as we don't have dialog for inline form and all the content will be inside base object

        kcpAppDialogObj_embed_atf.find('.kcpAppRecipient').unbind('keypress'); //Unbind previous clicks to avoid duplicate bindings.
        kcpAppDialogObj_embed_atf.find('.kcpAppRecipient').keypress(function(e){
            if (e.which == 13){
                kcpAppDialogObj_embed_atf.find(".kcpAppSendButton").click();
            }
        });
        
        if(jQuery.browser && jQuery.browser.msie){
            kcpAppDialogObj_embed_atf.find('[placeholder]').focus(function() {
              var input = jQuery(this);
              if (input.val() == input.attr('placeholder')) {
                input.val('');
                input.removeClass('placeholder');
              }
            }).blur(function() {
              var input = jQuery(this);
              if (input.val() == '' || input.val() == input.attr('placeholder')) {
                input.addClass('placeholder');
                input.val(input.attr('placeholder'));
              }
            }).blur();
        }
        
        kcpAppDialogObj_embed_atf.find(".kcpAppSendButton").unbind("click"); //Unbind previous clicks to avoid duplicate bindings.
        kcpAppDialogObj_embed_atf.find(".kcpAppSendButton").bind("click", function(){
           var recipient = kcpAppDialogObj_embed_atf.find('.kcpAppRecipient').val();
           var inputData = {
            "recipient" : appendCountryCode(recipient),
            "downloadLink" : (emailSmartLink_embed_atf) ? emailSmartLink_embed_atf : "http://www.amazon.co.jp/gp/kindle/kcpApp.html",
            "toJSON" : "1"
           };
           if(isValidEmail(recipient)){
               inputData['communicationType'] = 'email';
               submitRTERequest(inputData);
           } else if(isValidPhone(recipient)){
               inputData['communicationType'] = 'sms';
               submitRTERequest(inputData);
               if(smsSmartLink_embed_atf){
                 inputData[downloadLink] = smsSmartLink_embed_atf;
               }
           } else {
               kcpAppDialogObj_embed_atf.find('.kcpAppDeliverySuccess, .kcpAppDeliveryError').html("");
               kcpAppDialogObj_embed_atf.find('.kcpAppDeliveryError').html("入力が無効です。有効なEメールアドレスを入力してください。");
           }
        });

      function assignSampleOwnership(){
         var refTagSuffix = "";
         var client = refTagSuffix.split("_")[0];
         if (!client) client = "kcpAppWidget";

         var input = {
            sid       : "000-0000000-0000000",
            asin      : "",
            pbookASIN : "",
            client    : client,
            site      : "RetailWebsite",
            encryptedDeviceType : "A2CTZ977SKFQZY"
         };

         jQuery.extend(input,csrfParamsJson);
         jQuery.ajax({
            url: "/gp/digital/fiona/ajax/fulfill-sample-ownership.html/ref=kcp_samp_" + refTagSuffix,
            type: "POST",
            dataType: "json",
            data: input
         });
      }

      function submitRTERequest(input){
        jQuery.extend(input,csrfParamsJson);
        jQuery.ajax({
                url: "/gp/digital/fiona/ajax/send-email-or-sms.html/ref=kcpapp_snt_embed_atf",
                type: "POST",
                dataType: "json",
                data: input,
                beforeSend:function(){
                    kcpAppDialogObj_embed_atf.find('.kcpAppDeliverySuccess, .kcpAppDeliveryError').html("");
                    kcpAppDialogObj_embed_atf.find('.kcpAppDeliveryInProgress').html('<div style="text-align:center"><img src="http://g-ec2.images-amazon.com/images/G/09/kindle/www/myk/icon_spinner._V155338383_.gif" /> リクエストを処理しています... </div>');
                },
                success:function(data){
                  kcpAppDialogObj_embed_atf.find('.kcpAppDeliveryInProgress').html("");
                  if(typeof(data) == "string"){
                      data=JSON.parse(data);
                  }
                  if(data && data.status == 1) {
                    kcpAppDialogObj_embed_atf.find('.kcpAppDeliverySuccess').html( successMessage + ((input.communicationType == 'email') ? "  " : " 携帯電話") +".");
                    kcpAppDialogObj_embed_atf.find('.kcpAppRecipient').val("");
                  } else {
                    kcpAppDialogObj_embed_atf.find('.kcpAppDeliverySuccess, .kcpAppDeliveryError').html("");
                    data['message'] = (data.status) ? "Invalid Request" : data.message;
                    kcpAppDialogObj_embed_atf.find('.kcpAppDeliveryError').html(((data.message) ? data.message : processError));
                  }
                },
                error:function(){
                    kcpAppDialogObj_embed_atf.find('.kcpAppDeliveryInProgress').html("");
                    kcpAppDialogObj_embed_atf.find('.kcpAppDeliverySuccess, .kcpAppDeliveryError').html("");
                    kcpAppDialogObj_embed_atf.find('.kcpAppDeliveryError').html(processError);
                }
            });
      }
      
      jQuery.ajax({
            url: "/gp/digital/fiona/ajax/reftagCall.html/ref=kcpapp_load_embed_atf",
            dataType: 'json',
            cache: false,
            type: 'GET'
      });
    });
    
</script>











 


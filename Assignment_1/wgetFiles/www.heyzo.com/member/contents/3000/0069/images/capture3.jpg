<script language="JavaScript">
var NETI_SSU = '';
var NETI_SSC = 0;

var sus_wrapper = document.getElementById( "sus-wrapper" );
var uss_wrapper = document.getElementById( "uss-wrapper" );
var neti_found = 0;
var username_changed = 0;
var pwd_changed = 0;

var dateObj    = new Date();
dateObj.setFullYear(dateObj.getFullYear() + 1);
var cookieDobj = new hmsCookie('r_login',dateObj.toUTCString());
var cookieDobj2 = new hmsCookie('r_login_rem_heyzo_flag',dateObj.toUTCString());
var cookieDobj3 = new hmsCookie('r_login_heyzo',dateObj.toUTCString()); 
cookieDobj.get();
cookieDobj2.get();
cookieDobj3.get();


function set_check_css(sus, uss, sus_wrap, uss_wrap)
{
   if(sus == 1 )
   { 
     document.login_form.NETI_SUS.checked = true;
   }else
   {
     document.login_form.NETI_SUS.checked = false;
   }

   if(uss == 1)
   {
     document.login_form.NETI_USS.checked = true;
   }else
   {
     document.login_form.NETI_USS.checked = false;
   }
   
   sus_wrapper.style.display = sus_wrap;
   uss_wrapper.style.display = uss_wrap;
}


// reason for this part is we have two cookies but we want to use
// each site clp. if each site cookie is not there then use old one 
var cookie_user_name = cookieDobj.args.u;
if(cookieDobj3.args.u)
{
cookie_user_name = cookieDobj3.args.u;
}

if(cookieDobj2.args.flag == '1'){
    document.login_form.remember_login.checked = true;
    document.login_form.FORM_PASSWD.value = '';
    document.login_form.FORM_USER.value = cookie_user_name.toLowerCase();
    neti_found = 0;
    set_check_css(0, 0, 'block', 'none');
}else 
{ 
  if (cookie_user_name) {
    document.login_form.FORM_USER.value = cookie_user_name.toLowerCase();
    if ( NETI_SSU && document.login_form.FORM_USER.value == NETI_SSU) {
      set_check_css(0, 1, 'none', 'block');
    } else {
      document.login_form.remember_login.checked = true;
      neti_found = 0;
    }
  }

  if ( NETI_SSC > 19 && document.login_form.FORM_USER.value == NETI_SSU ) {
    sus_wrapper.style.display = 'none';
    document.login_form.NETI_SUS.checked = false;
    document.login_form.FORM_PASSWD.value = '**************';
    neti_found = 1;
  } else {
    uss_wrapper.style.display = 'none';
    document.login_form.NETI_USS.checked = false;
  }
}

function checkbox_checked(box) {
  if(box == 'NETI_USS' || box == 'NETI_SUS') {
   if(!pwd_changed) {
      document.login_form.FORM_PASSWD.value = '';
    }

    if(document.login_form.remember_login.checked) {
      document.login_form.remember_login.checked = false;
    }
  } else {
    if(document.login_form.NETI_USS.checked) {
      document.login_form.NETI_USS.checked = false;
    } else if(document.login_form.NETI_SUS.checked) {
      document.login_form.NETI_SUS.checked = false;
    } else if(document.login_form.remember_login.checked == false )
    {
      if(!pwd_changed){
       document.login_form.FORM_PASSWD.value = '';
      }
    }
  }
}

function username_change() {
  if(!document.login_form.NETI_USS.checked && !document.login_form.NETI_SUS.checked) {
    return false;
  }
  if ( NETI_SSU && document.login_form.FORM_USER.value == NETI_SSU) {
    set_check_css(0, 1, 'block', 'none');
  } else {
    set_check_css(1, 0, 'none', 'block');
  }
}

function set_username_changed() {
  username_changed = 1;
  document.login_form.FORM_PASSWD.value = '';
  set_check_css(0, 0, 'block', 'none');


}

function set_pwd_changed() {
  pwd_changed = 1;
  // if any check box is checked then keep but change USS -> SUS
  if(document.login_form.NETI_USS.checked)
  {
    set_check_css(1, 0, 'block', 'none');
  }

}

function neti_found2() {
  neti_found = 1;
}

function remember_me() {
  if ( document.login_form.remember_login.checked || document.login_form.NETI_USS.checked || document.login_form.NETI_SUS.checked ) {
    var dateObj    = new Date();
    dateObj.setFullYear(dateObj.getFullYear() + 1);
    var cookieDobj3 = new hmsCookie('r_login_heyzo',dateObj.toUTCString());
    cookieDobj3.add('u',document.login_form.FORM_USER.value);
    cookieDobj3.set();

    // remove old cookie
    var dateObj    = new Date();
    dateObj.setFullYear(dateObj.getFullYear() - 1);
    var cookieDobj= new hmsCookie('r_login',dateObj.toUTCString());
    cookieDobj.set();
 
  } else {
    var dateObj    = new Date();
    dateObj.setFullYear(dateObj.getFullYear() - 1);
    var cookieDobj3= new hmsCookie('r_login_heyzo',dateObj.toUTCString());
    cookieDobj3.add('u','');
    cookieDobj3.set();

    // remove old cookie
    var dateObj    = new Date();
    dateObj.setFullYear(dateObj.getFullYear() - 1);
    var cookieDobj= new hmsCookie('r_login',dateObj.toUTCString());
    cookieDobj.add('u','');
    cookieDobj.set();
                 
  }

  if(document.login_form.remember_login.checked )
  {
    var dateObj    = new Date();
    dateObj.setFullYear(dateObj.getFullYear() + 1);
    var cookieDobj2 = new hmsCookie('r_login_rem_heyzo_flag',dateObj.toUTCString());
    cookieDobj2.add('flag','1');
    cookieDobj2.set();
  }else
  {
    var cookieDobj2 = new hmsCookie('r_login_rem_heyzo_flag',dateObj.toUTCString());
    cookieDobj2.add('flag','');
    cookieDobj2.set();

  } 


  if (neti_found == 0) {
    document.cookie = 'NetiUSS=something; expires=Thu, 2 Aug 2006 20:47:11 UTC; domain=.d2pass.com; path=/ ';
  }
}

function clear_me() {
  if (document.login_form.NETI_USS.checked) {
    neti_found = 1;
  } else {
    neti_found = 0;
  }
}

function submit_clicked() {

  if(pwd_changed || username_changed) {
    neti_found = 0;
  } else  if(document.login_form.remember_login.checked) {
    set_check_css(0, 1, 'block', 'none');
  } else if((document.login_form.NETI_USS.checked || document.login_form.NETI_SUS.checked) 
            && !(pwd_changed || username_changed)
            && document.login_form.FORM_PASSWD.value != ''){
    neti_found = 1;
  } else {
    document.login_form.NETI_USS.checked = false;
    document.login_form.NETI_SUS.checked = true;
    neti_found = 0;
  }

  if(document.login_form.FORM_PASSWD.value == '')
  {
    document.login_form.NETI_USS.checked = false;
    document.login_form.NETI_SUS.checked = false;
    neti_found = 0;
  }

  remember_me();
}

</script>
</body>
</html>

      function (e, fan) {
    if ('profile_become_a_fan_button_' + fan.page_object_id + '_large' !== 'profile_become_a_fan_button_artist_3021006_large') {
      return;
    }

    var $button = $j('#profile_become_a_fan_button_artist_3021006_large');

<script type="text/javascript">
  function load_retargeting_pixels() {
    if (window.Reverb && Reverb.include_pixel) {
      drop_retargeting_pixels();
    } else {
      jQuery.getScript("/javascripts/tracking/retargeting_pixels.js").done(drop_retargeting_pixels);
    }
    
    function drop_retargeting_pixels() {
      if (Reverb.log) {
        Reverb.log("Dropping pixels: 405198, 465579");
      }

      setTimeout(function() {Reverb.include_pixel("21710", "genre_pixel_for_rock", "&lt;!-- Segment Pixel - genre_pixel_for_rock - DO NOT MODIFY --&gt;&lt;script src=&quot;http://ib.adnxs.com/seg?add=405198&amp;t=1&quot; type=&quot;text/javascript&quot;&gt;&lt;/script&gt;&lt;!-- End of Segment Pixel --&gt;");}, 100);
      setTimeout(function() {Reverb.include_pixel("29479", "genre_pixel_for_rock", "&lt;!-- Segment Pixel - genre_pixel_for_rock - DO NOT MODIFY --&gt;&lt;script src=&quot;http://ib.adnxs.com/seg?add=465579&amp;t=1&quot; type=&quot;text/javascript&quot;&gt;&lt;/script&gt;&lt;!-- End of Segment Pixel --&gt;");}, 200);
    };
  };
  
  jQuery('#pixel_auto_load_marker_21710_29479').ready(load_retargeting_pixels);
</script>

        	var start;
        	function startTimer(){
        		start = new Date().getTime();
            }

            function calculateTime(){
            	var end = new Date().getTime();
            	var time = end - start;
            	if(time <= 1000){
                	$('#ctrlf2').attr('value',true);
                }
            }
        </script>
        

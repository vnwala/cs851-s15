<script type="text/javascript">

var onedit = false;
function toggle_edit(){
	if (onedit){
		onedit = false;
		$("#editbox").hide("fast");
	}else{
		onedit = true;
		$("#editbox").show("fast");
	}
}

function update_title(){
	dataparam = "t=" + encodeURIComponent($("#moviename").attr("value"));
	$.ajax({
		url: "/userajax.php?c=edittitle&m=139278827",
		data: dataparam,
		dataType: "json",
		type: "POST",
		success: function(data){
			$("#moviename").attr("value", data["title"]);
			toggle_edit();
			$("#movietitle").text(data["title"]);
		}
	});
	return false;
}


$(document).ready(function(){
	$("#editlink").click(toggle_edit);
	$("#movietitlebutton").click(update_title);
		toggle_edit();
		$("#moviename").focus();
});

</script>


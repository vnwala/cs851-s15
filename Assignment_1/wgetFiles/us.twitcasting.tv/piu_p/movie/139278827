		<script type="text/javascript">
		function check_is_live(){
			$.ajax({
				url: "/userajax.php?c=islive&u=piu_p",
				dataType: "json",
				type: "GET",
				success: function(data){
					if (data){
						$("#liveicon").show("slow");
						if (data["url"]){
							$("#liveiconlink").attr("href", data["url"]);
						}
					}else{
						$("#liveicon").hide("slow");
					}
					setTimeout(check_is_live, 30000);
				}
			});
		}
		$(document).ready(function(){
			check_is_live();
		});
		</script>


		

	<script type="text/javascript">
	function onBackerButtonLoginFinished(result, userid){
		if (result) back_user('piu_p', 1);
	}
	function show_back_user(){
		$("#back_user").show();
		$("#backing_user").hide();
		$("#back_user").unbind("click");
		$("#back_user").click(function(){
			back_user('piu_p', 1);
			return false;
		});
	}
	function show_backing_user(){
		$("#back_user").hide();
		$("#backing_user").show();
		$("#backing_user").unbind("click");
		$("#backing_user").click(function(){
			var r =  confirm('Are you sure you want to quit being a fan for this user?');
			if (r){
				back_user('piu_p', 0);
			}
			return false;
		});
	}	
	function back_user(target, mode){
		$.ajax({
			url: "/userajax.php?",
			data: {
								"c": "backuser",
				"m": mode,
				"u": target
			},
			dataType: "json",
			type: "POST",
			success: function(data){
				if (data){
					if (mode){
						show_backing_user();
					}else{
						show_back_user();
					}
				}
			}
		});
	}
	</script>

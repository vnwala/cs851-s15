<script>
$(function(){
	checkSlot();
    countSlot(1);
});	
function checkSlot(){
    var strURL = '/slotmachine/checkSlot?_rand=' + Math.random();
    var xmlhttp = getXmlhttp();
    if(!xmlhttp){
        return;
    }
    xmlhttp.open('GET', strURL, true);
    xmlhttp.onreadystatechange = function(){
        if(xmlhttp.readyState == 4 && xmlhttp.status == 200){
            isslot = xmlhttp.responseText;
            if(isslot == 1 || !isLogin()){
                $('.slotBtn').attr('id','control');
            }else if(isslot == 2){
				$('.slotBtn').attr('id','points_buy');
            }else{
				$('.slotBtn').attr('id','slot_buy');
            }
            var slotOpen = 0;//no need cookie default is close slot 2014-06-09 getCookie('slot_open');
            if(!isLogin()){
                slotOpen = 0;
            }
            if(slotOpen == 1 || slotOpen == null){
                $('.slot_container').css('display','block');
            }else{
                $('.slot_container').css('display','none');
            }
        }
    }
    xmlhttp.send(null);
}
function countSlot(no_cache){
    if(isLogin()){
        xmlHttpGet('/slotmachine/countSlot/'+no_cache+'?remain_eine=1','count_slot');
    }
}
function openSlot(){
    $('.slot_container').css('display', $('.slot_container').css('display') == 'block' ? 'none' : 'block');
}
function completePurchase(){
    updateUserPoints(1);
    countSlot(1);
}

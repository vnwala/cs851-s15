<script>
var dis,rest,pos,popWidth;
var totalWidth=220*10;
$(function(){
	var windowHeight=Math.max(window.innerHeight,$(window).height());//,$(document).height()
	$('.overlay').css('padding-top',(windowHeight/2)+'px')
	if(window._agck === 0 || (parseInt($.cookie('ageVerifiedC'))===1 && window._agck !== 1) ){
		$('#idchk').css('display','none').remove();
	}
	pos=parseInt($('#thumbSlide').css('margin-left'));
	showSlider();
	$('#nextThumbs').click(function(){
		popWidth=$('.popBase').width()-80;
		rest=totalWidth-(popWidth+40-pos);
		if(rest>0){
			if(rest<220){
				dis=rest;
			}else{
				dis=220;
			}
			$('#thumbSlide').animate({
				marginLeft:pos-dis+'px' 
			},'fast')
			pos=pos-dis;
		}
		showSlider();
	})
	$('#prevThumbs').click(function(){
		popWidth=$('.popBase').width()-80;
		rest=totalWidth-(popWidth+40-pos);
		if(pos<0){
			if(pos>-220){
				dis=-pos;
			}else{
				dis=220;
			}
			$('#thumbSlide').animate({
				marginLeft:pos+dis+'px' 
			},'fast')
			pos=pos+dis;
		}
		showSlider();
	})
})
function showSlider(){
	if(pos>=0){
		$('#prevThumbs').hide();
		$('#nextThumbs').show();
	}else if(pos+totalWidth<=popWidth+40){
		console.log('next end');
		$('#prevThumbs').show();
		$('#nextThumbs').hide();
	}else{
		$('#prevThumbs').show();
		$('#nextThumbs').show();
	}
}
if((UA.indexOf('iPhone') !== -1) || (UA.indexOf('iPad') != -1)  || (UA.indexOf('iPod') != -1) || ((UA.indexOf('Android') !== -1) && (UA.indexOf('Mobile') !== -1)) || (UA.indexOf('Windows Phone') !== -1) || (UA.indexOf('BlackBerry') !== -1)){
	$('.loginSignup').css('display','none');
}

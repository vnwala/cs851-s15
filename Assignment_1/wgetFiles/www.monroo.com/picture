function setSP(swf){
    document.writeln('<object classid="clsid:D27CDB6E-AE6D-11cf-96B8-444553540000" codebase="http://download.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version=7,0,0,0" width="400" height="50">');
    document.writeln('<param name="movie" value="/img/bg/'+swf+'.swf">');
    document.writeln('<param name="quality" value="high">');
    document.writeln('<param name="wmode" value="transparent"/>');
    document.writeln('<embed src="http://www.monroo.com/img/bg/'+swf+'.swf" quality="high" wmode="transparent" width="400" height="50" name="'+swf+'" type="application/x-shockwave-flash" pluginspage="http://www.macromedia.com/go/getflashplayer" />');
    document.writeln('</object>');
}

/* -----------------------------------------
   General Form Styling
----------------------------------------- */
form { margin: 0; }

input[type="text"], input[type="password"], input[type="date"], input[type="datetime"], input[type="email"], input[type="number"], input[type="search"], input[type="tel"], input[type="time"], input[type="url"], textarea {

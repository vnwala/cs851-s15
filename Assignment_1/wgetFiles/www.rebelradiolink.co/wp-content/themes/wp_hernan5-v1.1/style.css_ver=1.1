h4 { font-size: 16px; }
h5 { font-size: 14px; }
h6 { font-size: 12px; }

a, a:visited { color: #FFC600; text-decoration: none; line-height: inherit;
	-moz-transition: 0.2s all linear;
	-webkit-transition: 0.2s all linear;
	transition: 0.2s all linear;

}


/* -----------------------------------------
   Pagination
----------------------------------------- */
#paging {
	margin-top: 25px;
}

#paging a,
#paging span {
	margin-right: 5px;
	background: #0b0b0b;
	padding: 6px 14px;
	display: inline-block;
}

/* -----------------------------------------
   Buttons
----------------------------------------- */
a.action-btn,
a.action-btn:visited,
.action-btn {
	border-radius: 1px 1px 1px 1px;
	display: inline-block !important;
	font-size: 10px;
	height: 23px;
	padding: 6px 12px 6px;
	position: relative;
	text-transform: uppercase;
	color: #161718;
	border: none;
	line-height: 1;

	-moz-transition: 0.2s all linear;
	-webkit-transition: 0.2s all linear;
	transition: 0.2s all linear;
}

a.action-btn:active,
.action-btn:active {
	top: 1px;
}

a.action-btn.buy,
a.action-btn.buy:visited,
.action-btn { }

a.action-btn.buy:hover,
.action-btn:hover {
	color: #222222;
}


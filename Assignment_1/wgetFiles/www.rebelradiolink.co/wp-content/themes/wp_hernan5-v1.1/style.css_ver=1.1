}

.single-post .entry-thumb,
.page .entry-thumb { margin-bottom: 20px; }

.entry blockquote {
	padding: 0 0 0 20px;
}

.entry blockquote p {
	font-size: 1.2em;
	color: #fff;
}

.entry-content ul {

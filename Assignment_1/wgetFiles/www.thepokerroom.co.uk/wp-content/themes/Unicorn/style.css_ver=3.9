.error404_title { font-weight:bold; font-size:50px; text-align:center; margin-bottom:30px }
.error404_text { font-weight:bold; font-size:30px; text-align:center; margin-bottom:30px;}

.getting-started-img { clear:both;float:left; width}

#cash-games th {
    padding-bottom: 5px;
    border-bottom: 1px solid #777;
    border-bottom: 1px solid rgba(119,119,119,0.5);
    font-size: 15px;
    width: 23%;
    height: 24px;
} 

#cash-games td {
    padding: 6px 0;
    border-bottom: 1px solid #777;
    border-bottom: 1px solid rgba(119,119,119,0.5);
    border-right: 10px transparent solid;
    vertical-align: middle;
}

.widget.widget_sp_image{
    border-bottom: 1px solid rgba(119, 119, 119, 0.5);
    padding-top: 10px;
}

.widget.widget_sp_image h3{
    display: none;
}

.widget.widget_sp_image a{
    line-height: 30px;
}

.widget.widget_sp_image a img{
    width: 183px;
}

.widget.widget_sp_image a:after{
    background: url("images/bg/bg_r_arrow.png") no-repeat scroll right 11px rgba(0, 0, 0, 0);    
    clear:both;
    float: left;
    content: attr(title);
    font-size: 15px;
    color: #ffffff;
    width: 100%;
}